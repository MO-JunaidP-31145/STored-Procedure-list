CREATE TABLE [dbo].[tbl_SpanExposurePercent](
	[ExchangeCode] [smallint] NOT NULL,
	[ScripCode] [int] NOT NULL,
	[Symbol] [varchar](50) NOT NULL,
	[MarketLot] [int] NOT NULL,
	[SpanMarginPercent] [real] NOT NULL,
	[ExpMarginPercent] [real] NOT NULL,
	[TotalMarginPercent] [real] NOT NULL,
	[SpanMarginPerLot] [real] NOT NULL,
	[ExpMarginPerLot] [real] NOT NULL,
	[TotalMarginPerLot] [real] NOT NULL,
	[ClosePrice] [real] NOT NULL,
	[ExpiryDate] [int] NOT NULL,
	[RecordInsertedDate] [datetime] NOT NULL
) ON [PRIMARY]
