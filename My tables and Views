CREATE TABLE [dbo].[tbl_Trade](
	[ExchangeCode] [tinyint] NULL,
	[OrderNo] [varchar](50) NULL,
	[TerminalID] [varchar](50) NULL,
	[ScripCode] [int] NULL,
	[ClientCode] [varchar](50) NULL,
	[BuyOrSell] [char](1) NULL,
	[OrderQty] [int] NULL,
	[DisclosedQty] [int] NULL,
	[QtyRemaining] [int] NULL,
	[TradeNo] [varchar](50) NULL,
	[TradeQty] [int] NULL,
	[TradePrice] [int] NULL,
	[TradeDateTime] [int] NULL,
	[OrderType] [char](1) NULL,
	[BookType] [char](1) NULL,
	[OrderLife] [char](1) NULL,
	[ProductCode] [int] NULL,
	[UniqueOrderId] [varchar](50) NULL,
	[ExchangeDispatcherID] [int] NULL,
	[OrderInitiatorID] [varchar](50) NULL,
	[NumberOfDecimals] [int] NULL,
	[RecordInsertedDate] [datetime] NOT NULL,
	[PanNumber] [varchar](10) NOT NULL,
	[AlgoId] [int] NOT NULL,
	[AlgoCategory] [int] NOT NULL,
	[ProductFlag] [char](1) NULL
) ON [PRIMARY]
