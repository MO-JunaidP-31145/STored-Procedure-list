/****** Object:  View [dbo].[Vw_OrderDetails]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE view [dbo].[Vw_OrderDetails]    
as    
select a.UniqueOrderId,TradedQty,TotalQty,TotalRemainingQty,a.OrderType,a.OrderStatus,ConfirmedSequenceNo,    
MessageCode,ErrorCode,NumberOfDecimals,Source,ExchangeCode,ScripCode,OrderNo,ClientCode,BuyOrSell,DisclosedQty,    
TotalQtyRemaining,OrderQty,QtyTradedToday,Price,TriggerPrice,GoodTillDate,EntryDateTime,MinFillQty,LastModifiedTime,    
OrderInitiatorId,Remarks,BookType,OrderLife,ExchangeDispatcherId,ProductType,ProductFlag,TerminalID,    
ExpiryDate,BunchSeqNo,AMOOrder,AMOOrderValidated,MPlusDiffPrice,Symbol,Series,    
Instrumentname,StrikePrice,CALevel,ErrorDetail,UserRefText,ParticipantCode     
from tbl_OrderMaster a with (nolock)
inner join tbl_Order b with (nolock)
on a.UniqueOrderId=b.UniqueOrderId and b.BunchSeqNo=a.ConfirmedSequenceno    
where b.OrderNo<>'0' 

GO
