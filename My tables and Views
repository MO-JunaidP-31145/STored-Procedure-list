CREATE view [dbo].[vw_Scrips_Intelym_ScripMapping]
as
Select ExchangeCode, ScripCode, IndexCode From tbl_indices_scrip_mapping 
Union  
Select ExchangeCode, ScripCode,100001 From tbl_scrips where OptionType='XX'  and isfoscrip = 1
union
--Select ExchangeCode, ScripCode,100001 From tbl_scrips where isfoscrip = 0
Select ExchangeCode, ScripCode,100001 From tbl_scrips where (ExchangeCode = 0 or (ExchangeCode = 1 and OptionType not in ('F','G')))

