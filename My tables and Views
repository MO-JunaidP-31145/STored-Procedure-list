create view [dbo].[vw_ParticipantMaster]
as
Select Distinct ParticipantId, ParticipantName,'NSE' as ExchangeCode   
from tbl_NSE_ParticipantMaster  
Union ALL  
Select Distinct ParticipantId, ParticipantName,'NCX' as ExchangeCode   
from tbl_NCX_FOParticipantMaster  
UNION ALL  
Select Distinct ParticipantId, ParticipantName,'MCX' as ExchangeCode   
from tbl_MCX_FOParticipantMaster
