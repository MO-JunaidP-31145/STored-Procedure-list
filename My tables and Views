	[Remarks] [varchar](50) NULL,
	[OrderType] [char](1) NULL,
	[BookType] [char](1) NULL,
	[OrderLife] [char](1) NULL,
	[ExchangeDispatcherId] [int] NULL,
	[ProductType] [int] NULL,
	[ProductFlag] [char](1) NULL,
	[TerminalID] [varchar](50) NULL,
	[UniqueOrderId] [varchar](50) NULL,
	[ExpiryDate] [int] NULL,
	[BunchSeqNo] [int] NULL,
	[AMOOrder] [char](1) NULL,
	[AMOOrderValidated] [char](1) NULL,
	[Symbol] [varchar](50) NULL,
	[Series] [varchar](2) NULL,
