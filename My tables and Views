/****** Object:  View [dbo].[vw_Trade_Holding_CurrentPosition_BAsedOnPOA]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create view [dbo].[vw_Trade_Holding_CurrentPosition_BAsedOnPOA]
as
select ClientCode,ScripShortName,b.ScripIsInNo,POAQty
,sum(case when buyorsell = 'S' then tradeqty else 0 end)Sell
,sum(case when buyorsell = 'B' then tradeqty else 0 end)Buy
,sum(case when buyorsell = 'S' then tradeqty else 0 end) - sum(case when buyorsell = 'B' then tradeqty else 0 end)NetSell

from awacs10.dbo.tbl_trade(nolock) a
inner join AWACS10.dbo.tbl_scrips (nolock)b on a.ExchangeCode = b.ExchangeCode and a.scripcode = b.scripcode 
inner join awacs10.dbo.tbl_dpholdings (nolock) c on a.clientCode = c.MemberCode and b.ScripIsInNo = c.ScripIsInNo
where c.POAQty > 0 and a.ExchangeCode < 2
group by ClientCode,ScripShortName,b.ScripIsInNo,POAQty



GO
