/****** Object:  View [dbo].[vw_Mobile_BatchOrder]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[vw_Mobile_BatchOrder]
as
select a.ExchangeCode,
       a.OrderNo,
       a.TerminalID,
       a.ScripCode,
       a.ClientCode,
       a.BuyOrSell,
       a.OrderQty,
       a.DisclosedQty,
       a.QtyRemaining,
       a.TradeNo,
       a.TradeQty,
       a.TradePrice,
       a.TradeDateTime,
       a.OrderType,
       a.BookType,
       a.OrderLife,
       a.ProductCode,
       a.UniqueOrderId,
       a.ExchangeDispatcherID,
       a.OrderInitiatorID,
       a.NumberOfDecimals,
       a.RecordInsertedDate,
       a.PanNumber,
       a.AlgoId,
       a.AlgoCategory,
       a.ProductFlag,
       b.remarks,
       c.ScripIsinNo,
       c.ScripShortName,
       c.ScripName
from tbl_trade a (nolock)
         inner join (select distinct uniqueorderid, Remarks
                     from tbl_order (nolock)
                     where Remarks like 'BasketOrder|%'
                       and source in ('U', 'K', 'V', 'P', 'C', 'D', 'F', 'G')) b
                    on a.UniqueOrderId = b.UniqueOrderId
         inner join tbl_Scrips c (nolock)
                    on a.ExchangeCode = c.ExchangeCode
                        and a.ScripCode = c.ScripCode
GO
