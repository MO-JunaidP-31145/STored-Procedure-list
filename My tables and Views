/****** Object:  View [dbo].[vw_All_Exchange_ScripMaster]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE View [dbo].[vw_All_Exchange_ScripMaster]  
as  
select 'NSE' as ExchangeName,Token,WarningPercent,FreezePercent,
convert(varchar,dateadd(ss,convert(int,IssueStartDate),'01/01/1980'),106) as IssueStartDate,
convert(varchar,dateadd(ss,convert(int,MaturityDate),'01/01/1980'),106) as MaturityDate,
convert(varchar,dateadd(ss,convert(int,NoDeliveryDateStart),'01/01/1980'),106) as NoDeliveryDateStart,  
convert(varchar,dateadd(ss,convert(int,NoDeliveryDateEnd),'01/01/1980'),106) as NoDeliveryDateEnd,
BoardLotQuantity,TickSize,
convert(varchar,dateadd(ss,convert(int,ListingDate),'01/01/1980'),106) as ListingDate,
convert(varchar,dateadd(ss,convert(int,BookClosureStartDate),'01/01/1980'),106) as BookClosureStartDate,  
convert(varchar,dateadd(ss,convert(int,BookClosureEndDate),'01/01/1980'),106) as BookClosureEndDate,
ExpulsionDate,ReAdmissionDate,Remarks,NormalMarketEligibility,  
OddLotMarketEligibility,NormalMarketStatus,OddLotMarketStatus,AuctionMarketStatus,  
AuctionMarketEligibility,Dividend,FaceValue,'NA' as DividendPercent from Tbl_NSE_CMScripMaster  
Union  
select 'BSE',BSEScripCode,0 as WarningPercent,0 as FreezePercent,'NA' as IssueStartDate,'NA' as MaturityDate,  
NDSDate as NoDeliveryDateStart,NDEDate as NoDeliveryDateEnd,MarketLot as BoardLotQuantity,TickSize_l as TickSize,  
'NA' as ListingDate,BCStartDate as BookClosureStartDate,BCEndDate as BookClosureEndDate,'NA' as ExpulsionDate,  
'NA' as ReAdmissionDate,Rating as Remarks,'NA' as NormalMarketEligibility,'NA' as OddLotMarketEligibility,  
'NA' as NormalMarketStatus,'NA' as OddLotMarketStatus,'NA' as AuctionMarketStatus,'NA' as AuctionMarketEligibility,  
DivFinYear as Dividend,FaceValue,TotDividend as DividendPercent from Tbl_BSE_ScripMaster  
Union  
select 'NSEFO',Token,WarningQuantity,FreezeQuantity,IssueStartDate,ExpiryDate,  
NoDeliveryStartDate,NoDeliveryEndDate,BoardLotQuantity,TickSize,  
ListingDate,BookClosureStartDate,BookClosureEndDate,ExpulsionDate,  
'NA' as ReAdmissionDate,Remarks,NormalMarketEligibility,OddLotMarketEligibility,  
NormalMarketStatus,OddLotMarketStatus,AuctionMarketStatus,AuctionMarketEligibility,'NA' as Dividend,  
BasePrice as FaceValue,'NA' as DividendPercent from tbl_NSE_FOScripMaster  
Union  
select 'NSECD',Token,WarningQuantity,FreezeQuantity,IssueStartDate,ExpiryDate,  
NoDeliveryStartDate,NoDeliveryEndDate,BoardLotQuantity,TickSize,  
ListingDate,BookClosureStartDate,BookClosureEndDate,ExpulsionDate,  
'NA' as ReAdmissionDate,Remarks,NormalMarketEligibility,OddLotMarketEligibility,  
NormalMarketStatus,OddLotMarketStatus,AuctionMarketStatus,AuctionMarketEligibility,'NA' as Dividend,  
BasePrice as FaceValue,'NA' as DividendPercent from tbl_NSE_CDScripMaster  
Union  
select 'MCXSXCD',ScripCode,'NA' as WarningPercent,'NA' as FreezePercent,'NA' as IssueStartDate,'NA' as MaturityDate,  
DeliveryStartDate as NoDeliveryDateStart,DeliveryEndDate as NoDeliveryDateEnd,LotSize as BoardLotQuantity,TickSize,  
'NA' as ListingDate,'NA' as BookClosureStartDate,'NA' as BookClosureEndDate,'NA' as ExpulsionDate,  
'NA' as ReAdmissionDate,InstrumentDesc as Remarks,'NA' as NormalMarketEligibility,'NA' as OddLotMarketEligibility,  
'NA' as NormalMarketStatus,'NA' as OddLotMarketStatus,'NA' as AuctionMarketStatus,'NA' as AuctionMarketEligibility,  
'NA' as Dividend,FaceValue,'NA' as DividendPercent from Tbl_MCXSX_CDScripMaster
Union
select 'NCDEX',Token,WarningQuantity,FreezeQuantity,IssueStartDate,ExpiryDate,  
NoDeliveryStartDate,NoDeliveryEndDate,BoardLotQuantity,TickSize,  
ListingDate,BookClosureStartDate,BookClosureEndDate,ExpulsionDate,  
'NA' as ReAdmissionDate,Remarks,NormalMarketEligibility,OddLotMarketEligibility,  
NormalMarketStatus,OddLotMarketStatus,AuctionMarketStatus,AuctionMarketEligibility,'NA' as Dividend,  
BasePrice as FaceValue,'NA' as DividendPercent from tbl_NCDEX_ScripMaster
Union
select 'MCX',ScripCode,'NA' as WarningQuantity,'NA' as FreezeQuantity,ContractStartDate as IssueStartDate,ExpiryDate,  
'NA' as NoDeliveryStartDate,'NA' as NoDeliveryEndDate,MinLotQty as BoardLotQuantity,TickSize,  
'NA' as ListingDate,'NA' as BookClosureStartDate,'NA' as BookClosureEndDate,'NA' as ExpulsionDate,  
'NA' as ReAdmissionDate,Specification as Remarks,'NA' as NormalMarketEligibility,'NA' as OddLotMarketEligibility,  
'NA' as NormalMarketStatus,'NA' as OddLotMarketStatus,'NA' as AuctionMarketStatus,'NA' as AuctionMarketEligibility,'NA' as Dividend,  
FaceValue,'NA' as DividendPercent from Tbl_MCX_ScripMaster 
union
select 'BSEFO',ContactTokenNumber,'NA' as WarningPercent,'NA' as FreezePercent,ContractStartDate as IssueStartDate,'NA' as MaturityDate,  
'NA' as  NoDeliveryDateStart,'NA' as NoDeliveryDateEnd,BoardLotQty as BoardLotQuantity,TickSize as TickSize,  
'NA' as ListingDate,'NA' as BookClosureStartDate,'NA' as BookClosureEndDate,'NA' as ExpulsionDate,  
'NA' as ReAdmissionDate,'NA' as Remarks,'NA' as NormalMarketEligibility,'NA' as OddLotMarketEligibility,  
'NA' as NormalMarketStatus,'NA' as OddLotMarketStatus,'NA' as AuctionMarketStatus,'NA' as AuctionMarketEligibility,  
'NA' as Dividend,BasePrice as FaceValue,'NA' as DividendPercent from tbl_BSE_FOScripMaster
Union
select 'BSECD',ContactTokenNumber,'NA' as WarningPercent,'NA' as FreezePercent,ContractStartDate as IssueStartDate,'NA' as MaturityDate,  
'NA' as  NoDeliveryDateStart,'NA' as NoDeliveryDateEnd,BoardLotQty as BoardLotQuantity,TickSize as TickSize,  
'NA' as ListingDate,'NA' as BookClosureStartDate,'NA' as BookClosureEndDate,'NA' as ExpulsionDate,  
'NA' as ReAdmissionDate,'NA' as Remarks,'NA' as NormalMarketEligibility,'NA' as OddLotMarketEligibility,  
'NA' as NormalMarketStatus,'NA' as OddLotMarketStatus,'NA' as AuctionMarketStatus,'NA' as AuctionMarketEligibility,  
'NA' as Dividend,BasePrice as FaceValue,'NA' as DividendPercent from tbl_BSE_CDScripMaster 
GO
/****** Object:  View [dbo].[vw_CBOS_TradeFileForBO_BSE]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO



CREATE view [dbo].[vw_CBOS_TradeFileForBO_BSE]
as

select --top 10
'446'MemCode
,c.TerminalId [TerminalID]
,a.scripCode ScripCd
,ltrim(rtrim(b.ScripShortName))[Symbol]
,cast(a.tradePrice as float) MktRate
,a.TradeQty
,'11'OppMId
,'0'OppTrId
,CONVERT(VARCHAR(10), dateadd(ss,a.TradeDateTime,'1980-01-01'), 108)[Date1]
,CONVERT(VARCHAR(10), dateadd(ss,a.TradeDateTime,'1980-01-01'), 111)[Date2]
,a.ClientCode PartyCode
,a.OrderNo
,a.BookType 
,a.BuyOrSell
,a.TradeNo
,'CLIENT' InstId
,b.ScripIsInNo
,b.OptionType
,''SettNo
,convert(varchar(10),dateadd(ss,d.entrydatetime,'1980-01-01'),108)OrderEnteredModDtTime
,'1'AOPFlag
,c.UserRefText
,e.ProductName
,c.Source
,a.TerminalID[TerminalNo]
,c.remarks
from tbl_trade (nolock) A
left outer join tbl_Scrips (nolock) b on a.exchangecode = b.ExchangeCode and a.ScripCode = b.ScripCode
left outer join TBL_ORDER (nolock) C on C.BunchSeqNo=1 AND A.UniqueOrderId = C.UniqueOrderId
left outer join (select uniqueorderid,max(entrydatetime)entrydatetime from tbl_order (nolock) group by uniqueorderid) d on a.UniqueOrderId = d.UniqueOrderId
left outer join tbl_enum_ProductMaster e on c.ProductType = e.ProductCode
where a.exchangecode = 1




GO
/****** Object:  Table [dbo].[Tbl_WebReports_Log]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
