GO
SET QUOTED_IDENTIFIER ON
GO

CREATE View [dbo].[Vw_M2MSqoff_BOSMSClient]
as
       select a.Membercode,a.VPlusLossPercent,a.GlobalPercentage, a.Flag, b.MOBILENo as Mobile,b.Email as EmailId, b.Branch,
	   a.VPlusSqOffPercent,a.CommodityGlobalSqOffPercent
       from
       (
              select MemberCode,ROUND(VPlusLossPercent,2) AS VPlusLossPercent, ROUND(CommodityGlobalLossPercent,2) as GlobalPercentage, 'N' as Flag,
              Round(VPlusSqOffPercent,2) as VPlusSqOffPercent, Round(CommodityGlobalSqOffPercent,2) as CommodityGlobalSqOffPercent
			  from tbl_members 
			  where VPlusLossPercent>50 or (CommodityGlobalLossPercent>50 and M2MBasedSquareOffFired = 'Y')
			  /*where (VPlusLossPercent>50 or CommodityGlobalLossPercent>50)
              and M2MBasedSquareOffFired = 'Y'*/
       ) a ,
       (
       select ClientCode, MOBILENo,Email, Branch from PMP.DBO.Tbl_BOD_ClientActivation with(nolock)
       --where len(MOBILENo)>8 and isnumeric(MOBILENo)=1
       ) b
       where a.membercode=b.ClientCode


              
GO
