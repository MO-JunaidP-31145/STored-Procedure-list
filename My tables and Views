/****** Object:  View [dbo].[vw_MultipleLogins_IpAddressWise_new]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
--select * from vw_MultipleLogins_IpAddressWise_new

Create view  [dbo].[vw_MultipleLogins_IpAddressWise_new]
as
select  
GlobalIpAddress,UserCode,UserType,LogDate as MinLoginDt, LogDate as MaxLoginDt, 
 isnull(Latitude, 0) as Latitude , isnull(Longitude, 0) as Longitude, A.MacAddress,	IPAddress2
,
isnull(c.Branch,'') as Branch,
isnull(c.SubBroker,'') as SubBroker, 
isnull(d.DealerCode,'') as DealerCode,
isnull(c.ClientName,'') as ClientName,
isnull(d.DealerName,'') as DealerName,
case when e.ClientCode is null then 'Not Traded' else 'Traded' end [TradedToday]
from
(select * from dbo.tbl_Login_DetailLog with (nolock) where status = 'Success'and SessionNo <> 0 and LoginFrom = 'E'  
--and  UserCode = '37195' 
) A
left join 
(
select * from
(select  ROW_NUMBER() over (partition by SessionId order by SessionId ) as ID,   MemberCode, IPAddress2, Longitude, Latitude, SessionId   
from tbl_ClientSystemInformationCapture B with (nolock) 

   where   
   (Longitude <> '0' or Latitude <> '0'  )) C where C.ID = 1

) B
on A.UserCode = B.MemberCode and A.SessionNo = B.SessionId
 
left outer join pmp.dbo.Tbl_BOD_ClientActivation c with (nolock) on a.UserCode = c.ClientCode
left outer join pmp.dbo.Tbl_BOD_CTCLActivation d with (nolock) on a.UserCode = d.DealerCode
left outer join 
(
select ClientCode from dbo.tbl_Trade (nolock) group by ClientCode
) e on a.UserCode = e.ClientCode



GO
