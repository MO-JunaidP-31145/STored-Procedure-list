create view [dbo].[vw_PhysicalOptionsDeliveryMarking]
as
SELECT ltrim(rtrim(Clientcode))ClientCode,
a.ExchangeCode,a.ScripCode,ltrim(rtrim(b.ScripName))ScripName
,OpenQty,QtyMArkedForConvert
from tbl_PhysicalOptionsDelivery (nolock) a
inner join tbl_Scrips (nolock) b on a.ExchangeCode=b.ExchangeCode and a.ScripCode=b.ScripCode
where --month(CreatedDate) = month(getdate())
convert(varchar(7),CreatedDate,126) = convert(varchar(7), getdate (),126)
GO
