/****** Object:  View [dbo].[vw_MultipleLogins_IpAddressWise]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[vw_MultipleLogins_IpAddressWise]
as

with cte_Login  as 
(
select GlobalIpAddress,userCode,UserType,min(LogDate)MinLoginDt,max(LogDate)MaxLoginDt,
isnull(B.Latitude,0) as  Latitude, isnull(B.Longitude,0)  as Longitude, A.MacAddress as MacAddress, B.IPAddress2  
 from dbo.tbl_Login_DetailLog (nolock) A 
left join tbl_ClientSystemInformationCapture (nolock) B on A.UserCode = B.MemberCode
where GlobalIpAddress <> '' and status = 'Success'
group by GlobalIpAddress,userCode,UserType, B.Latitude, B.Longitude, A.MacAddress, IPAddress2
)

select a.*,c.Branch,
c.SubBroker
--,c.TraderCode
,d.DealerCode 
,c.ClientName
,d.DealerName
--,a.IPAddress2
--,a.MacAddress
,case when e.ClientCode is null then 'Not Traded' else 'Traded' end [TradedToday]
from cte_Login a 
inner join (select GlobalIpAddress from cte_Login group by GlobalIpAddress having count(1) > 1) b on a.GlobalIpAddress = b.GlobalIpAddress
left outer join pmp.dbo.Tbl_BOD_ClientActivation c on a.UserCode = c.ClientCode
left outer join pmp.dbo.Tbl_BOD_CTCLActivation d on a.UserCode = d.DealerCode
left outer join 
(
select ClientCode from dbo.tbl_Trade (nolock) group by ClientCode
) e on a.UserCode = e.ClientCode





GO
