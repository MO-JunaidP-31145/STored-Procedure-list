	[AdditionalShortMargin] [numeric](5, 2) NULL,
	[SpecialLongMargin] [numeric](5, 2) NULL,
	[SpecialShortMargin] [numeric](5, 2) NULL,
	[ScripFullName] [varchar](500) NULL,
	[FaceValue] [varchar](50) NULL,
	[StaggeredPeriodScrip] [char](1) NULL,
	[IsOnlyLimitOrderAllowed] [char](1) NULL,
	[SPScripCode1] [int] NULL,
	[SPScripCode2] [int] NULL,
	[CA] [char](1) NULL,
	[ULToken] [int] NULL,
	[PerClientAmount] [float] NULL,
	[PerClientQty] [int] NULL,
	[IsExpiry] [char](1) NOT NULL,
	[IsGsmScrip] [char](1) NULL,
	[MPVPAllowSqOff] [char](1) NULL,
	[PerClientQtyHNI] [bigint] NOT NULL,
	[PerClientAmountHNI] [float] NOT NULL,
	[IsCustomGTD] [char](1) NOT NULL,
	[IsPhysicalDerivScrip] [char](1) NOT NULL,
	[IsSLBM] [bit] NULL,
	[TERHighRange] [float] NOT NULL,
	[TERLowRange] [float] NOT NULL,
	[CFSVarPercent] [numeric](5, 2) NOT NULL,
	[PrevOI] [float] NULL,
	[VPPerClientAmt] [int] NOT NULL,
	[IsPreopenScrip] [char](1) NOT NULL,
	[ExchVarPercent] [numeric](5, 2) NULL,
	[CashEquivalentScrip] [char](1) NULL,
	[CCApprovedScrip] [char](1) NULL,
	[CombinedExchCode] [int] NULL,
	[CombinedScripCode] [int] NULL,
	[SurveillanceIndicator] [varchar](50) NULL,
	[SurveillanceIndicatorNew] [varchar](255) NULL,
	[mktPxProtect] [real] NOT NULL,
	[AllowSquareOffMTF] [char](1) NULL,
	[SellMarPer] [numeric](18, 6) NULL,
	[BuyMarPer] [numeric](18, 6) NULL,
	[ShortSellInValuePlus] [char](1) NOT NULL,
	[ShortSellInNormal] [char](1) NOT NULL,
	[SettlementType] [int] NULL,
	[T1ScripCode] [int] NULL
) ON [FG1]
GO
/****** Object:  View [dbo].[vw_PhysicalOption_View_BSEFO]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[vw_PhysicalOption_View_BSEFO]  
as  
select    
a.MemberCode [ClientCode]  
,c.MemberName [ClientName]  
,c.Branch  
,c.SubBroker  
,b.ScripShortName [Symbol]  
,b.InstrumentName [InstrumentType]  
,b.optionType [OptionType]  
,b.StrikePrice [StrikePrice]  
,convert(varchar,CAst(dateadd(ss,b.ExpiryDate ,'1980-01-01') as dateTime),106)[ExpiryDate]  
,a.BotQty - a.SoldQty [OpenQty]  
--,a.exposureMargin [Exposure]  
,ISNULL(d.MarketPrice,0) * (abs(a.BotQty - a.SoldQty)) [Exposure]  
,b.MarketPrice [LTP]  
,ISNULL(d.MarketPrice,0) [SpotPrice]  
,case when b.optionType = 'XX' then 0 else ISNULL(d.marketPrice,0) - b.strikePrice end [SpotMinusStrike]  

,CASE 
        WHEN B.OptionType = 'CE' AND B.StrikePrice <  D.MARKETPRICE THEN 'ITM' 
        WHEN B.OptionType = 'PE' AND B.StrikePrice >  D.MARKETPRICE THEN 'ITM' 
        WHEN B.OptionType = 'CE' AND B.StrikePrice >= D.MARKETPRICE THEN 'OTM' 
        WHEN B.OptionType = 'PE' AND B.StrikePrice <= D.MARKETPRICE THEN 'OTM' 
        ELSE '' 
    END
AS [ITMOTM]

,Round(c.baseCapital,2) [BaseCapital]
,c.IsNonPOA
,isnull(e.AvailableQty,0)AvailableQty  
,null [QtyToConvert]  
,b.marketLot  
,b.exchangeCode  
,ISNULL(f.OpenQty,0)[IntentQty],ISNULL(f.Exposure,0)[IntentExposure],ISNULL(f.LTP,0)[IntentLTP],ISNULL(f.SpotPrice,0)[IntentSportPrice],ISNULL(f.SpotMinusStrike,0)[IntentSpotMinusStrike]  
,a.ScripCode  
,ISNULL(f.id,0)id,ISNULL(f.ITMOTM,0)[IntentITMOTM],ISNULL(f.AvailableCash,0)[IntentAvailableCash],ISNULL(f.AvailableQty,0)[IntentAvailableQty],ISNULL(f.IntentonForDelivery,0)IntentonForDelivery,ISNULL(f.QtyMArkedForConvert,0) QtyMArkedForConvert 

,b.scripFullName 
,b.MarketPrice
,case 
when (a.BotQty - a.SoldQty) > 0 then Round((b.MarketPrice * (100 - (select top 1 CapitalM2MCircuitFilter from dbo.tbl_Config)))/100,1) 
when (a.BotQty - a.SoldQty) < 0 then Round((b.MarketPrice * (100 + (select top 1 CapitalM2MCircuitFilter from dbo.tbl_Config)))/100,1) end as Price
from dbo.tbl_MemberInstrument a with (nolock)  
inner join dbo.tbl_Scrips b with (nolock) on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.ScripCode  
left outer join
(
select ScripFullName,ScripShortName ,expirydate ,StrikePrice ,OptionType ,FOExposurePercent,MARKETPRICE,ScripCode,ExchangeCode from dbo.tbl_Scrips (nolock)
where ExchangeCode = 1 and MARKETPRICE > 0 and OptionType  in ('A')
)D on  D.ScripCode=B.ULToken

left outer join dbo.tbl_Members c with (nolock)  on a.membercode = c.MemberCode  
left outer join dbo.tbl_DPHoldings e with (nolock) on B.scripisinno = e.ScripIsInNo and a.MemberCode = e.MemberCode  
  
LEFT OUTER JOIN tbl_PhysicalOptionsDelivery F with (nolock) ON A.ExchangeCode = f.ExchangeCode and a.scripcode = f.scripcode and a.membercode = f.Clientcode  
INNER JOIN
(
SELECT EXPIRYDATE FROM (
select ROW_NUMBER() OVER(ORDER BY expirydate)ID,expirydate from dbo.tbl_Scrips b with (nolock) where ExchangeCode = 9 and optiontype='XX' and ScripShortName = 'ONGC'
)ID WHERE ID = 1
)G ON G.ExpiryDate = B.ExpiryDate
left outer join pmp.dbo.tbl_PhysicalOptionsException H on c.Branch = H.val and h.Typ = 'B'
left outer join pmp.dbo.tbl_PhysicalOptionsException I on c.SUBBROKER = I.val and I.Typ = 'S'
left outer join pmp.dbo.tbl_PhysicalOptionsException J on a.MemberCode = J.val and J.Typ = 'C'
left outer join dbo.tbl_MultipleExceptionMaster K on a.MemberCode = k.clientCode and k.exceptionType = 'PHYSICAL'
where b.IsFOScrip = 1 
and b.ExchangeCode = 9 
and b.optiontype in ('CE','PE','XX') 
and b.InstrumentName like '%STK%'  
and BotQty - SoldQty <> 0  
GO
