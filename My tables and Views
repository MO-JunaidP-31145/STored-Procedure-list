/****** Object:  View [dbo].[vw_CBOS_TradeFileForBO_BSECD]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO







CREATE view [dbo].[vw_CBOS_TradeFileForBO_BSECD]
as

select
a.TradeNo
,'11' [TradeStatus]/*dummy*/
,ltrim(rtrim(b.ScripShortName))[Symbol]
,B.InstrumentName [Instrument Type]
,REPLACE(CONVERT(VARCHAR,DATEADD(SS,B.ExpiryDate,'1980-01-01'),106),' ','-') ExpiryDate
,B.StrikePrice
,B.OPTIONTYPE
,ltrim(rtrim(b.ScripShortName)) + UPPER(SUBSTRING(CONVERT(VARCHAR,DATEADD(SS,B.ExpiryDate,'1980-01-01'),106),4,3))  + CASE WHEN OPTIONTYPE = 'XX' THEN 'FUT' ELSE 'OPT' END ScripShortName
,0[InstrumentType]/*dummy*/
,case when a.BookType = 'P' then '3' else '1' end BookType
,'1' MarketType
,'10412'[UserId]
,'1'[BranchId]/*dummy*/
,case when a.BuyOrSell = 'B' then '1' else '2' end [BuySellIndicator]
,a.TradeQty
,cast(a.tradePrice as float)/100 TradePrice
,'1' as ProCli/*dummy*/
,a.ClientCode ClientAC
,c.ParticipantCode
,'10'Settle/*dummy*/
,convert(varchar(20),dateadd(ss,a.TradeDateTime,'1980-01-01'),113)EntryDtTime
,convert(varchar(20),dateadd(ss,d.entrydatetime,'1980-01-01'),113)OrderEnteredModDtTime
,a.OrderNo
,'NIL'CPId/*dummy*/
,e.ProductName
,c.Source
,a.TerminalID[TerminalNo]
,c.Remarks
from tbl_trade (nolock) A
left outer join tbl_Scrips (nolock) b on a.exchangecode = b.ExchangeCode and a.ScripCode = b.ScripCode
left outer join TBL_ORDER (nolock) C on C.BunchSeqNo=1 AND A.UniqueOrderId = C.UniqueOrderId
left outer join (select uniqueorderid,max(entrydatetime)entrydatetime from tbl_order (nolock) group by uniqueorderid) d on a.UniqueOrderId = d.UniqueOrderId
left outer join tbl_enum_ProductMaster e on c.ProductType = e.ProductCode
where a.exchangecode = 10
	




GO
