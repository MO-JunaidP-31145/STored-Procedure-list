/****** Object:  View [dbo].[vw_Trading_Counts]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE view [dbo].[vw_Trading_Counts]
as
with cte_Time as(
select datediff(second,'1980-01-01',dateadd(second,-120,getdate())) as Dt)
select 01 as Ord,count(1)RecordCount,'tbl_trade'Tabletype from awacs10.dbo.tbl_trade (nolock) union
select 04 as Ord,count(1)RecordCount,'tbl_memberInstrument'Tabletype from awacs10.dbo.tbl_memberInstrument (nolock) union
select 05 as Ord,count(1)RecordCount,'tbl_dpholdings'Tabletype from awacs10.dbo.tbl_dpholdings (nolock) union
select 06 as Ord,count(1)RecordCount,'tbl_membersymbols'Tabletype from awacs10.dbo.tbl_membersymbols (nolock) union
select 03 as Ord,count(1)RecordCount,'tbl_ordermaster'Tabletype from awacs10.dbo.tbl_ordermaster (nolock) union
select 02 as Ord,count(1)RecordCount,'tbl_order'Tabletype  from awacs10.dbo.tbl_order (nolock)  union
select 07 as Ord,count(1)RecordCount,'tbl_trade 2 min old' from AWACS10.dbo.tbl_trade where TradeDateTime < (select dt from cte_Time)


GO
/****** Object:  View [dbo].[vw_PhysicalOption_View_Pre_Expiry]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[vw_PhysicalOption_View_Pre_Expiry] 
as

select   
a.MemberCode [ClientCode]  
,c.MemberName [ClientName]  
,c.Branch  
,c.SubBroker  
,b.ScripShortName [Symbol]  
,b.InstrumentName [InstrumentType]  
,b.optionType [OptionType]  
,b.StrikePrice [StrikePrice]  
,convert(varchar,CAst(dateadd(ss,b.ExpiryDate ,'1980-01-01') as dateTime),106)[ExpiryDate]  
,a.BotQty - a.SoldQty [OpenQty]  
--,a.exposureMargin [Exposure]  
,d.MarketPrice * (abs(a.BotQty - a.SoldQty)) [Exposure]  
,b.MarketPrice [LTP]  
,d.MarketPrice [SpotPrice]  
,case when b.optionType = 'XX' then 0 else d.marketPrice - b.strikePrice end [SpotMinusStrike]  
,case   
when b.optionType = 'CE' then case when (d.marketPrice - b.strikePrice) > 0 then 'ITM' else 'OTM' end  
when b.optionType = 'PE' then case when (d.marketPrice - b.strikePrice) < 0 then 'ITM' else 'OTM' end  
else '' end [ITMOTM]  

,Round(c.baseCapital,2) [BaseCapital]
,c.IsNonPOA
,isnull(e.AvailableQty,0)AvailableQty  
,null [QtyToConvert]  
,b.marketLot  
,b.exchangeCode  
,f.OpenQty[IntentQty],f.Exposure[IntentExposure],f.LTP[IntentLTP],f.SpotPrice[IntentSportPrice],f.SpotMinusStrike[IntentSpotMinusStrike]  
,a.ScripCode  
,f.id,f.ITMOTM[IntentITMOTM],f.AvailableCash[IntentAvailableCash],f.AvailableQty[IntentAvailableQty],f.IntentonForDelivery,f.QtyMArkedForConvert  

,b.scripFullName 
,b.MarketPrice
,case 
when (a.BotQty - a.SoldQty) > 0 then Round((b.MarketPrice * (100 - (select top 1 CapitalM2MCircuitFilter from dbo.tbl_Config)))/100,1) 
when (a.BotQty - a.SoldQty) < 0 then Round((b.MarketPrice * (100 + (select top 1 CapitalM2MCircuitFilter from dbo.tbl_Config)))/100,1) end as Price
from dbo.tbl_MemberInstrument a with (nolock)  
inner join dbo.tbl_Scrips b with (nolock) on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.ScripCode  
inner join   
(select a.ScripShortName,a.MarketPrice,a.ExchangeCode,a.ScripCode,a.ScripIsinNo from dbo.tbl_Scrips a with (nolock) left outer join  
(select ScripShortName,ULToken from dbo.tbl_Scrips b with (nolock) where ExchangeCode = 2 and optiontype in ('XX','CE','PE') group by ScripShortName,ULToken) b  
on a.scripshortname = b.scripshortname and a.ultoken = b.ultoken  
where a.exchangecode = 0 and b.ScripShortName is not null  
) d on b.ScripShortName = d.ScripShortName  
left outer join dbo.tbl_Members c with (nolock)  on a.membercode = c.MemberCode  
left outer join dbo.tbl_DPHoldings e with (nolock) on d.scripisinno = e.ScripIsInNo and a.MemberCode = e.MemberCode  
  
LEFT OUTER JOIN tbl_PhysicalOptionsDelivery F with (nolock) ON A.ExchangeCode = f.ExchangeCode and a.scripcode = f.scripcode and a.membercode = f.Clientcode  
INNER JOIN
--(
--SELECT EXPIRYDATE FROM (
--select ROW_NUMBER() OVER(ORDER BY expirydate)ID,expirydate from tbl_NSE_FOScripMaster where IssueMaturityDate <> ExpiryDate and OptionType = 'XX' 
--)ID WHERE ID = 1

(
select  Symbol , expirydate from tbl_NSE_FOScripMaster where IssueMaturityDate <> ExpiryDate and OptionType = 'XX'  and InstrumentName = 'FUTSTK'
)G ON G.ExpiryDate = B.ExpiryDate and G.Symbol = B.ScripShortName
left outer join pmp.dbo.tbl_PhysicalOptionsException H on c.Branch = H.val and h.Typ = 'B'
left outer join pmp.dbo.tbl_PhysicalOptionsException I on c.SUBBROKER = I.val and I.Typ = 'S'
left outer join pmp.dbo.tbl_PhysicalOptionsException J on a.MemberCode = J.val and J.Typ = 'C'
left outer join dbo.tbl_MultipleExceptionMaster K on a.MemberCode = k.clientCode and k.exceptionType = 'PHYSICAL'
where b.IsFOScrip = 1 and b.ExchangeCode = 2 and b.optiontype in ('CE','PE','XX') and b.InstrumentName like '%STK%'  
and BotQty - SoldQty <> 0  
AND (H.val IS NULL AND i.VAL IS NULL AND J.VAL IS NULL)
and K.Clientcode is null
--and (a.membercode like 'h169%')
--and a.MemberCode in ('','AA017','AA018','AA019')
--order by b.ScripShortName,a.MemberCode  

  
GO
