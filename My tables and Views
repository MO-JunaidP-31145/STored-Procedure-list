/****** Object:  UserDefinedFunction [dbo].[Func_NetPosition_CalcAvgPrice]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE FUNCTION [dbo].[Func_NetPosition_CalcAvgPrice](
														 @buyQty bigint,
                                                         @BuyAmt float,
                                                         @sellQty bigint,
                                                         @SellAmt float,
                                                         @DayBotQty bigint,
                                                         @DayBotAmount float,
                                                         @DaySoldQty bigint,
                                                         @DaySoldAmount float,
                                                         @ActualBotQty bigint,
                                                         @ActualBotAmount float,
                                                         @ActualSoldQty bigint,
                                                         @ActualSoldAmount float,
														 @MarketLot int,
														 @Exchangecode smallint,
														 @Multiplier int)
    RETURNS float
AS
BEGIN
 
			 if(@Exchangecode =6)
			 begin			 
			set @ActualBotAmount = case when @ActualBotQty > 0 then @ActualBotAmount / ((round(cast(@Multiplier  as float) / @MarketLot,4))) / @ActualBotQty else 0 end ;
			set @ActualBotQty = @ActualBotQty / @MarketLot;
			set @ActualBotAmount = @ActualBotAmount * @ActualBotQty;

			set @ActualSoldAmount = case when @ActualSoldQty > 0 then @ActualSoldAmount / ((round(cast(@Multiplier  as float) / @MarketLot,4))) / @ActualSoldQty else 0 end;
			set @ActualSoldQty = @ActualSoldQty / @MarketLot;
			set @ActualSoldAmount = @ActualSoldAmount * @ActualSoldQty;

			 end
			 else if(@Exchangecode =5)
			 begin
			set @ActualBotAmount = @ActualBotAmount / (round(cast(@Multiplier  as float),2)) ;
			set @ActualSoldAmount = @ActualSoldAmount / (round(cast(@Multiplier  as float),2));

			 end
			 declare @locActualBotAmount float  = @ActualBotAmount + @DayBotAmount;    
             declare @locActualSoldAmount float= @ActualSoldAmount + @DaySoldAmount;  
             declare @loclActualBotQty float= @ActualBotQty + @dayBotQty;
             declare @loclActualSoldQty float= @ActualSoldQty + @daySoldQty;
             declare @loclNetQty float= @buyQty - @sellQty;
			 declare @loclModBotQty bigint;
			 declare @loclModSoldQty bigint;
			 declare @NetAvgPrice float;
			

			 if(@Exchangecode = 6 and @MarketLot != @Multiplier)
			 begin
				set @loclModBotQty = @buyQty * @MarketLot;
				set @loclModSoldQty = @sellQty * @MarketLot;
			 end
			 else
			 begin
			 set @loclModBotQty = @buyQty;
			 set @loclModSoldQty = @sellQty;
			 end


			 if (@ActualBotQty > 0)
            begin
                if (@loclNetQty = 0)
                begin
                   set @NetAvgPrice = 0;
                end
                else if (@loclNetQty > 0 and @loclActualBotQty > 0)
                begin
                    set @NetAvgPrice  = @locActualBotAmount / @loclActualBotQty;
                end
                else if (@sellQty > 0)
                begin
                    set @NetAvgPrice  = @sellAmt /@sellQty;
                end
                else begin  set @NetAvgPrice = 0; end
            end
            else if (@ActualSoldQty > 0)
            begin
                if (@loclNetQty = 0)
                begin
                    set @NetAvgPrice  = 0;
                end
                else if (@loclNetQty < 0 and @loclActualSoldQty > 0)
                begin
                    set @NetAvgPrice  = @locActualSoldAmount / @loclActualSoldQty;
                end
                else if (@buyQty > 0)
                begin
                    set @NetAvgPrice  = @buyAmt / @buyQty;
                end
                else begin   set @NetAvgPrice = 0; end
            end
            else if (@ActualBotQty = 0 and @ActualSoldQty = 0)
            begin
                if (@loclNetQty = 0)
                begin
                    set @NetAvgPrice  = 0;
                end
                else if (@loclNetQty > 0 and  @buyQty > 0)
                begin
                    set @NetAvgPrice  = @buyAmt / @buyQty;
                end
				 else if (@loclNetQty < 0 and @sellQty > 0)
                begin
                    set @NetAvgPrice  = @sellAmt / @sellQty;
                end
                else begin set @NetAvgPrice  = 0; end
            end
			 else begin set @NetAvgPrice  = 0; end
            --else if (@ActualSoldQty = 0)
            --begin
            --    if (@loclNetQty = 0)
            --    begin
            --        set @NetAvgPrice  = 0;
            --    end
            --    else if (@loclNetQty < 0 and @sellQty > 0)
            --    begin
            --        set @NetAvgPrice  = @sellAmt / @sellQty;
            --    end
            --    else begin set @NetAvgPrice  = 0; end
            --end

			return @NetAvgPrice;


end


GO
/****** Object:  UserDefinedFunction [dbo].[GetEffectiveGrossAmount]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
Create Function [dbo].[GetEffectiveGrossAmount](@TotalQtyRemaining int, @ExchangeCode int,     
  @ScripCode int, @MemberCode char(12), @BuyOrSell char)    
RETURNS float    
AS    
BEGIN    
  Declare @Value float, @NettQtyShortOptionValue float, @OrderShortOptionValue float,    
    @OrderShortOptionValueWithThisOrder float, @BotQty int, @SoldQty int,     
    @SoldOrderQty int, @QtyRemaining int, @NettAmount float, @BotOrderAmount float,     
    @SoldOrderAmount float, @TradeForTrade char, @OrderNettAmount float, @TempValue float,    
    @wGEMultiplier float     
    
  Set @Value = 0    
  Set @TempValue = 0    
  Set @QtyRemaining = 0    
  Set @OrderNettAmount = 0    
  Set @NettQtyShortOptionValue = 0    
  Set @OrderShortOptionValue = 0    
  Set @OrderShortOptionValueWithThisOrder = 0    
    
  select @BotQty = BotQty, @SoldQty = SoldQty, @SoldOrderQty = SoldOrderQty,    
    @NettAmount = NettAmount, @BotOrderAmount = BotOrderAmount, @SoldOrderAmount = SoldOrderAmount    
  from Aw_MemberScrips(NOLOCK) where MemberCode = @MemberCode and ScripCode = @ScripCode and ExchangeCode = @ExchangeCode      
    
  set @BotQty = IsNull(@BotQty,0)        
  set @SoldQty = IsNull(@SoldQty,0)        
  set @SoldOrderQty = IsNull(@SoldOrderQty,0)    
  set @NettAmount = IsNull(@NettAmount,0)        
  set @BotOrderAmount = IsNull(@BotOrderAmount,0)        
  set @SoldOrderAmount = IsNull(@SoldOrderAmount,0)    
    
  select @TradeForTrade = IsTradeForTradeSegment,  
    @wGEMultiplier = Isnull(GEMultiplier,0)/Power(10,Isnull(GENumberOfDecimals,0))   
  from Aw_Scrips(NOLOCK) where ScripCode = @ScripCode and ExchangeCode = @ExchangeCode    
    
  set @wGEMultiplier = case when @wGEMultiplier > 0 then @wGEMultiplier else 1 end

  if (@BotQty - @SoldQty) < 0     
  begin    
    Set @QtyRemaining = (@BotQty - @SoldQty)    
    Set @NettQtyShortOptionValue = dbo.GetShortOptionValue(@QtyRemaining, @ExchangeCode, @ScripCode)    
  end    
    
  if (@BotQty - @SoldQty) > 0    
  begin    
    if (@BotQty - @SoldQty) < @SoldOrderQty    
    begin    
      Set @QtyRemaining = @SoldOrderQty - (@BotQty - @SoldQty)    
      Set @OrderShortOptionValue = dbo.GetShortOptionValue(@QtyRemaining, @ExchangeCode, @ScripCode)    
    end    
  end    
  else    
  begin    
  Set @OrderShortOptionValue = dbo.GetShortOptionValue(@SoldOrderQty, @ExchangeCode, @ScripCode)    
 end    
    
  Set @OrderShortOptionValueWithThisOrder = @OrderShortOptionValue    
 Set @OrderShortOptionValueWithThisOrder = dbo.GetEffectiveTurnoverAmount(@TotalQtyRemaining,    
    @ExchangeCode, @ScripCode, @MemberCode, @BuyOrSell)    
    
  if @TradeForTrade = 'Y'    
  begin    
    Set @OrderNettAmount = @NettAmount + @BotOrderAmount + @SoldOrderAmount    
  end    
  else    
  begin    
  if Abs((@NettAmount - @NettQtyShortOptionValue) + @BotOrderAmount) >     
      Abs((@NettAmount - @NettQtyShortOptionValue) - (@SoldOrderAmount + @OrderShortOptionValue))    
    begin    
      Set @OrderNettAmount = Abs((@NettAmount - @NettQtyShortOptionValue) + @BotOrderAmount)    
  end    
    else    
    begin    
      Set @OrderNettAmount = Abs((@NettAmount - @NettQtyShortOptionValue) - (@SoldOrderAmount + @OrderShortOptionValue))    
  end    
 end    
    
  if @TradeForTrade = 'Y'    
  begin    
    Set @TempValue = @NettAmount + @BotOrderAmount + @SoldOrderAmount    
  end    
  else    
  begin    
  if @BuyOrSell = 'B'     
    begin    
      Set @TempValue = Abs((@NettAmount - @NettQtyShortOptionValue) + @BotOrderAmount)    
  end    
    else    
    begin    
      Set @TempValue = Abs((@NettAmount - @NettQtyShortOptionValue) - (@SoldOrderAmount + @OrderShortOptionValueWithThisOrder))    
  end    
 end    
      
  Set @Value = (@TempValue - Abs(@OrderNettAmount))    
  Return (@Value * @wGEMultiplier)   
END     


GO
/****** Object:  UserDefinedFunction [dbo].[GetEffectiveTurnoverAmount]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
Create Function [dbo].[GetEffectiveTurnoverAmount](@TotalQtyRemaining int, @ExchangeCode int, 
  @ScripCode int, @MemberCode char(12), @BuyOrSell char)
RETURNS float
AS
BEGIN
  Declare @Value float, @BotQty int, @SoldQty int, @SoldOrderQty int, @QtyRemaining int

  Set @Value = 0
  Set @QtyRemaining = 0

  select @BotQty = BotQty, @SoldQty = SoldQty, @SoldOrderQty = SoldOrderQty from Aw_MemberScrips(NOLOCK) 
  where MemberCode = @MemberCode and ScripCode = @ScripCode and ExchangeCode = @ExchangeCode  

  set @BotQty = IsNull(@BotQty,0)    
  set @SoldQty = IsNull(@SoldQty,0)    
  set @SoldOrderQty = IsNull(@SoldOrderQty,0)

  if @BuyOrSell = 'S' 
  begin
    if (@BotQty - @SoldQty) > 0
    begin
      if (@BotQty - @SoldQty) < (@SoldOrderQty + @TotalQtyRemaining)
      begin
        Set @QtyRemaining = (@SoldOrderQty + @TotalQtyRemaining) - (@BotQty - @SoldQty)
        Set @Value = dbo.GetShortOptionValue(@QtyRemaining, @ExchangeCode, @ScripCode)
      end
    end
    else
      set @Value = dbo.GetShortOptionValue(@TotalQtyRemaining, @ExchangeCode, @ScripCode)
  end
  
  Return @Value
END                 

GO
/****** Object:  UserDefinedFunction [dbo].[GetNettExposuresForCombinedLimits]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
Create Function [dbo].[GetNettExposuresForCombinedLimits](@BuyOrSell char, @OrderAmount float,   
  @ExchangeCode int, @MemberCode char(12))  
RETURNS float  
AS  
BEGIN  
  Declare @Value float, @NettAmount float, @FONettAmount float, @BotOrderAmount float,  
    @FOBotOrderAmount float, @SoldOrderAmount float, @FOSoldOrderAmount float, @TotalAmount float,   
    @SoldAmount float, @BotAmount float  
  
  Select @NettAmount = NettAmount, @FONettAmount = FONettAmount, @BotOrderAmount = BotOrderAmount,  
    @FOBotOrderAmount = FOBotOrderAmount, @SoldOrderAmount = SoldOrderAmount,  
    @FOSoldOrderAmount = FOSoldOrderAmount  
  from Aw_Members(NOLOCK) where MemberCode = @MemberCode and ExchangeCode = @ExchangeCode  
  
  Set @Value = 0  
  Set @TotalAmount = 0  
  Set @NettAmount = Isnull(@NettAmount,0)   
  Set @FONettAmount = Isnull(@FONettAmount,0)   
  Set @BotOrderAmount = Isnull(@BotOrderAmount,0)   
  Set @FOBotOrderAmount = Isnull(@FOBotOrderAmount,0)   
  Set @SoldOrderAmount = Isnull(@SoldOrderAmount,0)   
  Set @FOSoldOrderAmount = Isnull(@FOSoldOrderAmount,0)   
  
  Set @TotalAmount = @NettAmount + @FONettAmount  
  if @BuyOrSell = 'B'   
  begin  
    Set @BotAmount = @TotalAmount + @BotOrderAmount + @FOBotOrderAmount + @OrderAmount  
    Set @SoldAmount = @TotalAmount - (@SoldOrderAmount + @FOSoldOrderAmount)  
    if Abs(@BotAmount) > Abs(@SoldAmount)  
      Set @Value = @TotalAmount + @BotOrderAmount + @FOBotOrderAmount  
    else  
      Set @Value = @TotalAmount - (@SoldOrderAmount + @FOSoldOrderAmount)  
 end    
  else  
  begin  
    Set @BotAmount = @TotalAmount + @BotOrderAmount + @FOBotOrderAmount  
    Set @SoldAmount = @TotalAmount - (@SoldOrderAmount + @FOSoldOrderAmount + @OrderAmount)  
    if Abs(@BotAmount) > Abs(@SoldAmount)  
      Set @Value = @TotalAmount + @BotOrderAmount + @FOBotOrderAmount   
    else  
      Set @Value = @TotalAmount - (@SoldOrderAmount + @FOSoldOrderAmount)  
  end  
  
  Return @Value  
END   

GO
/****** Object:  UserDefinedFunction [dbo].[GetShortOptionValue]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
Create Function [dbo].[GetShortOptionValue] (@TotalQtyRemaining int, @ExchangeCode int,   
  @ScripCode int)  
RETURNS float  
AS  
BEGIN  
  Declare @Value float, @IsFOScrip int, @InstrumentName char(6), @StrikePrice float  
    
  select @IsFOScrip = IsFOScrip, @InstrumentName = InstrumentName, @StrikePrice = StrikePrice    
  from Aw_Scrips(NOLOCK) where  ScripCode = @ScripCode and ExchangeCode = @ExchangeCode  
  
  Set @Value = 0   
  set @StrikePrice = Isnull(@StrikePrice,0)  

  if @TotalQtyRemaining > 0   
  begin  
    if (@IsFOScrip = 1) and (SubString(Upper(@InstrumentName),1,3) = 'OPT')  
    begin  
      set @Value = (@StrikePrice * @TotalQtyRemaining)  
    end  
  end  
  
  Return @Value  
END     


GO
/****** Object:  UserDefinedFunction [dbo].[isValidEmailFormat]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE FUNCTION [dbo].[isValidEmailFormat]
(
    @Email varchar(100)
)
RETURNS bit
AS
BEGIN
    DECLARE @pattern varchar(4000)
    SET @pattern = '[a-zA-Z0-9_\-]+@([a-zA-Z0-9_\-]+\.)+([a-zA-Z0-9_\-])'
    DECLARE @Result bit

    DECLARE @objRegexExp INT
    EXEC sp_OACreate 'VBScript.RegExp', @objRegexExp OUT

    EXEC sp_OASetProperty @objRegexExp, 'Pattern', @pattern
    EXEC sp_OASetProperty @objRegexExp, 'IgnoreCase', 1
    EXEC sp_OASetProperty @objRegexExp, 'MultiLine', 0
    EXEC sp_OASetProperty @objRegexExp, 'Global', false
    EXEC sp_OASetProperty @objRegexExp, 'CultureInvariant', true

    EXEC sp_OAMethod @objRegexExp, 'Test', @Result OUT, @Email

    EXEC sp_OADestroy @objRegexExp

    RETURN @Result
END
GO
/****** Object:  UserDefinedFunction [dbo].[RandomnumberPassword]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

create function [dbo].[RandomnumberPassword]()
RETURNS int
 as
    begin
    DECLARE @RETURN int
    DECLARE @Upper INT;
    DECLARE @Lower INT;
    DECLARE @Random float;

    SELECT @Random = rndResult
    FROM vw_RandomNumberview

      SET @Lower = 1000 
      SET @Upper = 9999 
      set @RETURN= (ROUND(((@Upper - @Lower -1) * @Random + @Lower), 0))

 return @RETURN
 end;

 
GO
/****** Object:  UserDefinedFunction [dbo].[RemoveNonAlphaCharacters]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE Function [dbo].[RemoveNonAlphaCharacters](@Temp VarChar(1000))
Returns VarChar(1000)
AS
Begin

    Declare @KeepValues as varchar(50)
    Set @KeepValues = '%[^a-z 0-9]%'
    While PatIndex(@KeepValues, @Temp) > 0
        Set @Temp = Stuff(@Temp, PatIndex(@KeepValues, @Temp), 1, '')

    Return @Temp
End
GO
/****** Object:  UserDefinedFunction [dbo].[RT_FNO_Maximum]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create function [dbo].[RT_FNO_Maximum](@A varchar(3000))
returns decimal(24,6)
--WITH ENCRYPTION
as
BEGIN
declare @V1 decimal(24,6) ,
@V2 decimal(24,6)

set @V1 =substring(@A,1, charindex(',',@A)-1)
set @A=substring(@A, charindex(',',@A)+1,len(@A))
While charindex(',',@a) > 0
begin
set @V2 =substring(@A,1, charindex(',',@A)-1)
set @A=substring(@A, charindex(',',@A)+1,len(@A))
if @V1 <  @V2
begin
set @V1=@V2
continue
end
end
return @V1
END
GO
/****** Object:  UserDefinedFunction [dbo].[SplitString_OPENAPI]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create FUNCTION [dbo].[SplitString_OPENAPI]
(
@Input NVARCHAR(MAX),
@Delimiter CHAR(1)
)

Returns @Output Table( Value Nvarchar(MAX))
AS 
BEGIN
DECLARE @Start INT , @End INT
SET @Start = 1
SET @End = CHARINDEX(@Delimiter,@Input)
While @Start <= LEN(@Input)
BEGIN
	IF @End = 0 
		SET @End = LEN(@Input)+1
		Insert into @Output (Value)
		Values (SUBSTRING(@Input,@Start,@End-@Start))

		SET @Start = @End +1
		SET @End = CHARINDEX(@Delimiter,@Input,@Start)

		END
		RETURN
		END
GO
/****** Object:  UserDefinedFunction [dbo].[ufn_DatetimeToString]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE function [dbo].[ufn_DatetimeToString]
(
@dt datetime
)
returns varchar(30)
as
begin
declare @s varchar(50)
set @s =  REPLACE(CONVERT(VARCHAR(11),@dt, 106), ' ', '-') 
 + ' ' + CONVERT(VARCHAR(8), @dt, 108)
return @s
end
GO
/****** Object:  UserDefinedFunction [dbo].[usp_getSlippageForSQOrder]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


CREATE function [dbo].[usp_getSlippageForSQOrder]
(
@marketPrice float, @noOfDecimals int,@TickSize float,@buyOrSell char(1),@CircuitFilter int
)
returns float
as
begin
declare @tickFactor float
set @tickfactor = 1/@ticksize

declare @res float
if(@buyOrSell='B')
begin
set @res = @marketPrice + (@CircuitFilter * @marketPrice)/100
set @res = ceiling(@res * @tickFactor)/@tickFactor
end
else
begin
set @res = @marketPrice - (@CircuitFilter * @marketPrice)/100
set @res = ceiling(@res * @tickFactor)/@tickFactor
end

return round(round(@res,@noofDecimals) * power(10,@noofDecimals),0)

end




GO
