select ExchangeCode, ScripCode, ScripName, ScripShortName, MarketLot,  ExpiryDate, OptionType, StrikePrice from tbl_Scrips with (nolock)  
GO
/****** Object:  View [dbo].[vw_Trading_AvgPrices]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create view [dbo].[vw_Trading_AvgPrices]
as
select 2 as Ord,'NSEFO' Exchange,0 BuyAvgPrice,
sum(case when ActualBotQty <> 0 then 1 else 0 end)[ActualBotQty] ,
sum(case when ActualSoldQty <> 0 then 1 else 0 end)[ActualSotQty] ,
sum(case when ActualGainLoss <> 0 then 1 else 0 end)[ActualGainLoss] ,
sum(case when actualAveragePrice <> 0 then 1 else 0 end)[ActualAveragePrice] ,
sum(case when BotQty <> 0 then 1 else 0 end)[BotQty] ,
sum(case when SoldQty <> 0 then 1 else 0 end)[SoldQty] 
from AWACS10.dbo.tbl_MemberInstrument (nolock) where ExchangeCode=2 and (ActualBotQty <> 0 or ActualSoldQty <> 0)
union
select 3 as Ord,'NSECD' Exchange,0 BuyAvgPrice,
sum(case when ActualBotQty <> 0 then 1 else 0 end)[ActualBotQty] ,
sum(case when ActualSoldQty <> 0 then 1 else 0 end)[ActualSotQty] ,
sum(case when ActualGainLoss <> 0 then 1 else 0 end)[ActualGainLoss] ,
sum(case when actualAveragePrice <> 0 then 1 else 0 end)[ActualAveragePrice]  ,
sum(case when BotQty <> 0 then 1 else 0 end)[BotQty] ,
sum(case when SoldQty <> 0 then 1 else 0 end)[SoldQty] 
from AWACS10.dbo.tbl_MemberInstrument (nolock) where ExchangeCode=3 and (ActualBotQty <> 0 or ActualSoldQty <> 0)
union
select 4 as Ord,'MCX' Exchange,0 BuyAvgPrice,
sum(case when ActualBotQty <> 0 then 1 else 0 end)[ActualBotQty] ,
sum(case when ActualSoldQty <> 0 then 1 else 0 end)[ActualSotQty] ,
sum(case when ActualGainLoss <> 0 then 1 else 0 end)[ActualGainLoss] ,
sum(case when actualAveragePrice <> 0 then 1 else 0 end)[ActualAveragePrice] ,
sum(case when BotQty <> 0 then 1 else 0 end)[BotQty] ,
sum(case when SoldQty <> 0 then 1 else 0 end)[SoldQty] 
from AWACS10.dbo.tbl_MemberInstrument (nolock) where ExchangeCode=6 and (ActualBotQty <> 0 or ActualSoldQty <> 0)
union
select 5 as Ord,'NCDEX' Exchange,0 BuyAvgPrice,
sum(case when ActualBotQty <> 0 then 1 else 0 end)[ActualBotQty] ,
sum(case when ActualSoldQty <> 0 then 1 else 0 end)[ActualSotQty] ,
sum(case when ActualGainLoss <> 0 then 1 else 0 end)[ActualGainLoss] ,
sum(case when actualAveragePrice <> 0 then 1 else 0 end)[ActualAveragePrice] ,
sum(case when BotQty <> 0 then 1 else 0 end)[BotQty] ,
sum(case when SoldQty <> 0 then 1 else 0 end)[SoldQty] 
from AWACS10.dbo.tbl_MemberInstrument (nolock) where ExchangeCode=5 and (ActualBotQty <> 0 or ActualSoldQty <> 0)
union
select 6 as Ord,'BSECD' Exchange,0 BuyAvgPrice,
sum(case when ActualBotQty <> 0 then 1 else 0 end)[ActualBotQty] ,
sum(case when ActualSoldQty <> 0 then 1 else 0 end)[ActualSotQty] ,
sum(case when ActualGainLoss <> 0 then 1 else 0 end)[ActualGainLoss] ,
sum(case when actualAveragePrice <> 0 then 1 else 0 end)[ActualAveragePrice] ,
sum(case when BotQty <> 0 then 1 else 0 end)[BotQty] ,
sum(case when SoldQty <> 0 then 1 else 0 end)[SoldQty] 
from AWACS10.dbo.tbl_MemberInstrument (nolock) where ExchangeCode=10 and (ActualBotQty <> 0 or ActualSoldQty <> 0)
union
select 1 as Ord,'Holding',count(1)BuyAvgPrice,0,0,0,0,
(SELECT sum(case when BotQty <> 0 then 1 else 0 end)[BotQty] from AWACS10.dbo.tbl_MemberInstrument (nolock) where ExchangeCode in(0,1)),
(SELECT sum(case when SoldQty <> 0 then 1 else 0 end)[SoldQty] from AWACS10.dbo.tbl_MemberInstrument (nolock) where ExchangeCode in(0,1))
 from awacs10.dbo.tbl_DPHoldings with (nolock) where BuyAvgPrice <> 0
GO
