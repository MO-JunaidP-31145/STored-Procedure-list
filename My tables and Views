ALTER TABLE [dbo].[temp_MemberMaster]  WITH NOCHECK ADD  CONSTRAINT [CK_temp_MemberMaster_MaxPerLotValue_Positive] CHECK  (([MaxPerLotValue]>=(0)))
GO
ALTER TABLE [dbo].[temp_MemberMaster] CHECK CONSTRAINT [CK_temp_MemberMaster_MaxPerLotValue_Positive]
GO
