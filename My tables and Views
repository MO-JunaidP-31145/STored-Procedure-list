	[ClientCode] [varchar](50) NULL,
	[BuyOrSell] [char](1) NULL,
	[DisclosedQty] [int] NULL,
	[TotalQtyRemaining] [int] NULL,
	[OrderQty] [int] NULL,
	[QtyTradedToday] [int] NULL,
	[Price] [int] NULL,
	[TriggerPrice] [int] NULL,
	[GoodTillDate] [int] NULL,
