/****** Object:  View [dbo].[vw_CBOS_TradeFileForBO_BSE_FNO]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
Create View [dbo].[vw_CBOS_TradeFileForBO_BSE_FNO]
as
select
a.TradeNo
,'11' [TradeStatus]/*dummy*/
,ltrim(rtrim(b.ScripShortName))[Symbol]
,B.InstrumentName [Instrument Type]
,REPLACE(CONVERT(VARCHAR,DATEADD(SS,B.ExpiryDate,'1980-01-01'),106),' ','-') ExpiryDate
,B.StrikePrice
,B.OPTIONTYPE
,ltrim(rtrim(b.ScripShortName)) + UPPER(SUBSTRING(CONVERT(VARCHAR,DATEADD(SS,B.ExpiryDate,'1980-01-01'),106),4,3))  + CASE WHEN OPTIONTYPE = 'XX' THEN 'FUT' ELSE 'OPT' END ScripShortName
,0[InstrumentType]/*dummy*/
,case when a.BookType = 'P' then '3' else '1' end BookType
,'1' MarketType
,'10412'[UserId]
,'1'[BranchId]/*dummy*/
,case when a.BuyOrSell = 'B' then '1' else '2' end [BuySellIndicator]
,a.TradeQty
,cast(a.tradePrice as float)/100 TradePrice
,'1' as ProCli/*dummy*/
,a.ClientCode ClientAC
,c.ParticipantCode
,'10'Settle/*dummy*/
,convert(varchar(20),dateadd(ss,a.TradeDateTime,'1980-01-01'),113)EntryDtTime
,convert(varchar(20),dateadd(ss,d.entrydatetime,'1980-01-01'),113)OrderEnteredModDtTime
,a.OrderNo
,'NIL'CPId/*dummy*/
,e.ProductName
,c.Source
,a.TerminalID[TerminalNo]
,c.remarks
from tbl_trade (nolock) A
left outer join tbl_Scrips (nolock) b on a.exchangecode = b.ExchangeCode and a.ScripCode = b.ScripCode
left outer join TBL_ORDER (nolock) C on C.BunchSeqNo=1 AND A.UniqueOrderId = C.UniqueOrderId
left outer join (select uniqueorderid,max(entrydatetime)entrydatetime from tbl_order (nolock) group by uniqueorderid) d on a.UniqueOrderId = d.UniqueOrderId
left outer join tbl_enum_ProductMaster e on c.ProductType = e.ProductCode
where a.exchangecode = 9
	




GO
/****** Object:  View [dbo].[vw_Trading_5050_MemberMArginDetails]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[vw_Trading_5050_MemberMArginDetails]
as

select    
A.MemberCode, 
B.BaseCapital, 
A.ScripCapital	,
-- A.MarginPaid,
case when A.MarginPaid < 0  then 0 else A.MarginPaid end as MarginPaid,
A.InitialMargin	,
A.ExposureMargin	,
A.StockCapital	,
A.VarIntraIntialMargin	,
A.VarDeliveryInitialMargin	,
A.WithDrawnAmount	,
A.FundTransferAmt	,
A.MPLUSMargin	,
A.MPLUSFOMargin	,
A.ValuePlusFOMargin	,
A.StockCapitalCommodity	,
A.InitialMarginCommodity	,
A.CommodityExposureMargin	,
A.InitialMarginCurrency	,
A.CurrencyExposureMargin	,
A.StockCapitalDelivery	,
A.StockCapitalFO	,
A.StockCapitalCurrency	,
A.UnRealisedAmount	,
A.MaxUnrealisedAmount	,
A.MFEquityAmount	,
A.AdditionalLongMarginBlocked	,
A.AdditionalShortMarginBlocked	,
A.SpecialLongMarginBlocked	,
A.SpecialShortMarginBlocked	,
A.InitialMarginNCDEX	,
A.NCDEXExposureMargin	,
A.StockBH	,
A.ValuePlusEquityMargin	,
A.ValuePlusCurrencyMargin	,
A.ValuePlusFnOMargin	,
A.EPLUSFOMargin	,
A.EPLUSCurrMargin	,
A.AdditionalLongMarginBlockedNcdex	,
A.AdditionalShortMarginBlockedNcdex	,
A.SpecialLongMarginBlockedNcdex	,
A.SpecialShortMarginBlockedNcdex	,
A.AdditionalLongMarginBlockedFO	,
A.AdditionalShortMarginBlockedFO	,
A.SLBIntialMargin	,
A.TotalCFSAmount	,
A.StockCapitalMtf	,
A.MtfInitialMargin	,
A.ScripCapitalCash,
A.StockCapitalCash,
A.StockCapitalCashApproved,
A.MTFMargin,  
A.PhysicalMargin,
A.CashBrokerage, 
A.FOBrokerage, 
A.CurrBrokerage, 
A.CommBrokerage

from 
(select 
UserUCC as MemberCode,
--case when UserUCC = '' then MemberCode else UserUCC end as MemberCode	,
--UserUCC as MemberCode	,
sum(BaseCapital) as BaseCapital	,
sum(ScripCapital) as ScripCapital	,
sum(MarginPaid) as MarginPaid,
sum(InitialMargin) as InitialMargin	,
sum(ExposureMargin) as ExposureMargin	,
sum(StockCapital) as StockCapital,
sum(VarIntraIntialMargin) as VarIntraIntialMargin,
sum(VarDeliveryInitialMargin) as VarDeliveryInitialMargin	,
sum(WithDrawnAmount) as WithDrawnAmount	,
sum(FundTransferAmt) as FundTransferAmt ,
sum(MPLUSMargin) as MPLUSMargin	,
sum(MPLUSFOMargin) as MPLUSFOMargin	,
sum(ValuePlusFOMargin) as ValuePlusFOMargin	,
sum(StockCapitalCommodity) as StockCapitalCommodity	,
sum(InitialMarginCommodity) as InitialMarginCommodity	,
sum(CommodityExposureMargin) as CommodityExposureMargin	,
sum(InitialMarginCurrency) as InitialMarginCurrency	,
sum(CurrencyExposureMargin)	as CurrencyExposureMargin ,
sum(StockCapitalDelivery) as StockCapitalDelivery	,
sum(StockCapitalFO) as StockCapitalFO	,
sum(StockCapitalCurrency) as StockCapitalCurrency	,
sum(UnRealisedAmount) as UnRealisedAmount	,
sum(MaxUnrealisedAmount) as MaxUnrealisedAmount	,
sum(MFEquityAmount) as MFEquityAmount	,
sum(AdditionalLongMarginBlocked) as AdditionalLongMarginBlocked	,
sum(abs(AdditionalShortMarginBlocked))AdditionalShortMarginBlocked	,
sum(SpecialLongMarginBlocked) as SpecialLongMarginBlocked	,
sum(SpecialShortMarginBlocked) SpecialShortMarginBlocked	,
sum(InitialMarginNCDEX) as InitialMarginNCDEX	,
sum(NCDEXExposureMargin) as NCDEXExposureMargin ,
sum(StockBH) as StockBH	,
sum(ValuePlusEquityMargin) as ValuePlusEquityMargin	,
sum(ValuePlusCurrencyMargin) as ValuePlusCurrencyMargin	,
sum(ValuePlusFnOMargin) as ValuePlusFnOMargin	,
sum(EPLUSFOMargin) as EPLUSFOMargin	,
sum(EPLUSCurrMargin) as EPLUSCurrMargin	,
sum(AdditionalLongMarginBlockedNcdex) as AdditionalLongMarginBlockedNcdex	,
sum(abs(AdditionalShortMarginBlockedNcdex))AdditionalShortMarginBlockedNcdex	,
sum(SpecialLongMarginBlockedNcdex) as SpecialLongMarginBlockedNcdex	,
sum(SpecialShortMarginBlockedNcdex) as SpecialShortMarginBlockedNcdex	,
sum(AdditionalLongMarginBlockedFO)	as AdditionalLongMarginBlockedFO ,
sum(AdditionalShortMarginBlockedFO) as AdditionalShortMarginBlockedFO	,
sum(SLBIntialMargin) as SLBIntialMargin	,
sum(TotalCFSAmount) as TotalCFSAmount	,
sum(StockCapitalMtf) as StockCapitalMtf 	,
sum(MtfInitialMargin) as MtfInitialMargin	,
sum(MTFMargin) as MTFMargin,   
sum(PhysicalMargin) as PhysicalMargin, 
sum(CashBrokerage) as CashBrokerage, 
sum(FOBrokerage) as FOBrokerage, 
sum(CurrBrokerage) as CurrBrokerage, 
sum(CommBrokerage) as CommBrokerage, 

0 ScripCapitalCash,0 StockCapitalCash,0 StockCapitalCashApproved



from awacs10.dbo.tbl_members with (nolock)  where  UserCat <>  0
group by UserUCC ) A 
inner join 

(

select 
MemberCode	,
BaseCapital	,
0 ScripCapital	,
0 MarginPaid	,
0 InitialMargin	,
0 ExposureMargin	,
0 StockCapital	,
0 VarIntraIntialMargin	,
0 VarDeliveryInitialMargin	,
0 WithDrawnAmount	,
0 FundTransferAmt	,
0 MPLUSMargin	,
0 MPLUSFOMargin	,
0 ValuePlusFOMargin	,
0 StockCapitalCommodity	,
0 InitialMarginCommodity	,
0 CommodityExposureMargin	,
0 InitialMarginCurrency	,
0 CurrencyExposureMargin	,
0 StockCapitalDelivery	,
0 StockCapitalFO	,
0 StockCapitalCurrency	,
0 UnRealisedAmount	,
0 MaxUnrealisedAmount	,
0 MFEquityAmount	,
0 AdditionalLongMarginBlocked	,
0 AdditionalShortMarginBlocked	,
0 SpecialLongMarginBlocked	,
0 SpecialShortMarginBlocked	,
0 InitialMarginNCDEX	,
0 NCDEXExposureMargin	,
0 StockBH	,
0 ValuePlusEquityMargin	,
0 ValuePlusCurrencyMargin	,
0 ValuePlusFnOMargin	,
0 EPLUSFOMargin	,
0 EPLUSCurrMargin	,
0 AdditionalLongMarginBlockedNcdex	,
0 AdditionalShortMarginBlockedNcdex	,
0 SpecialLongMarginBlockedNcdex	,
0 SpecialShortMarginBlockedNcdex	,
0 AdditionalLongMarginBlockedFO	,
0 AdditionalShortMarginBlockedFO	,
0 SLBIntialMargin	,
0 TotalCFSAmount	,
0 StockCapitalMtf	,
0 MtfInitialMargin	,

0 ScripCapitalCash,
0 StockCapitalCash,
0 StockCapitalCashApproved,
0 MTFMargin,  
0 PhysicalMargin, 
0 CashBrokerage, 
0 FOBrokerage, 
0 CurrBrokerage, 
0 CommBrokerage

from awacs10.dbo.tbl_members with (nolock) 

) B on A.MemberCode = B.MemberCode

GO
