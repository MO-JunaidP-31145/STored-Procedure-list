/****** Object:  View [dbo].[vw_BannerActivity_Log]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[vw_BannerActivity_Log]
As
Select * From Tbl_WebReports_Log (nolock)
Where RptShortName in ('MENUBANNER','TGSBANNER') and entrydate > (getdate() - 6)
and datepart(hh,getdate()) < 7
GO
/****** Object:  View [dbo].[vw_CBOS_TradeFileForBO_FNO]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[vw_CBOS_TradeFileForBO_FNO]
as
select
a.TradeNo
,'11' [TradeStatus]/*dummy*/
,ltrim(rtrim(b.ScripShortName))[Symbol]
,B.InstrumentName [Instrument Type]
,REPLACE(CONVERT(VARCHAR,DATEADD(SS,B.ExpiryDate,'1980-01-01'),106),' ','-') ExpiryDate
,B.StrikePrice
,B.OPTIONTYPE
,ltrim(rtrim(b.ScripShortName)) + UPPER(SUBSTRING(CONVERT(VARCHAR,DATEADD(SS,B.ExpiryDate,'1980-01-01'),106),4,3))  + CASE WHEN OPTIONTYPE = 'XX' THEN 'FUT' ELSE 'OPT' END ScripShortName
,0[InstrumentType]/*dummy*/
,case when a.BookType = 'P' then '3' else '1' end BookType
,'1' MarketType
,'10412'[UserId]
,'1'[BranchId]/*dummy*/
,case when a.BuyOrSell = 'B' then '1' else '2' end [BuySellIndicator]
,a.TradeQty
,cast(a.tradePrice as float)/100 TradePrice
,'1' as ProCli/*dummy*/
--,a.ClientCode ClientAC
, ltrim(rtrim( case when f.UserCat = 2 then f.UserUCC else f.MemberCode end)) as ClientAC
,c.ParticipantCode
,'10'Settle/*dummy*/
,convert(varchar(20),dateadd(ss,a.TradeDateTime,'1980-01-01'),113)EntryDtTime
,convert(varchar(20),dateadd(ss,d.entrydatetime,'1980-01-01'),113)OrderEnteredModDtTime
,a.OrderNo
,'NIL'CPId/*dummy*/
,e.ProductName
,c.Source
,a.TerminalID[TerminalNo]
,c.remarks
from tbl_trade (nolock) A
inner join tbl_Members f on a.ClientCode = f.MemberCode
left outer join tbl_Scrips (nolock) b on a.exchangecode = b.ExchangeCode and a.ScripCode = b.ScripCode
left outer join TBL_ORDER (nolock) C on C.BunchSeqNo=1 AND A.UniqueOrderId = C.UniqueOrderId
left outer join (select uniqueorderid,max(entrydatetime)entrydatetime from tbl_order (nolock) group by uniqueorderid) d on a.UniqueOrderId = d.UniqueOrderId
left outer join tbl_enum_ProductMaster e on c.ProductType = e.ProductCode
where a.exchangecode = 2
GO
/****** Object:  View [dbo].[vw_CBOS_TradeFileForBO_NSE]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO





CREATE view [dbo].[vw_CBOS_TradeFileForBO_NSE]
as
select 
a.TradeNo
,'11' [TradeStatus]/*dummy*/
,ltrim(rtrim(b.ScripShortName))[Symbol]
,B.OptionType [Series]
,ltrim(rtrim(B.ScripFullName))[SecurityName]
,0[InstrumentType]/*dummy*/
,case when a.BookType = 'P' then '3' else '1' end BookType
,'1' MarketType
,'10412'[UserId]
,'1'[BranchId]/*dummy*/
,case when a.BuyOrSell = 'B' then '1' else '2' end [BuySellIndicator]
,a.TradeQty
,cast(a.tradePrice as float)/100 TradePrice
,'1' as ProCli/*dummy*/
,a.ClientCode ClientAC
,c.ParticipantCode
,'Nil' as AuctionPartType
,0 as [AuctionNo]/*dummy*/
,7 as [SettPeriod]/*dummy*/
,convert(varchar(20),dateadd(ss,a.TradeDateTime,'1980-01-01'),113)TradeEntryDtTime
,convert(varchar(20),dateadd(ss,a.TradeDateTime,'1980-01-01'),113)TradeModifiedDtTime
,a.OrderNo
,'NIL'CPId/*dummy*/
,convert(varchar(20),dateadd(ss,d.entrydatetime,'1980-01-01'),113)OrderEnteredModDtTime

,e.ProductName
,c.Source
,a.TerminalID[TerminalNo]
,c.remarks
from tbl_trade (nolock) A
left outer join tbl_Scrips (nolock) b on a.exchangecode = b.ExchangeCode and a.ScripCode = b.ScripCode
left outer join TBL_ORDER (nolock) C on C.BunchSeqNo=1 AND A.UniqueOrderId = C.UniqueOrderId
left outer join (select uniqueorderid,max(entrydatetime)entrydatetime from tbl_order (nolock) group by uniqueorderid) d on a.UniqueOrderId = d.UniqueOrderId
left outer join tbl_enum_ProductMaster e on c.ProductType = e.ProductCode
where a.exchangecode = 0



GO
/****** Object:  View [dbo].[vw_BO_CTCL_TRadeFile_Processing_NSE]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[vw_BO_CTCL_TRadeFile_Processing_NSE]
as
SELECT 
					ltrim(rtrim(tradeno)) FillId,
					ltrim(rtrim('11')) ReportType,
					ltrim(rtrim(b.ScripShortName)) SymbolName,
					ltrim(rtrim(b.OptionType)) Series,
					ltrim(rtrim(b.ScripShortName)) TradingSymbol,
					ltrim(rtrim(d.InstrumentType)) InstrumentType,
					ltrim(rtrim('1')) BookType,
					ltrim(rtrim('1')) MarketType,
					ltrim(rtrim(A.TerminalID)) UserId, 
					2 BranchId,
					ltrim(rtrim(case when a.BuyOrSell='B' then 1 else 2 end)) TransactionType,
					ltrim(rtrim(a.TradeQty)) FillSize,
					ltrim(rtrim(convert(float,a.TradePrice)/100)) FillPrice,
					1 CustomerFirm,
					--rtrim(ltrim(ExchAccountId)) as  AccountId,
					--case when rtrim(ltrim(ExchAccountId))=''Z08687'' then rtrim(ltrim(AccountId)) else  rtrim(ltrim(ExchAccountId)) end as  AccountId,
					a.clientCode Accountid,

					case when c.participantcode = '' then '10412' else c.participantcode end as ExecutingBroker,
					ltrim(rtrim('N')) AuctionPartType,
					ltrim(rtrim('0'))  AuctionNo,
					'7'SettlementPeriod,
					LTRIM(RTRIM(convert(varchar(20), dateadd(ss,a.tradedatetime,'1980-01-01'), 113)))EntryDateTime,
					LTRIM(RTRIM(convert(varchar(20), dateadd(ss,e.lastmodifiedTime,'1980-01-01'), 113)))ModifiedDateTime
					,ltrim(rtrim(a.orderno)) ExchOrdId
					,'NIL'CPId
					,LTRIM(RTRIM(convert(varchar(20), dateadd(ss,e.entrydatetime,'1980-01-01'), 113)))OrderDateTime
					,ltrim(rtrim(c.userReftext)) [Filler1],a.ProductCode,c.Source [OrderSource]
					from awacs10.dbo.tbl_trade a (nolock) inner join awacs10.dbo.tbl_scrips (nolock)b  on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.ScripCode
					inner join awacs10.dbo.tbl_order c (nolock) on a.UniqueOrderId = c.UniqueOrderId and c.BunchSeqNo = 1
					inner join awacs10.dbo.Tbl_NSE_CMScripMaster d (nolock) on a.ScripCode = d.Token
					left outer join
					(
					select OrderNo,max(lastmodifiedtime)lastmodifiedTime,min(entrydatetime)entrydatetime from tbl_order (nolock)
					where ExchangeCode=0 and EntryDateTime <> 0 group by OrderNo
					)e on a.OrderNo = e.OrderNo
					--where a.exchangecode = 0 		and		a.booktype = 'L'
					where a.exchangecode = 0 		
GO
/****** Object:  Table [dbo].[tmp_Spn_A_PR]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[tmp_Spn_A_PR](
	[a_text] [numeric](9, 2) NULL,
	[ra_id] [numeric](9, 0) NULL,
	[SrNo] [int] IDENTITY(1,1) NOT NULL
) ON [PRIMARY]
GO
/****** Object:  View [dbo].[vw_spn_a]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[vw_spn_a]  
AS  
	Select ra_Id,   
	 Sum(Case when Sno=1 then convert(numeric(18,4),a_text) else 0 end) as Line1,  
	 Sum(Case when Sno=2 then convert(numeric(18,4),a_text) else  0 end) as Line2,   
	 Sum(Case when Sno=3 then convert(numeric(18,4),a_text) else  0 end) as Line3,  
	 Sum(Case when Sno=4 then convert(numeric(18,4),a_text) else  0 end) as Line4,  
	 Sum(Case when Sno=5 then convert(numeric(18,4),a_text) else  0 end) as Line5,  
	 Sum(Case when Sno=6 then convert(numeric(18,4),a_text) else  0 end) as Line6,  
	 Sum(Case when Sno=7 then convert(numeric(18,4),a_text) else  0 end) as Line7,  
	 Sum(Case when Sno=8 then convert(numeric(18,4),a_text) else  0 end) as Line8,  
	 Sum(Case when Sno=9 then convert(numeric(18,4),a_text) else  0 end) as Line9,  
	 Sum(Case when Sno=10 then convert(numeric(18,4),a_text) else 0 end) as Line10,  
	 Sum(Case when Sno=11 then convert(numeric(18,4),a_text) else 0 end) as Line11,  
	 Sum(Case when Sno=12 then convert(numeric(18,4),a_text) else 0 end) as Line12,  
	 Sum(Case when Sno=13 then convert(numeric(18,4),a_text) else 0 end) as Line13,  
	 Sum(Case when Sno=14 then convert(numeric(18,4),a_text) else 0 end) as Line14,  
	 Sum(Case when Sno=15 then convert(numeric(18,4),a_text) else 0 end) as Line15,  
	 Sum(Case when Sno=16 then convert(numeric(18,4),a_text) else 0 end) as Line16  
	 from (SELECT ROW_NUMBER() OVER (PARTITION BY ra_id order by srno) as sno, * FROM tmp_Spn_A_PR with(nolock) ) A  
	group by ra_Id  
GO
/****** Object:  View [dbo].[VW_Scrips_Telegram]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[VW_Scrips_Telegram]  
