CREATE TABLE [dbo].[tbl_AutoClient](
	[SrNo] [int] IDENTITY(1,1) NOT NULL,
	[ReadFlag] [char](1) NOT NULL,
	[MemberCode] [char](12) NOT NULL,
	[BaseCapital] [float] NULL,
	[BaseCapitalCommodity] [float] NOT NULL,
	[BasketName] [varchar](30) NULL,
	[ExchangeRule] [int] NULL,
	[AllowedMarket] [int] NULL,
	[SuspensionRule] [int] NOT NULL,
	[IsOrionUser] [char](1) NULL,
	[IsNRI] [char](1) NOT NULL,
	[RecordInsertedDate] [datetime] NOT NULL,
	[Panno] [varchar](20) NULL,
	[UserUCC] [char](12) NULL,
	[UserCat] [int] NULL,
	[Branch] [varchar](15) NULL,
	[Subbroker] [varchar](25) NULL
) ON [PRIMARY]
