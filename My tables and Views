/****** Object:  Table [dbo].[tbl_indices_scrip_mapping]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[tbl_indices_scrip_mapping](
	[ExchangeCode] [smallint] NULL,
