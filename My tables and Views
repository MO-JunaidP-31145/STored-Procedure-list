/****** Object:  View [dbo].[Vw_QRA_OrderDetails]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[Vw_QRA_OrderDetails]

as

select z.srno,a.UniqueOrderId,TradedQty,TotalQty,TotalRemainingQty,a.OrderType,a.OrderStatus,ConfirmedSequenceNo,

b.MessageCode,ErrorCode,b.NumberOfDecimals,Source,b.ExchangeCode,b.ScripCode,b.OrderNo,b.ClientCode,b.BuyOrSell,b.DisclosedQty,

b.TotalQtyRemaining,b.OrderQty,b.QtyTradedToday,b.Price,b.TriggerPrice,b.GoodTillDate,b.EntryDateTime,b.MinFillQty,

b.LastModifiedTime,b.OrderInitiatorId,b.Remarks,b.BookType,b.OrderLife,b.ExchangeDispatcherId,b.ProductType,b.ProductFlag,

b.TerminalID,b.ExpiryDate,b.BunchSeqNo,b.AMOOrder,b.AMOOrderValidated,MPlusDiffPrice,b.Symbol,b.Series,

b.Instrumentname,b.StrikePrice,b.CALevel,ErrorDetail,UserRefText,ParticipantCode

from tbl_AutoPushedOrder z left outer join tbl_Order b

on z.Remarks = b.remarks and z.ClientCode = b.ClientCode and z.ExchangeCode = b.ExchangeCode

and z.ScripCode = b.ScripCode

inner join tbl_ordermaster a

on a.UniqueOrderId=b.UniqueOrderId and b.BunchSeqNo=a.ConfirmedSequenceno

where z.remarks like 'QRA%'

GO
/****** Object:  View [dbo].[vw_OnlyPOA_ShortSell_SQuareOffData_ProductMerging]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
--select * from tbl_enum_ProductMaster

-----------------------------------------------------------------Kashif Khan End-----------------------------------------------------------------
---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------


-------------------------------------------------- Mandar K Start ---------------------------------------------------------------------------------------------------------------------------
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

-- AS-18147 POA/NONPOA Cancel / Sq-off job takes time to complete	

CREATE view [dbo].[vw_OnlyPOA_ShortSell_SQuareOffData_ProductMerging]
as

with TradedBTSTClients (ClientCode,ScripIsInNo,Bot,Sold,NetBTST)
as
(
select a.ClientCode,b.ScripIsInNo
,sum(case when buyorSell = 'B' then tradeqty else 0 end)Bot
,sum(case when buyorSell = 'S' then tradeqty else 0 end)Sold
,sum(case when buyorSell = 'B' then tradeqty else 0 end)-sum(case when buyorSell = 'S' then tradeqty else 0 end)NetBTST
from dbo.tbl_Trade (nolock) a 
inner join dbo.tbl_scrips(nolock) b on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.ScripCode
where a.ProductCode = 256
group by a.ClientCode,b.ScripIsInNo
)

select  
a.MemberCode,a.ScripIsInNo,l.ScripShortName
,e.party_code OutSideDP
,j.party_code SecondOutsideDp
,BotQty , SoldQty,(BotQty - SoldQty)NetQty
--,n.AceQty
--,isnull(c.AvailableQty,0) [OnlyPOADpQty]
,isnull(f.CollateralQty,0) CollateralQty,isnull(f.TPinQty,0)TPinQty,isnull(f.DebitQty,0) DebitQty, isnull(f.OutStandingQty,0) OutStandingQty, isnull(f.nbfcQty,0) nbfcQty,isnull(f.POAQty ,0)POAQty
--,f.CollateralQty + f.DebitQty + f.OutStandingQty + f.POAQty [OtherThanDP]
,isnull(k.[SecMoslHoldQty],0)[SecMoslHoldQty]
,isnull(f.BTSTQty,0)[OnlyPOABtst]
--,(BotQty - SoldQty) + isnull(c.AvailableQty,0) +isnull(f.CollateralQty,0) +isnull(f.DebitQty,0) +isnull(f.OutStandingQty,0) +isnull(f.POAQty,0)
--,cast(isnull(g.Qty,0) as int) [TpinQty]
,i.NetBTST [NetBTSTTraded]
,h.ClientCode [ExceptionClients]
,f.tradedQty
,n.AceQty,f.NonPoaQty

,case 
--when b.IsTradeForTradeSegment = 'Y' then 0
when e.party_code is not null then 0 
when j.party_code is not null then 0 
when h.ClientCode is not null then 0 
--when m.ClientCode is not null then 0 
when o.clientcode is not null then 0 
when p.ScripIsin is not null then 0 
when q.clientcode is not null then 0
--when R.MemberCode is not null then 0
else
case when 
case when 
(
(isnull(f.collateralQty,0)+isnull(f.poaQty,0)+isnull(f.NonPoaQty,0)+isnull(f.DebitQty,0) + isnull(f.BTSTQty,0)+ isnull(f.OutStandingQty,0)+ isnull(f.nbfcQty,0)) + isnull(k.[SecMoslHoldQty],0) - ISNULL(F.TPinQty,0)
--- ( isnull(f.TradedQty,0) + abs(isnull(i.NetBTST,0)) )-(ISNULL(n.AceQty,0))
- ((ISNULL(n.AceQty,0)))
--- isnull(f.collateralQty,0)
) > abs(BotQty - SoldQty)
then 0 else
(
(isnull(f.collateralQty,0)+isnull(f.poaQty,0)+isnull(f.NonPoaQty,0)+isnull(f.DebitQty,0) + isnull(f.BTSTQty,0)+ isnull(f.OutStandingQty,0)+ isnull(f.nbfcQty,0)) + isnull(k.[SecMoslHoldQty],0) - ISNULL(F.TPinQty,0)
--- ( isnull(f.TradedQty,0) + abs(isnull(i.NetBTST,0)) )-(ISNULL(n.AceQty,0))
- ((ISNULL(n.AceQty,0)))
-- - isnull(f.collateralQty,0)
) - abs(BotQty - SoldQty)
end  < (botQty - SoldQty) 
 then 
 (botqty - soldqty)
 else
 
 case when 
(
(isnull(f.collateralQty,0)+isnull(f.poaQty,0)+isnull(f.NonPoaQty,0)+isnull(f.DebitQty,0) + isnull(f.BTSTQty,0) + isnull(f.OutStandingQty,0)+ isnull(f.nbfcQty,0)) + isnull(k.[SecMoslHoldQty],0) - ISNULL(F.TPinQty,0)
--- ( isnull(f.TradedQty,0) + abs(isnull(i.NetBTST,0)) )-(ISNULL(n.AceQty,0))
- ((ISNULL(n.AceQty,0)))
-- - isnull(f.collateralQty,0)
) > abs(BotQty - SoldQty)
then 0 else
(
(isnull(f.collateralQty,0)+isnull(f.poaQty,0)+isnull(f.NonPoaQty,0)+isnull(f.DebitQty,0) + isnull(f.BTSTQty,0)+ isnull(f.OutStandingQty,0)+ isnull(f.nbfcQty,0)) + isnull(k.[SecMoslHoldQty],0) - ISNULL(F.TPinQty,0)
--- ( isnull(f.TradedQty,0) + abs(isnull(i.NetBTST,0)) )-(ISNULL(n.AceQty,0))
- ((ISNULL(n.AceQty,0)))
-- - isnull(f.collateralQty,0)
) - abs(BotQty - SoldQty)
end
end
end
[QtytoSquareOff]


from 
(
-- Mandar K -- 03-07-2023 -- AS-18147 - POA/NONPOA Cancel / Sq-off job takes time to complete -- Start
select distinct   a.MemberCode, a.ScripIsInNo,  (isnull(a.BotQty, 0) + isnull(b.BotQty, 0)) BotQty , (isnull(a.SoldQty, 0) + isnull(b.SoldQty, 0)) SoldQty  from
(select membercode,ScripIsInNo,BotQty,SoldQty
from dbo.tbl_MemberInstrument (nolock) a
inner join dbo.tbl_scrips (nolock) b on a.ExchangeCode = b.CombinedExchCode and a.ScripCode = b.CombinedScripCode
where a.ProductCode in(1) and a.ExchangeCode = 21 and BotQty - SoldQty < 0  --and MemberCode =  'AJM1169' 
) a
left join 
(select membercode,ScripIsInNo,BotQty,SoldQty
from dbo.tbl_MemberInstrument (nolock) a
inner join dbo.tbl_scrips (nolock) b on  a.ExchangeCode = b.CombinedExchCode and a.ScripCode = b.CombinedScripCode
where a.ProductCode in(2) and a.ExchangeCode = 21 and BotQty - SoldQty <> 0  --and MemberCode =  'AJM1169' 
) b on A.MemberCode = B.MemberCode and a.ScripIsInNo = B.ScripIsInNo
-- Mandar K -- 03-07-2023 -- AS-18147 - POA/NONPOA Cancel / Sq-off job takes time to complete -- Start
-- Mandar K -- 03-07-2023 -- AS-18147 - POA/NONPOA Cancel / Sq-off job takes time to complete -- Old Commented 
--select membercode,ScripIsInNo,sum(botqty)BotQty,Sum(SoldQty)SoldQty
--from dbo.tbl_MemberInstrument (nolock) a
--inner join dbo.tbl_scrips (nolock) b on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.scripcode
--where a.ProductCode in(1,2) and a.ExchangeCode < 2
--group by membercode,ScripIsInNo
-- Mandar K -- 03-07-2023 -- AS-18147 - POA/NONPOA Cancel / Sq-off job takes time to complete -- Old Commented 
)a

left outer join dbo.tbl_dpholdings (nolock) f on a.MemberCode = f.membercode and a.ScripIsInNo = f.scripisinno
left outer join 
(select Clientcode, isin,sum(Quantity)AceQty from pmp.dbo.VW_RMS_SCH_COMBINEACEHOLDING 
group by Clientcode, isin)n on a.MemberCode=n.clientcode and a.ScripIsInNo=n.isin

inner join (select clcode from pmp.dbo.tbl_Advstk_client (nolock) group by clcode) d on a.MemberCode = d.clcode
left outer join dbo.tbl_NONPOA_SQoff_OutSideDPClients e on a.MemberCode = e.party_code
--left outer join 
--(
--select clientCode,ISIN,sum(SellQty) Qty
----from [192.168.103..178].edis.[dbo].[VW_eDIS_DPTRANSFERDETAILS_TWS]
--from pmp.[dbo].[VW_eDIS_DPTRANSFERDETAILS_TWS]
--group by clientCode,ISIN
--)g  on a.MemberCode = g.clientCode and a.ScripIsInNo = g.ISIN
left outer join pmp.dbo.TBL_NONPOASqoffClient_Exception h on a.MemberCode = h.Clientcode
--left outer join (select membercode from tbl_members (nolock) where IsNormalShortSellSquareOffException='Y')R on a.MemberCode = R.membercode
left outer join (select clientcode from tbl_NormalShortSellClientScripExcpetion (nolock) where ScripIsin = 'ALL' )o on  a.membercode=o.clientcode 
left outer join (select ScripIsin from tbl_NormalShortSellClientScripExcpetion (nolock) where clientcode = 'ALL')p on a.ScripIsInNo=p.ScripIsin 
left outer join (select clientcode,ScripIsin from tbl_NormalShortSellClientScripExcpetion (nolock) where clientcode != 'ALL' and ScripIsin != 'ALL')q on a.ScripIsInNo=q.ScripIsin  and a.membercode=q.clientcode

left outer join TradedBTSTClients i on a.MemberCode = i.clientCode and a.ScripIsInNo = i.ScripIsInNo
left outer join dbo.tbl_OnlyPOA_SQoff_OutSideDPClients j on a.MemberCode = j.party_code
left outer join tbl_OnlyPOA_SQoff_BothMoslDpDetails k on a.MemberCode = k.ClientCode and a.ScripIsInNo = k.ScripIsinno
--left outer join (select ClientCode from tbl_MultipleExceptionMaster where ExceptionType = 'nonpoa' 
--group by ClientCode) m on a.MemberCode = m.ClientCode
left outer join 
(
select scripisinno,ScripShortName from(
select row_number() over(partition by scripisinno order by exchangecode)id,scripisinno,ScripShortName from 
dbo.tbl_Scrips (nolock) where exchangecode in(0,1)
) g where id = 1

) l on a.ScripIsInNo = l.ScripIsInNo
where --a.ProductCode = 1 and 
(BotQty - SoldQty) < 0 
--and a.ExchangeCode in (0,1)
--order by (BotQty - SoldQty) + isnull(c.AvailableQty,0) 

GO
/****** Object:  View [dbo].[vw_NonPOA_ShortSell_SQuareOffData_ProductMerging]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
Create view [dbo].[vw_NonPOA_ShortSell_SQuareOffData_ProductMerging]
as

with TradedBTSTClients (ClientCode,ScripIsInNo,Bot,Sold,NetBTST)
as
(
select a.ClientCode,b.ScripIsInNo
,sum(case when buyorSell = 'B' then tradeqty else 0 end)Bot
,sum(case when buyorSell = 'S' then tradeqty else 0 end)Sold
,sum(case when buyorSell = 'B' then tradeqty else 0 end)-sum(case when buyorSell = 'S' then tradeqty else 0 end)NetBTST
from dbo.tbl_Trade (nolock) a 
inner join dbo.tbl_scrips(nolock) b on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.ScripCode
where a.ProductCode = 256
group by a.ClientCode,b.ScripIsInNo
)

select  
a.MemberCode,a.ScripIsInNo,l.ScripShortName,
e.party_code OutSideDP,BotQty , SoldQty,(BotQty - SoldQty)NetQty
--,isnull(c.AvailableQty,0) [NonPOADpQty]
,isnull(f.CollateralQty,0) CollateralQty,isnull(f.TPinQty,0)TPinQty,isnull(f.DebitQty,0) DebitQty, isnull(f.OutStandingQty,0) OutStandingQty, isnull(f.nbfcQty,0) nbfcQty,isnull(f.POAQty ,0)POAQty
--,f.CollateralQty + f.DebitQty + f.OutStandingQty + f.POAQty [OtherThanDP]
,isnull(f.BTSTQty,0)[NonPOABtst]
--,(BotQty - SoldQty) + isnull(c.AvailableQty,0) +isnull(f.CollateralQty,0) +isnull(f.DebitQty,0) +isnull(f.OutStandingQty,0) +isnull(f.POAQty,0)
--,cast(isnull(g.Qty,0) as int) [TpinQty]
,i.NetBTST [NetBTSTTraded]
,h.ClientCode [ExceptionClients]
,f.tradedQty
,n.AceQty,f.NonPoaQty
/*
,case 
--when b.IsTradeForTradeSegment = 'Y' then 0
when e.party_code is not null then 0 
when h.ClientCode is not null then 0 
else
case when ((BotQty - SoldQty) + isnull(f.DebitQty,0) + isnull(f.POAQty,0) + isnull(f.BTSTQty,0))  < 0 then 
((BotQty - SoldQty) + isnull(f.DebitQty,0) + isnull(f.POAQty,0) + isnull(f.BTSTQty,0))
else 0 end
end [QtytoSquareOff]
*/
/*
,case 
--when b.IsTradeForTradeSegment = 'Y' then 0
when e.party_code is not null then 0 
when h.ClientCode is not null then 0 
else
case when ((isnull(f.collateralQty,0)+isnull(f.poaQty,0)+isnull(f.DebitQty,0) + isnull(f.BTSTQty,0)) 
- (isnull(f.TradedQty,0)+ abs(isnull(i.NetBTST,0))) - isnull(f.CollateralQty,0)) > abs(BotQty - SoldQty) then 0
else ((isnull(f.collateralQty,0)+isnull(f.poaQty,0)+isnull(f.DebitQty,0) + isnull(f.BTSTQty,0)) 
- (isnull(f.TradedQty,0)+ abs(isnull(i.NetBTST,0))) - isnull(f.CollateralQty,0)) - abs(BotQty - SoldQty)
end end [QtytoSquareOff]
*/
,
case 
--when b.IsTradeForTradeSegment = 'Y' then 0
when e.party_code is not null then 0 
when h.ClientCode is not null then 0 
--when m.ClientCode is not null then 0 
when o.clientcode is not null then 0 
when p.ScripIsin is not null then 0 
when q.clientcode is not null then 0 
else

case when 

case when 
(
(isnull(f.collateralQty,0)-isnull(f.TPinQty,0)+isnull(f.poaQty,0)+isnull(f.NonPoaQty,0)+isnull(f.DebitQty,0)+ isnull(f.OutStandingQty,0)+isnull(f.nbfcqty,0) + isnull(f.BTSTQty,0))
--- ( isnull(f.TradedQty,0) + abs(isnull(i.NetBTST,0)) )-(ISNULL(n.AceQty,0))
- (ISNULL(n.AceQty,0))
- isnull(f.collateralQty,0)
) > abs(BotQty - SoldQty)
then 0 else
(
(isnull(f.collateralQty,0)-isnull(f.TPinQty,0)+isnull(f.poaQty,0)+isnull(f.NonPoaQty,0)+isnull(f.DebitQty,0)+ isnull(f.OutStandingQty,0)+isnull(f.nbfcqty,0) + isnull(f.BTSTQty,0))
--- ( isnull(f.TradedQty,0) + abs(isnull(i.NetBTST,0)) )-(ISNULL(n.AceQty,0))
- (ISNULL(n.AceQty,0))
-- - isnull(f.collateralQty,0)
) - abs(BotQty - SoldQty)
end
 < (botQty - SoldQty) 
 then 
 (botqty - soldqty)
 else
 
case when 
(
(isnull(f.collateralQty,0)-isnull(f.TPinQty,0)+isnull(f.poaQty,0)+isnull(f.NonPoaQty,0)+isnull(f.DebitQty,0)+ isnull(f.OutStandingQty,0)+isnull(f.nbfcqty,0) + isnull(f.BTSTQty,0))
--- ( isnull(f.TradedQty,0) + abs(isnull(i.NetBTST,0)) )-(ISNULL(n.AceQty,0))
- (ISNULL(n.AceQty,0))
-- - isnull(f.collateralQty,0)
) > abs(BotQty - SoldQty)
then 0 else
(
(isnull(f.collateralQty,0)-isnull(f.TPinQty,0)+isnull(f.poaQty,0)+isnull(f.NonPoaQty,0)+isnull(f.DebitQty,0)+ isnull(f.OutStandingQty,0)+isnull(f.nbfcqty,0) + isnull(f.BTSTQty,0))
--- ( isnull(f.TradedQty,0) + abs(isnull(i.NetBTST,0)) )-(ISNULL(n.AceQty,0))
- (ISNULL(n.AceQty,0))
-- - isnull(f.collateralQty,0)
) - abs(BotQty - SoldQty)
end
end


end
[QtytoSquareOff],h.[ClientCode],o.Clientcode[SCMC],p.ScripIsin[SCMS],q.Clientcode[SCMBoth]

from 
(
select membercode,ScripIsInNo,sum(botqty)BotQty,Sum(SoldQty)SoldQty
from dbo.tbl_MemberInstrument (nolock) a
inner join dbo.tbl_scrips (nolock) b on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.scripcode
where a.ProductCode in(1,2) and a.ExchangeCode < 2 
group by membercode,ScripIsInNo 

--select  C.membercode,C.ScripIsInNo,	(C.BotQty) as BotQty,	
-- --(C.SoldQty  - case when C.SoldQty >= isnull((abs(D.BotQty -D.SoldQty)),0) then isnull((abs(D.BotQty -D.SoldQty)),0)  else  C.SoldQty  end) as  SoldQty
-- (C.SoldQty  - case when C.SoldQty >= isnull((D.BotQty -D.SoldQty),0) then isnull((D.BotQty -D.SoldQty),0)  else  C.SoldQty  end) as  SoldQty
--from
--(select membercode,ScripIsInNo,sum(botqty)BotQty,Sum(SoldQty)SoldQty
--from dbo.tbl_MemberInstrument (nolock) a
--inner join dbo.tbl_scrips (nolock) b on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.scripcode
--where   a.ProductCode = 1 and a.ExchangeCode < 2
--group by a.MemberCode,ScripIsInNo) C 
--left join 
--(
--select membercode,ScripIsInNo,botqty  as BotQty, SoldQty  as SoldQty
--from dbo.tbl_MemberInstrument (nolock) a
--inner join dbo.tbl_scrips (nolock) b on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.scripcode
--where a.ProductCode = 2 and a.ExchangeCode < 2
--)  D
--on C.MemberCode = D.MemberCode and C.ScripIsInNo = D.ScripIsInNo



--select membercode,ScripIsInNo,sum(botqty)BotQty,Sum(SoldQty)SoldQty
--from dbo.tbl_MemberInstrument (nolock) a
--inner join dbo.tbl_scrips (nolock) b on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.scripcode
--where a.ProductCode = 1 and a.ExchangeCode < 2
--group by membercode,ScripIsInNo
)a

--inner join dbo.tbl_scrips (nolock) b on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.ScripCode
--left outer join pmp.dbo.tbl_dpholdings_nonpoa (nolock) c on a.MemberCode = c.membercode and a.ScripIsInNo = c.scripisinno
left outer join dbo.tbl_dpholdings (nolock) f on a.MemberCode = f.membercode and a.ScripIsInNo = f.scripisinno
left outer join 
(select Clientcode, isin,sum(Quantity)AceQty from pmp.dbo.VW_RMS_SCH_COMBINEACEHOLDING 
group by Clientcode, isin)n on a.MemberCode=n.clientcode and a.ScripIsInNo=n.isin

inner join pmp.dbo.tbl_Advstk_client_NONPOA (nolock) d on a.MemberCode = d.clcode
left outer join dbo.tbl_nonpoa_SQoff_OutSideDPClients e on a.MemberCode = e.party_code
--left outer join 
--(
--select clientCode,ISIN,sum(SellQty) Qty
--from [trd_secondary].edis.[dbo].[VW_eDIS_DPTRANSFERDETAILS_TWS]
------from PMP.[dbo].[VW_eDIS_DPTRANSFERDETAILS_TWS]
--group by clientCode,ISIN
--)g  on a.MemberCode = g.clientCode and a.ScripIsInNo = g.ISIN
left outer join pmp.dbo.TBL_NONPOASqoffClient_Exception h on a.MemberCode = h.Clientcode
left outer join TradedBTSTClients i on a.MemberCode = i.clientCode and a.ScripIsInNo = i.ScripIsInNo
left outer join (select clientcode from tbl_NormalShortSellClientScripExcpetion (nolock) where ScripIsin = 'ALL' )o on  a.membercode=o.clientcode 
left outer join (select ScripIsin from tbl_NormalShortSellClientScripExcpetion (nolock) where clientcode = 'ALL')p on a.ScripIsInNo=p.ScripIsin 
left outer join (select clientcode,ScripIsin from tbl_NormalShortSellClientScripExcpetion (nolock) where clientcode != 'ALL' and ScripIsin != 'ALL')q on a.ScripIsInNo=q.ScripIsin  and a.membercode=q.clientcode

--left outer join (select ClientCode from tbl_MultipleExceptionMaster where ExceptionType = 'nonpoa' 
--group by ClientCode) m on a.MemberCode = m.ClientCode
left outer join 
(
select scripisinno,ScripShortName from(
select row_number() over(partition by scripisinno order by exchangecode)id,scripisinno,ScripShortName from 
dbo.tbl_Scrips (nolock) where exchangecode in(0,1)
) g where id = 1

) l on a.ScripIsInNo = l.ScripIsInNo
where --a.ProductCode = 1 and 
(BotQty - SoldQty) < 0 
--and a.ExchangeCode in (0,1)
--order by (BotQty - SoldQty) + isnull(c.AvailableQty,0) 
