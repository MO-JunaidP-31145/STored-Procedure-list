SET QUOTED_IDENTIFIER ON
GO
create view [dbo].[vw_RmsSelling_Amt]
as
SELECT MEMBERCODE,RMSSELLINGAMOUNT FROM AWACS10.DBO.TBL_MEMBERS WITH(NOLOCK)
WHERE RMSSELLINGAMOUNT <> 0
