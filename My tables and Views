	[IndexCode] [int] NULL
) ON [PRIMARY]
GO
/****** Object:  View [dbo].[vw_Scrips_Intelym_Broadcast]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE view [dbo].[vw_Scrips_Intelym_Broadcast]
as
/*
--Existing Select Before Reset Packet Implementation
select
[ScripCode]
,[ScripShortName] Symbol 
,[OptionType] Series
,[ExchangeCode]
,[weeks52Highprice] [52wkHigh]
,[weeks52Lowprice] [52wkLow]
--,[UpperExchCircuitPrice] UpperCircuit
--,[LowerExchCircuitPrice] LowerCircuit
,CASE WHEN ExchangeCode in (3,10) THEN ROUND([UpperExchCircuitPrice],4) ELSE ROUND([UpperExchCircuitPrice],2) END as UpperCircuit
,CASE WHEN ExchangeCode in (3,10) THEN ROUND([LowerExchCircuitPrice],4) ELSE ROUND([LowerExchCircuitPrice],2) END as  LowerCircuit
,[ScripfullName]
,InstrumentName
,ExpiryDate
,StrikePrice 
,OptionType
,SPScripCode1,SPScripCode2
from awacs10.dbo.tbl_scrips (nolock)
--from [192.168.106.198].awacs10.dbo.tbl_scrips (nolock)
*/

select
a.[ScripCode]
,a.[ScripShortName] Symbol 
,a.[OptionType] Series
,a.[ExchangeCode]
,a.[weeks52Highprice] [52wkHigh]
,a.[weeks52Lowprice] [52wkLow]
--,[UpperExchCircuitPrice] UpperCircuit
--,[LowerExchCircuitPrice] LowerCircuit
,CASE WHEN a.ExchangeCode in (3,10) THEN ROUND(a.[UpperExchCircuitPrice],4) ELSE ROUND(a.[UpperExchCircuitPrice],2) END as UpperCircuit
,CASE WHEN a.ExchangeCode in (3,10) THEN ROUND(a.[LowerExchCircuitPrice],4) ELSE ROUND(a.[LowerExchCircuitPrice],2) END as  LowerCircuit
,a.[ScripfullName]
,a.InstrumentName
,a.ExpiryDate
,a.StrikePrice 
,a.OptionType
,a.SPScripCode1,a.SPScripCode2
,case when b.ExchangeCode is null and a.exchangecode in(0,1) then 'Y' else case when a.exchangecode in(0,1) then 'N' else '' end end [CashLiquid]
,MarketPrice
from awacs10.dbo.tbl_scrips (nolock) a
--from [192.168.106.198].awacs10.dbo.tbl_scrips (nolock)

left outer join 
(
select a.ExchangeCode,a.ScripCode from tbl_Scrips  a with (nolock)
left outer join
(select * from tbl_indices_scrip_mapping b with (nolock) where  b.IndexCode in  (999904,26003)) b
on A.ExchangeCode = b.ExchangeCode and a.ScripCode = b.ScripCode
where A.ExchangeCode in (0,1)  and B.ScripCode is null
)b
on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.ScripCode

GO
/****** Object:  View [dbo].[vw_ActualOrderPacketSequenceForOrder]    Script Date: 6/3/2025 9:23:40 AM ******/
