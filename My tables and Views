/****** Object:  UserDefinedTableType [dbo].[TableType_AutoSoffOrder]    Script Date: 6/3/2025 9:23:40 AM ******/
CREATE TYPE [dbo].[TableType_AutoSoffOrder] AS TABLE(
	[RowID] [int] IDENTITY(1,1) NOT NULL,
	[ReadFlag] [char](1) NULL,
	[MessageCode] [int] NULL,
	[NumberOfDecimals] [int] NULL,
	[ExchangeCode] [int] NULL,
	[ScripCode] [int] NULL,
	[OrderNo] [varchar](50) NULL,
	[ClientCode] [char](12) NULL,
	[BuyOrSell] [char](1) NULL,
	[DisclosedQty] [int] NULL,
	[TotalQtyRemaining] [int] NULL,
	[OrderQty] [int] NULL,
	[QtyTradedToday] [int] NULL,
	[Price] [int] NULL,
	[TriggerPrice] [int] NULL,
	[GoodTillDate] [int] NULL,
	[EntryDateTime] [int] NULL,
	[MinFillQty] [int] NULL,
	[LastModifiedTime] [int] NULL,
	[OrderInitiatorId] [varchar](50) NULL,
	[Remarks] [varchar](50) NULL,
	[OrderType] [char](1) NULL,
	[BookType] [char](1) NULL,
	[OrderLife] [char](1) NULL,
	[ExchangeDispatcherId] [int] NULL,
	[ProductType] [int] NULL,
	[ProductFlag] [char](1) NULL,
	[TerminalID] [varchar](50) NULL,
	[UniqueOrderId] [varchar](50) NULL,
	[ExpiryDate] [int] NULL,
	[BunchSeqNo] [int] NULL,
	[AMOOrder] [char](1) NULL,
	[AMOOrderValidated] [char](1) NULL,
	[Symbol] [varchar](50) NULL,
	[Series] [varchar](2) NULL,
	[Instrumentname] [varchar](6) NULL,
	[StrikePrice] [int] NULL,
	[CALevel] [int] NULL,
	[RemainingOrderQty] [int] NULL,
	[MaxOrderQty] [int] NULL,
	[OrderAmount] [float] NULL,
	[participantId] [char](12) NULL,
	PRIMARY KEY CLUSTERED 
(
	[RowID] ASC
)WITH (IGNORE_DUP_KEY = OFF)
)
GO
