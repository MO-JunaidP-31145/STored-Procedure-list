/****** Object:  View [dbo].[Vw_ClientDealerMapping]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE View [dbo].[Vw_ClientDealerMapping]  
as  
  
       SELECT ClientCode, DealerCode   FROM (  
       SELECT ClientCode,Dealers as DealerCode   FROM tbl_ClientDealerMapping_New WITH (NOLOCK)   
          UNION  
          SELECT MemberCode as ClientCode, DealerCode FROM tbl_BranchClientMapping A WITH (NOLOCK)  
    INNER Join tbl_BranchDealerMapping B WITH (NOLOCK) ON A.BRanch=b.branch  
     WHERE A.Branch IN (SELECT Branch FROM tbl_BranchDealerMapping WITH (NOLOCK))   AND UserType = ISNULL(0, 0) 
          UNION  
          SELECT MemberCode as ClientCode, DealerCode FROM tbl_BranchClientMapping A WITH (NOLOCK)   
    INNER Join tbl_BranchDealerMapping B WITH (NOLOCK) ON A.SubBroker=b.SubBroker  
    WHERE A.SubBroker IN (SELECT SubBroker FROM tbl_BranchDealerMapping  WITH (NOLOCK)) AND UserType = ISNULL(0, 0) 
    )A  
    --select ClientCode,Dealers as DealerCode from tbl_ClientDealerMapping_Optimized nolock  
    union   
    select A.ClientCode as ClientCode,B.TerminalNo as Dealers   
    from pmp.dbo.Tbl_BOD_ClientActivation A WITH (NOLOCK) , pmp.dbo.TBL_RMS_ClientMapping B WITH (NOLOCK)  
    where a.ExchRights<>0 and B.code='ALL'
	


	
GO
