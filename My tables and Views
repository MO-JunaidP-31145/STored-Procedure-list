/****** Object:  View [dbo].[vw_PhysicalOptions_HedgeClients]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO



CREATE VIEW [dbo].[vw_PhysicalOptions_HedgeClients]
as
select ClientCode,Symbol,
Long,sHORT,
CASE WHEN ABS(LONG) < ABS(SHORT) THEN ABS(LONG)
          WHEN ABS(SHORT) > ABS(LONG) THEN ABS (SHORT) 
 ELSE  0 END HEDGE
,TOTAL
from
(
select ClientCode,Symbol
,SUM(case when ((InstrumentType = 'OPTSTK' AND ITMOTM = 'ITM') OR InstrumentType = 'FUTSTK') and openqty > 0 THEN OPENQTY ELSE 0 END) Long
,SUM(case when ((InstrumentType = 'OPTSTK' AND ITMOTM = 'ITM') OR InstrumentType = 'FUTSTK') and openqty < 0 THEN OPENQTY ELSE 0 END) Short
,COUNT(1) tOTAL
from vw_PhysicalOption_View
group by ClientCode,Symbol
)g
where 
LONG > 0 AND Short < 0 and ABS(LONG) <> ABS(SHORT)




GO
