/****** Object:  UserDefinedFunction [dbo].[checkucc]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE FUNCTION [dbo].[checkucc](@clientCode varchar(100))  
RETURNS varchar(100)   
AS   
BEGIN  
        DECLARE @client_Code varchar(100);
		SET @CLIENT_CODE=@CLIENTCODE  
        IF EXISTS(SELECT TOP 1 * FROM [192.168.99.160].[MOPWM].DBO.VW_UCC_DETAILS WHERE UCC_CODE=@clientCode)
		BEGIN
			SELECT TOP 1 @client_Code=TCODE FROM [192.168.99.160].[MOPWM].DBO.VW_UCC_DETAILS WHERE UCC_CODE=@clientCode
		END
		
		
        RETURN @CLIENT_CODE;  
END; 
GO
/****** Object:  UserDefinedFunction [dbo].[CircuitForPhysicalSqoff]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE Function [dbo].[CircuitForPhysicalSqoff] 
(
@BuyOrSell char(1)
,@MarketPrice float
,@IsCurrencyScrip int,
--@CircuitFilter int
--,@CDCircuitFilter int,
@OptionType  varchar(2),
@TickSize float
)
returns float 
as 
begin
declare @res float
declare @GetFOFutureCktFilter float  
declare @GetFOOptionCktFilter float
declare @CDCircuitFilter float  
declare @tickFactor float
set @tickfactor = 1/@ticksize  

select @GetFOFutureCktFilter=FoCircuitFilterFuture from PMP.dbo.TBL_RMS_PhysicalCircuitFilter
select @GetFOOptionCktFilter=FoCircuitFilterOption from PMP.dbo.TBL_RMS_PhysicalCircuitFilter
--Original Code
set @res= (case when @BuyOrSell = 'B' then 
Round((@MarketPrice * (100 + (case when @IsCurrencyScrip = 1 then @CDCircuitFilter else 
(case when @OptionType='XX' then @GetFOFutureCktFilter else case when @OptionType in('ce','pe') then @GetFOOptionCktFilter else 0 end end) end)))/100,1) 
else
Round((@MarketPrice * (100 - (case when @IsCurrencyScrip = 1 then @CDCircuitFilter else 
(case when @OptionType='XX' then @GetFOFutureCktFilter else case when @OptionType in('ce','pe') then @GetFOOptionCktFilter else 0 end end) 
end)))/100,1) end )

--set @res= (case when @BuyOrSell = 'B' then 
--(@MarketPrice * (100 - (case when @IsCurrencyScrip = 1 then @CDCircuitFilter else 
--(case when @OptionType='XX' then @GetFOFutureCktFilter else case when @OptionType in('ce','pe') then @GetFOOptionCktFilter else 0 end end) end)))/100 
--else
--(@MarketPrice * (100 + (case when @IsCurrencyScrip = 1 then @CDCircuitFilter else 
--(case when @OptionType='XX' then @GetFOFutureCktFilter else case when @OptionType in('ce','pe') then @GetFOOptionCktFilter else 0 end end) 
--end)))/100 end )

if(@buyOrSell='B')
begin
set @res = ceiling(@res * @tickFactor)/@tickFactor
end
else
begin
set @res = ceiling(@res * @tickFactor)/@tickFactor
end


return round(round(@res,2) * power(10,2),0)

end


GO
/****** Object:  UserDefinedFunction [dbo].[fn_API_GetOptionObligationForMarginReport]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

Create FUNCTION [dbo].[fn_API_GetOptionObligationForMarginReport]( @ClientCode CHAR(12) ,@isCommodityOrder bit)
RETURNS Numeric(18,2)  
AS
BEGIN
    DECLARE @OptionObligation FLOAT = 0;

    -- Variables to store fetched values
    DECLARE @OptNetSellPremDer FLOAT, @OptNetSellPremCurr FLOAT, @OptNetSellPremComm FLOAT;
    DECLARE @OptNetSellPremNCDEX FLOAT, @OptNetSellPremNSECO FLOAT;
    DECLARE @InitialMarginPaid FLOAT, @InitialMarginPaidCurr FLOAT, @InitialMarginPaidComm FLOAT;
    DECLARE @InitialMarginPaidNcdex FLOAT, @InitialMarginPaidNseco FLOAT;

    -- Fetch data from tbl_members
  SELECT  
    @OptNetSellPremDer = ISNULL(CONVERT(numeric(18, 2), OptNetSellPremDer), 0.00),
    @OptNetSellPremCurr = ISNULL(CONVERT(numeric(18, 2), OptNetSellPremCurr), 0.00),
    @OptNetSellPremComm = ISNULL(CONVERT(numeric(18, 2), OptNetSellPremComm), 0.00),
    @OptNetSellPremNCDEX = ISNULL(CONVERT(numeric(18, 2), OptNetSellPremNCDEX), 0.00),
    @OptNetSellPremNSECO = ISNULL(CONVERT(numeric(18, 2), OptNetSellPremNSECO), 0.00),
    @InitialMarginPaid = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaid), 0.00),
    @InitialMarginPaidCurr = ISNULL(CONVERT(numeric(18, 2), InitialmarginPaidCurrency), 0.00),
    @InitialMarginPaidComm = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaidComm), 0.00),
    @InitialMarginPaidNcdex = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaidNcdex), 0.00),
    @InitialMarginPaidNseco = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaidNseco), 0.00)
	FROM tbl_members WITH (NOLOCK) WHERE MemberCode = @ClientCode;


    -- Fetch rules from tbl_config
Declare @InitialMarginRuleFO int,	@InitialMarginRuleCurrency	int, @InitialMarginRuleMCX int,
 @InitialMarginRuleNCDEX int,	@InitialMarginRuleNSECO int



		Select @InitialMarginRuleFO=InitialMarginRuleFO,
		@InitialMarginRuleCurrency	=InitialMarginRuleCurrency, @InitialMarginRuleMCX =InitialMarginRuleMCX,
 @InitialMarginRuleNCDEX =InitialMarginRuleNCDEX,	@InitialMarginRuleNSECO=InitialMarginRuleNSECO		
		from tbl_Config with(Nolock)

    -- Remaining margin variables
    DECLARE @remaingFO FLOAT = @InitialMarginPaid;
    DECLARE @remaingCurr FLOAT = @InitialMarginPaidCurr;
    DECLARE @remaingMcx FLOAT = @InitialMarginPaidComm;
    DECLARE @remaingNcdex FLOAT = @InitialMarginPaidNcdex;
    DECLARE @remaingNSECO FLOAT = @InitialMarginPaidNseco;

    -- Helper variables for utilization
    DECLARE @remaingUtilizationFO FLOAT = 0;
    DECLARE @remaingUtilizationCurr FLOAT = 0;
    DECLARE @remaingUtilizationMcx FLOAT = 0;
    DECLARE @remaingUtilizationNcdex FLOAT = 0;
    DECLARE @remaingUtilizationNSECO FLOAT = 0;

    -- Logic for FO
    IF (( @InitialMarginRuleFO | 1 )<>@InitialMarginRuleFO ) AND @OptNetSellPremDer > 0
    BEGIN
        IF @remaingFO < @OptNetSellPremDer
        BEGIN
            SET @remaingUtilizationFO = @OptNetSellPremDer - @remaingFO;

            -- Currency rule check
            IF (@InitialMarginRuleFO | @InitialMarginRuleCurrency = @InitialMarginRuleFO) AND (( @InitialMarginRuleCurrency | 2 )<>@InitialMarginRuleCurrency )
            BEGIN
                IF @remaingCurr < @remaingUtilizationFO
                BEGIN
                    SET @remaingUtilizationFO = @remaingUtilizationFO - @remaingCurr;
                    SET @remaingCurr = 0;
                END
                ELSE
                BEGIN
                    SET @remaingCurr = @remaingCurr - @remaingUtilizationFO;
                    SET @remaingUtilizationFO = 0;
                END
            END

            -- Similar checks for MCX, NCDEX, NSECO
            IF (@InitialMarginRuleFO  | @InitialMarginRuleMCX =@InitialMarginRuleFO) AND (( @InitialMarginRuleMCX | 4 )<>@InitialMarginRuleMCX )
            BEGIN
                IF @remaingMcx < @remaingUtilizationFO
                BEGIN
                    SET @remaingUtilizationFO = @remaingUtilizationFO - @remaingMcx;
                    SET @remaingMcx = 0;
                END
                ELSE
                BEGIN
                    SET @remaingMcx = @remaingMcx - @remaingUtilizationFO;
                    SET @remaingUtilizationFO = 0;
                END
            END

            -- NCDEX
            IF (@InitialMarginRuleFO | @InitialMarginRuleNCDEX =@InitialMarginRuleFO) AND (( @InitialMarginRuleNCDEX | 8 )<>@InitialMarginRuleNCDEX )
            BEGIN
                IF @remaingNcdex < @remaingUtilizationFO
                BEGIN
                    SET @remaingUtilizationFO = @remaingUtilizationFO - @remaingNcdex;
                    SET @remaingNcdex = 0;
                END
                ELSE
                BEGIN
                    SET @remaingNcdex = @remaingNcdex - @remaingUtilizationFO;
                    SET @remaingUtilizationFO = 0;
                END
            END

            -- NSECO
            IF (@InitialMarginRuleFO | @InitialMarginRuleNSECO = @InitialMarginRuleFO) AND ( ( @InitialMarginRuleNSECO | 16 )<>@InitialMarginRuleNSECO )
            BEGIN
                IF @remaingNSECO < @remaingUtilizationFO
                BEGIN
                    SET @remaingUtilizationFO = @remaingUtilizationFO - @remaingNSECO;
                    SET @remaingNSECO = 0;
                END
                ELSE
                BEGIN
                    SET @remaingNSECO = @remaingNSECO - @remaingUtilizationFO;
                    SET @remaingUtilizationFO = 0;
                END
            END
        END
        ELSE
        BEGIN
            SET @remaingFO = @remaingFO - @OptNetSellPremDer;
        END
  END

    -- Similar logic for Curr, MCX, NCDEX, and NSECO
 
 
-- Logic for CURRENCY
 IF ((@InitialMarginRuleCurrency | 2 )<>@InitialMarginRuleCurrency ) AND @OptNetSellPremCurr > 0
 BEGIN
        IF @remaingCurr < @OptNetSellPremCurr
        BEGIN
            SET @remaingUtilizationCurr = @OptNetSellPremCurr - @remaingCurr;

            -- FO rule check
            IF (@InitialMarginRuleCurrency | @InitialMarginRuleFO = @InitialMarginRuleCurrency) AND (( @InitialMarginRuleFO | 1 )<>@InitialMarginRuleFO)
            BEGIN
                IF @remaingFO < @remaingUtilizationCurr
                BEGIN
                    SET @remaingUtilizationCurr = @remaingUtilizationCurr - @remaingFO;
                    SET @remaingFO = 0;
                END
                ELSE
                BEGIN
                    SET @remaingFO = @remaingFO - @remaingUtilizationCurr;
                    SET @remaingUtilizationCurr = 0;
                END
            END

            -- Similar checks for MCX, NCDEX, NSECO
            IF (@InitialMarginRuleCurrency  | @InitialMarginRuleMCX =@InitialMarginRuleCurrency) AND (( @InitialMarginRuleMCX | 4 )<>@InitialMarginRuleMCX )
            BEGIN
                IF @remaingMcx < @remaingUtilizationCurr
                BEGIN
                    SET @remaingUtilizationCurr = @remaingUtilizationCurr - @remaingMcx;
                    SET @remaingMcx = 0;
                END
                ELSE
                BEGIN
                    SET @remaingMcx = @remaingMcx - @remaingUtilizationCurr;
                    SET @remaingUtilizationCurr = 0;
                END
            END

            -- NCDEX
            IF (@InitialMarginRuleCurrency | @InitialMarginRuleNCDEX =@InitialMarginRuleCurrency) AND (( @InitialMarginRuleNCDEX | 8 )<>@InitialMarginRuleNCDEX )
            BEGIN
                IF @remaingNcdex < @remaingUtilizationCurr
                BEGIN
                    SET @remaingUtilizationCurr = @remaingUtilizationCurr - @remaingNcdex;
                    SET @remaingNcdex = 0;
                END
                ELSE
                BEGIN
                    SET @remaingNcdex = @remaingNcdex - @remaingUtilizationCurr;
                    SET @remaingUtilizationCurr = 0;
                END
            END

            -- NSECO
            IF (@InitialMarginRuleCurrency | @InitialMarginRuleNSECO = @InitialMarginRuleFO) AND ( ( @InitialMarginRuleNSECO | 16 )<>@InitialMarginRuleNSECO )
            BEGIN
                IF @remaingNSECO < @remaingUtilizationCurr
                BEGIN
                    SET @remaingUtilizationCurr = @remaingUtilizationCurr - @remaingNSECO;
                    SET @remaingNSECO = 0;
                END
                ELSE
                BEGIN
                    SET @remaingNSECO = @remaingNSECO - @remaingUtilizationCurr;
                    SET @remaingUtilizationCurr = 0;
                END
            END
        END
        ELSE
        BEGIN
            SET @remaingCurr = @remaingCurr - @OptNetSellPremCurr;
        END
    END


	-- Logic for MCX
 IF (( @InitialMarginRuleMCX | 4 )<>@InitialMarginRuleMCX  ) AND @OptNetSellPremComm > 0
 BEGIN
        IF @remaingMcx < @OptNetSellPremComm
        BEGIN
            SET @remaingUtilizationMcx = @OptNetSellPremComm - @remaingMcx;

            -- FO rule check
            IF (@InitialMarginRuleMCX | @InitialMarginRuleFO = @InitialMarginRuleMCX) AND (( @InitialMarginRuleFO | 1 )<>@InitialMarginRuleFO)
            BEGIN
                IF @remaingFO < @remaingUtilizationMcx
                BEGIN
                    SET @remaingUtilizationMcx = @remaingUtilizationMcx - @remaingFO;
                    SET @remaingFO = 0;
                END
                ELSE
                BEGIN
                    SET @remaingFO = @remaingFO - @remaingUtilizationMcx;
                    SET @remaingUtilizationMcx = 0;
                END
            END

            -- Similar checks for CURRENCY, NCDEX, NSECO
            IF (@InitialMarginRuleMCX  | @InitialMarginRuleCurrency =@InitialMarginRuleMCX) AND (( @InitialMarginRuleCurrency | 2 )<>@InitialMarginRuleCurrency )
            BEGIN
                IF @remaingCurr < @remaingUtilizationMcx
                BEGIN
                    SET @remaingUtilizationMcx = @remaingUtilizationMcx - @remaingCurr;
                    SET @remaingCurr = 0;
                END
                ELSE
                BEGIN
                    SET @remaingCurr = @remaingCurr - @remaingUtilizationMcx;
                    SET @remaingUtilizationMcx = 0;
                END
            END

            -- NCDEX
            IF (@InitialMarginRuleMCX | @InitialMarginRuleNCDEX =@InitialMarginRuleCurrency) AND (( @InitialMarginRuleNCDEX | 8 )<>@InitialMarginRuleNCDEX )
            BEGIN
                IF @remaingNcdex < @remaingUtilizationMcx
                BEGIN
                    SET @remaingUtilizationMcx = @remaingUtilizationMcx - @remaingNcdex;
                    SET @remaingNcdex = 0;
                END
                ELSE
                BEGIN
                    SET @remaingNcdex = @remaingNcdex - @remaingUtilizationMcx;
                    SET @remaingUtilizationMcx = 0;
                END
            END

            -- NSECO
            IF (@InitialMarginRuleMCX | @InitialMarginRuleNSECO = @InitialMarginRuleFO) AND ( ( @InitialMarginRuleNSECO | 16 )<>@InitialMarginRuleNSECO )
            BEGIN
                IF @remaingNSECO < @remaingUtilizationMcx
                BEGIN
                    SET @remaingUtilizationMcx = @remaingUtilizationMcx - @remaingNSECO;
                    SET @remaingNSECO = 0;
                END
                ELSE
                BEGIN
                    SET @remaingNSECO = @remaingNSECO - @remaingUtilizationMcx;
                    SET @remaingUtilizationMcx = 0;
                END
            END
        END
        ELSE
        BEGIN
            SET @remaingMcx = @remaingMcx - @OptNetSellPremComm;
        END
    END

		-- Logic for NCDEX
 IF (( @InitialMarginRuleNCDEX | 8 )<>@InitialMarginRuleNCDEX  ) AND @OptNetSellPremNCDEX> 0
 BEGIN
        IF @remaingNcdex < @OptNetSellPremNCDEX
        BEGIN
            SET @remaingUtilizationNcdex = @OptNetSellPremNCDEX - @remaingNcdex;

            -- FO rule check
            IF (@InitialMarginRuleNCDEX | @InitialMarginRuleFO = @InitialMarginRuleNCDEX) AND (( @InitialMarginRuleFO | 1 )<>@InitialMarginRuleFO)
            BEGIN
                IF @remaingFO < @remaingUtilizationNcdex
                BEGIN
                    SET @remaingUtilizationNcdex = @remaingUtilizationNcdex - @remaingFO;
                    SET @remaingFO = 0;
                END
                ELSE
                BEGIN
                    SET @remaingFO = @remaingFO - @remaingUtilizationNcdex;
                    SET @remaingUtilizationNcdex = 0;
                END
            END

            -- Similar checks for CURRENCY, MCX, NSECO
            IF (@InitialMarginRuleNCDEX  | @InitialMarginRuleCurrency =@InitialMarginRuleNCDEX) AND (( @InitialMarginRuleCurrency | 2 )<>@InitialMarginRuleCurrency )
            BEGIN
                IF @remaingCurr< @remaingUtilizationNcdex
                BEGIN
                    SET @remaingUtilizationNcdex = @remaingUtilizationNcdex - @remaingCurr;
                    SET @remaingCurr = 0;
                END
                ELSE
                BEGIN
                    SET @remaingCurr = @remaingCurr - @remaingUtilizationNcdex;
                    SET @remaingUtilizationNcdex = 0;
                END
            END

            -- MCX
            IF (@InitialMarginRuleNCDEX | @InitialMarginRuleMCX =@InitialMarginRuleNCDEX) AND (( @InitialMarginRuleMCX | 4 )<>@InitialMarginRuleMCX )
            BEGIN
                IF @remaingMcx < @remaingUtilizationNcdex
                BEGIN
                    SET @remaingUtilizationNcdex = @remaingUtilizationNcdex - @remaingNcdex;
                    SET @remaingMcx = 0;
                END
                ELSE
                BEGIN
                    SET @remaingMcx = @remaingMcx - @remaingUtilizationNcdex;
                    SET @remaingUtilizationNcdex = 0;
                END
            END

            -- NSECO
            IF (@InitialMarginRuleNCDEX | @InitialMarginRuleNSECO = @InitialMarginRuleFO) AND ( ( @InitialMarginRuleNSECO | 16 )<>@InitialMarginRuleNSECO )
            BEGIN
                IF @remaingNSECO < @remaingUtilizationNcdex
                BEGIN
                    SET @remaingUtilizationNcdex = @remaingUtilizationNcdex - @remaingNSECO;
                    SET @remaingNSECO = 0;
                END
                ELSE
                BEGIN
                    SET @remaingNSECO = @remaingNSECO - @remaingUtilizationNcdex;
                    SET @remaingUtilizationNcdex = 0;
                END
            END
        END
        ELSE
        BEGIN
            SET @remaingNcdex = @remaingNcdex - @OptNetSellPremNCDEX;
        END
    END

-- Logic for NSECO
 IF (( @InitialMarginRuleNSECO | 16 )<>@InitialMarginRuleNSECO ) AND @OptNetSellPremNSECO> 0
 BEGIN
        IF @remaingNSECO < @OptNetSellPremNSECO
        BEGIN
            SET @remaingUtilizationNSECO = @OptNetSellPremNSECO - @remaingNSECO;

            -- FO rule check
            IF (@InitialMarginRuleNSECO | @InitialMarginRuleFO = @InitialMarginRuleNSECO) AND (( @InitialMarginRuleFO | 1 )<>@InitialMarginRuleFO)
            BEGIN
                IF @remaingFO < @remaingUtilizationNSECO
                BEGIN
                    SET @remaingUtilizationNSECO = @remaingUtilizationNSECO - @remaingFO;
                    SET @remaingFO = 0;
                END
                ELSE
                BEGIN
                    SET @remaingFO = @remaingFO - @remaingUtilizationNSECO;
                    SET @remaingUtilizationNSECO = 0;
                END
            END

            -- Similar checks for CURRENCY, MCX, NSECO
            IF (@InitialMarginRuleNSECO  | @InitialMarginRuleCurrency =@InitialMarginRuleNCDEX) AND (( @InitialMarginRuleCurrency | 2 )<>@InitialMarginRuleCurrency )
            BEGIN
                IF @remaingCurr< @remaingUtilizationNSECO
                BEGIN
                    SET @remaingUtilizationNSECO = @remaingUtilizationNSECO - @remaingCurr;
                    SET @remaingCurr = 0;
                END
                ELSE
                BEGIN
                    SET @remaingCurr = @remaingCurr - @remaingUtilizationNSECO;
                    SET @remaingUtilizationNSECO = 0;
                END
            END

            -- MCX
            IF (@InitialMarginRuleNSECO | @InitialMarginRuleMCX =@InitialMarginRuleNCDEX) AND (( @InitialMarginRuleMCX | 4 )<>@InitialMarginRuleMCX )
            BEGIN
                IF @remaingMcx < @remaingUtilizationNSECO
                BEGIN
                    SET @remaingUtilizationNSECO = @remaingUtilizationNSECO - @remaingNcdex;
                    SET @remaingMcx = 0;
                END
                ELSE
                BEGIN
                    SET @remaingMcx = @remaingMcx - @remaingUtilizationNSECO;
                    SET @remaingUtilizationNSECO = 0;
                END
            END

            -- NCDEX
            IF (@InitialMarginRuleNSECO | @InitialMarginRuleNCDEX = @InitialMarginRuleNSECO) AND ( ( @InitialMarginRuleNSECO | 8 )<>@InitialMarginRuleNSECO )
            BEGIN
                IF @remaingNcdex < @remaingUtilizationNSECO
                BEGIN
                    SET @remaingUtilizationNSECO = @remaingUtilizationNSECO - @remaingNcdex;
                    SET @remaingNcdex = 0;
                END
                ELSE
                BEGIN
                    SET @remaingNcdex = @remaingNcdex - @remaingUtilizationNSECO;
                    SET @remaingUtilizationNSECO = 0;
                END
            END
        END
        ELSE
        BEGIN
            SET @remaingNSECO = @remaingNSECO - @OptNetSellPremNSECO;
        END
    END


	-- Declare variables for the calculations
--DECLARE @BaseCapitalUtilization FLOAT;
DECLARE @Total FLOAT;

-- Calculate baseCapitalUtilization (renamed as OptionObligation)
SET @OptionObligation =     @remaingUtilizationFO +     @remaingUtilizationCurr +     @remaingUtilizationMcx + 
								 @remaingUtilizationNcdex +     @remaingUtilizationNSECO;

Declare @TotalReturnValue float

-- Conditional logic based on @isCommodityOrder
IF @isCommodityOrder = 1
BEGIN
    -- Calculate Total when isCommodityOrder is true
    SET @Total = 
        CASE WHEN @OptNetSellPremDer > 0 THEN @OptNetSellPremDer ELSE 0 END +
        CASE WHEN @OptNetSellPremCurr > 0 THEN @OptNetSellPremCurr ELSE 0 END;

    -- Return value
  SET  @TotalReturnValue= CASE WHEN @OptionObligation > 0 THEN @Total - @OptionObligation ELSE @Total END
END
ELSE
BEGIN
    -- Calculate Total when isCommodityOrder is false
    SET @Total = 
        CASE WHEN @OptNetSellPremDer > 0 THEN @OptNetSellPremDer ELSE 0 END +
        CASE WHEN @OptNetSellPremCurr > 0 THEN @OptNetSellPremCurr ELSE 0 END +
        CASE WHEN @OptNetSellPremComm > 0 THEN @OptNetSellPremComm ELSE 0 END +
        CASE WHEN @OptNetSellPremNCDEX > 0 THEN @OptNetSellPremNCDEX ELSE 0 END +
        CASE WHEN @OptNetSellPremNSECO > 0 THEN @OptNetSellPremNSECO ELSE 0 END;

	    -- Return value
	 SET  @TotalReturnValue=  CASE WHEN @OptionObligation > 0 THEN @Total - @OptionObligation ELSE 0   END 

END;


Return @TotalReturnValue;

END
GO
/****** Object:  UserDefinedFunction [dbo].[fn_API_GetOptionObligationForMarginReport_SegmentWise]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE FUNCTION [dbo].[fn_API_GetOptionObligationForMarginReport_SegmentWise] (@Clientcode CHAR(12))
RETURNS NUMERIC(18, 2)
AS
BEGIN
    DECLARE @OptionObligation NUMERIC(18, 2) = 0;

    -- Declare variables for input data
    DECLARE @OptNetSellPremDer FLOAT,
            @OptNetSellPremCurr FLOAT,
            @OptNetSellPremComm FLOAT,
            @OptNetSellPremNSECO FLOAT,
            @OptNetSellPremNCDEX FLOAT,
            @OptNetBuyPremCumulativeDer FLOAT,
            @OptNetBuyPremCumulativeCurr FLOAT,
            @OptNetBuyPremCumulativeComm FLOAT,
            @OptNetBuyPremCumulativeNSECO FLOAT,
            @OptNetBuyPremCumulativeNCDEX FLOAT,
            @InitialMarginPaid FLOAT,
            @InitialMarginPaidCurr FLOAT,
            @InitialMarginPaidComm FLOAT,
            @InitialMarginPaidNCDEX FLOAT,
            @InitialMarginPaidNSECO FLOAT,
            @InitialMarginRuleFO INT,
            @InitialMarginRuleCurrency INT,
            @InitialMarginRuleMCX INT,
            @InitialMarginRuleNCDEX INT,
            @InitialMarginRuleNSECO INT;

    -- Fetch rules and data from configuration and client details
    SELECT @InitialMarginRuleFO = InitialMarginRuleFO,@InitialMarginRuleCurrency = InitialMarginRuleCurrency,
        @InitialMarginRuleMCX = InitialMarginRuleMCX,@InitialMarginRuleNCDEX = InitialMarginRuleNCDEX,
        @InitialMarginRuleNSECO = InitialMarginRuleNSECO    FROM tbl_Config WITH (NOLOCK);

   SELECT 
    @InitialMarginPaid = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaid), 0.0),
    @InitialMarginPaidCurr = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaidCurrency), 0.0),
    @InitialMarginPaidComm = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaidComm), 0.0),
    @InitialMarginPaidNCDEX = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaidNcdex), 0.0),
    @InitialMarginPaidNSECO = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaidNseco), 0.0),
    @OptNetSellPremDer = ISNULL(CONVERT(numeric(18, 2), OptNetSellPremDer), 0.0),
    @OptNetSellPremCurr = ISNULL(CONVERT(numeric(18, 2), OptNetSellPremCurr), 0.0),
    @OptNetSellPremComm = ISNULL(CONVERT(numeric(18, 2), OptNetSellPremComm), 0.0),
    @OptNetSellPremNCDEX = ISNULL(CONVERT(numeric(18, 2), OptNetSellPremNCDEX), 0.0),
    @OptNetSellPremNSECO = ISNULL(CONVERT(numeric(18, 2), OptNetSellPremNSECO), 0.0),
    @OptNetBuyPremCumulativeDer = ISNULL(CONVERT(numeric(18, 2), OptNetBuyPremCumulativeDer), 0.0),
    @OptNetBuyPremCumulativeCurr = ISNULL(CONVERT(numeric(18, 2), OptNetBuyPremCumulativeCurr), 0.0),
    @OptNetBuyPremCumulativeComm = ISNULL(CONVERT(numeric(18, 2), OptNetBuyPremCumulativeComm), 0.0),
    @OptNetBuyPremCumulativeNCDEX = ISNULL(CONVERT(numeric(18, 2), OptNetBuyPremCumulativeNCDEX), 0.0),
    @OptNetBuyPremCumulativeNSECO = ISNULL(CONVERT(numeric(18, 2), OptNetBuyPremCumulativeNSECO), 0.0)
	FROM tbl_Members WITH (NOLOCK)WHERE MemberCode = @Clientcode;


    -- Calculation logic for FO
    IF (@InitialMarginRuleFO | 1) != @InitialMarginRuleFO ---No Rights
    BEGIN
        --IF @OptNetBuyPremCumulativeDer > @OptNetSellPremDer
        --BEGIN
        --    SET @OptionObligation += CASE 
        --                                 WHEN @OptNetSellPremDer <= 0 THEN @OptNetBuyPremCumulativeDer
        --                                 ELSE @OptNetBuyPremCumulativeDer - @OptNetSellPremDer
        --                             END;
        --    IF @OptNetSellPremDer > 0
        --    BEGIN
        --        SET @OptionObligation += CASE 
        --                                     WHEN @InitialMarginPaid - @OptNetSellPremDer < 0 THEN @InitialMarginPaid
        --                                     ELSE @OptNetSellPremDer
        --                                 END;
        --    END
        --END
        --ELSE
		IF @OptNetSellPremDer > 0
        BEGIN
            SET @OptionObligation += CASE 
                                         WHEN @InitialMarginPaid - @OptNetSellPremDer < 0 THEN @InitialMarginPaid
                                         ELSE @OptNetSellPremDer
                                     END;
        END
    END
	--Else--- Rights
	--Begin
	--		IF @OptNetBuyPremCumulativeDer > @OptNetSellPremDer
	--		Begin
	--			  SET @OptionObligation +=CASE WHEN @OptNetSellPremDer <= 0 then @OptNetBuyPremCumulativeDer Else (@OptNetBuyPremCumulativeDer-@OptNetSellPremDer) End

	--		End
	--End



    -- Repeat similar logic for Curr, MCX, NCDEX, and NSECO segments
    -- Curr Segment
    IF (@InitialMarginRuleCurrency | 2) != @InitialMarginRuleCurrency
    BEGIN
        IF @OptNetBuyPremCumulativeCurr > @OptNetSellPremCurr
        BEGIN
            SET @OptionObligation += CASE 
                                         WHEN @OptNetSellPremCurr <= 0 THEN @OptNetBuyPremCumulativeCurr
                                         ELSE @OptNetBuyPremCumulativeCurr - @OptNetSellPremCurr
                                     END;
            IF @OptNetSellPremCurr > 0
            BEGIN
                SET @OptionObligation += CASE 
                                             WHEN @InitialMarginPaidCurr - @OptNetSellPremCurr < 0 THEN @InitialMarginPaidCurr
                                             ELSE @OptNetSellPremCurr
                                         END;
            END
        END
        ELSE IF @OptNetSellPremCurr > 0
        BEGIN
            SET @OptionObligation += CASE 
                                         WHEN @InitialMarginPaidCurr - @OptNetSellPremCurr < 0 THEN @InitialMarginPaidCurr
                                         ELSE @OptNetSellPremCurr
                                     END;
        END
    END
	Else
	Begin
			IF @OptNetBuyPremCumulativeCurr > @OptNetSellPremCurr
			Begin
				  SET @OptionObligation +=CASE WHEN @OptNetSellPremCurr <= 0 then @OptNetBuyPremCumulativeCurr Else (@OptNetBuyPremCumulativeCurr-@OptNetSellPremCurr) End

			End
	
	End
	

    -- MCX Segment
    IF (@InitialMarginRuleMCX | 4)!= @InitialMarginRuleMCX
    BEGIN
        IF @OptNetBuyPremCumulativeComm > @OptNetSellPremComm
        BEGIN
            SET @OptionObligation += CASE 
                                         WHEN @OptNetSellPremComm <= 0 THEN @OptNetBuyPremCumulativeComm
                                         ELSE @OptNetBuyPremCumulativeComm - @OptNetSellPremComm
                                     END;
            IF @OptNetSellPremComm > 0
            BEGIN
                SET @OptionObligation += CASE 
                                             WHEN @InitialMarginPaidComm - @OptNetSellPremComm < 0 THEN @InitialMarginPaidComm
                                             ELSE @OptNetSellPremComm
                                         END;
            END
        END
        ELSE IF @OptNetSellPremComm > 0
        BEGIN
            SET @OptionObligation += CASE 
                                         WHEN @InitialMarginPaidComm - @OptNetSellPremComm < 0 THEN @InitialMarginPaidComm
                                         ELSE @OptNetSellPremComm
                                     END;
        END
    END
	Else
	Begin
			IF @OptNetBuyPremCumulativeComm> @OptNetSellPremComm
			Begin
				  SET @OptionObligation +=CASE WHEN @OptNetSellPremComm <= 0 then @OptNetBuyPremCumulativeComm Else (@OptNetBuyPremCumulativeComm-@OptNetSellPremComm) End

			End
	
	End

    -- logic for NCDEX 
IF (@InitialMarginRuleNCDEX | 8) != @InitialMarginRuleNCDEX
BEGIN
    IF @OptNetBuyPremCumulativeNCDEX > @OptNetSellPremNCDEX
    BEGIN
        SET @OptionObligation += CASE 
                                     WHEN @OptNetSellPremNCDEX <= 0 THEN @OptNetBuyPremCumulativeNCDEX
                                     ELSE @OptNetBuyPremCumulativeNCDEX - @OptNetSellPremNCDEX
                                 END;
        IF @OptNetSellPremNCDEX > 0
        BEGIN
            SET @OptionObligation += CASE 
                                         WHEN @InitialMarginPaidNCDEX - @OptNetSellPremNCDEX < 0 THEN @InitialMarginPaidNCDEX
                                         ELSE @OptNetSellPremNCDEX
                                     END;
        END
    END
    ELSE IF @OptNetSellPremNCDEX > 0
    BEGIN
        SET @OptionObligation += CASE 
                                     WHEN @InitialMarginPaidNCDEX - @OptNetSellPremNCDEX < 0 THEN @InitialMarginPaidNCDEX
                                     ELSE @OptNetSellPremNCDEX
                                 END;
    END
END
Else
	Begin
			IF @OptNetBuyPremCumulativeNCDEX> @OptNetSellPremNCDEX
			Begin
				  SET @OptionObligation +=CASE WHEN @OptNetSellPremNCDEX <= 0 then @OptNetBuyPremCumulativeNCDEX Else (@OptNetBuyPremCumulativeNCDEX-@OptNetSellPremNCDEX) End

	End
	
End

--logic for NSECO
IF (@InitialMarginRuleNSECO | 16) != @InitialMarginRuleNSECO
BEGIN
    IF @OptNetBuyPremCumulativeNSECO > @OptNetSellPremNSECO
    BEGIN
        SET @OptionObligation += CASE 
                                     WHEN @OptNetSellPremNSECO <= 0 THEN @OptNetBuyPremCumulativeNSECO
                                     ELSE @OptNetBuyPremCumulativeNSECO - @OptNetSellPremNSECO
                                 END;
        IF @OptNetSellPremNSECO > 0
        BEGIN
            SET @OptionObligation += CASE 
                                         WHEN @InitialMarginPaidNSECO - @OptNetSellPremNSECO < 0 THEN @InitialMarginPaidNSECO
                                         ELSE @OptNetSellPremNSECO
                                     END;
        END
    END
    ELSE IF @OptNetSellPremNSECO > 0
    BEGIN
        SET @OptionObligation += CASE 
                                     WHEN @InitialMarginPaidNSECO - @OptNetSellPremNSECO < 0 THEN @InitialMarginPaidNSECO
                                     ELSE @OptNetSellPremNSECO
                                 END;
    END
END
Else
	Begin
			IF @OptNetBuyPremCumulativeNSECO> @OptNetSellPremNSECO
			Begin
				  SET @OptionObligation +=CASE WHEN @OptNetSellPremNSECO <= 0 then @OptNetBuyPremCumulativeNSECO Else (@OptNetBuyPremCumulativeNSECO-@OptNetSellPremNSECO) End

	End
	
End

    RETURN @OptionObligation;
END
GO
/****** Object:  UserDefinedFunction [dbo].[fn_API_GettoalCapital_FinalM2MAndBPL]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
Create function [dbo].[fn_API_GettoalCapital_FinalM2MAndBPL](@isFo bit,@CashFinalNettBPL numeric(18,2),@FOFinalNettBPL numeric(18,2),@CashFinalNettM2M numeric(18,2),@FOFinalNettM2M numeric(18,2))
returns numeric(18,2)
as
begin
declare @CashNettBookedProfit char(1),@CashNettBookedLoss char(1), @FONettBookedProfit char(1), @FONettBookedLoss char(1)
,@CashNettM2MProfit char(1),@CashNettM2MLoss char(1),@CashValidateM2MwithNett char(1),@FOValidateM2MwithNett char(1)
,@FONettM2MProfit char(1),@FONettM2MLoss char(1)

select @CashNettBookedProfit =CashNettBookedProfit,@CashNettBookedLoss=CashNettBookedLoss
, @FONettBookedProfit =FONettBookedProfit , @FONettBookedLoss=FONettBookedLoss,@CashNettM2MProfit=CashNettM2MProfit,@CashNettM2MLoss=CashNettM2MLoss
,@CashValidateM2MwithNett =CashValidateM2MwithNett,@FOValidateM2MwithNett=FOValidateM2MwithNett
,@FONettM2MProfit=FONettM2MProfit,@FONettM2MLoss=FONettM2MLoss
from dbo.tbl_config with (nolock)

declare @FinalAmount numeric(18,2)=0,@NettBPL numeric(18,2)=0,@NettM2M numeric(18,2)=0
if ((@CashFinalNettBPL > 0) and (@CashNettBookedProfit = 'Y'))
begin if (@isFO=0) begin set @FinalAmount += @CashFinalNettBPL; end end
 if ((@CashFinalNettBPL < 0) and (@CashNettBookedLoss = 'Y'))
begin set @FinalAmount += @CashFinalNettBPL; end
if ((@FOFinalNettBPL > 0) and (@FONettBookedProfit = 'Y'))
begin if (@isFO=0) begin set @FinalAmount += @FOFinalNettBPL;end end
if ((@FOFinalNettBPL < 0) and (@FONettBookedLoss = 'Y'))
begin set @FinalAmount += @FOFinalNettBPL; end
set @NettBPL = @FinalAmount;

if ((@CashFinalNettM2M > 0) and (@CashNettM2MProfit = 'Y'))
begin  set    @FinalAmount += @CashFinalNettM2M; end
if ((@CashFinalNettM2M < 0) and (@CashNettM2MLoss = 'Y')) 
begin  set   @FinalAmount += @CashFinalNettM2M;end
if ((@FOFinalNettM2M > 0) and (@FONettM2MProfit = 'Y'))
begin  set    @FinalAmount += @FOFinalNettM2M;end
if ((@FOFinalNettM2M < 0) and (@FONettM2MLoss = 'Y'))
begin  set   @FinalAmount += @FOFinalNettM2M;end
set @NettM2M = @FinalAmount - @NettBPL;

if ((@isFO=0) and (@CashValidateM2MwithNett = 'Y'))
begin if (@NettM2M > 0) begin return @NettBPL; end end
if ((@isFO=1) and (@FOValidateM2MwithNett = 'Y'))
begin if (@NettM2M > 0) begin return @NettBPL;end end
return @FinalAmount
end

GO
/****** Object:  UserDefinedFunction [dbo].[fn_API_GettoalCapital_FinalM2MAndBPL_comm]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
Create function [dbo].[fn_API_GettoalCapital_FinalM2MAndBPL_comm](@isFO bit,@FOFinalNettBPL numeric(18,2),@FOFinalNettM2M numeric(18,2))
returns numeric(18,2)
as
begin
declare @FinalAmount numeric(18,2)= 0
declare @FONettBookedProfit char(1),@FONettBookedLoss char(1),@FONettM2MProfit char(1),@FONettM2MLoss char(1),@FOValidateM2MwithNett char(1)
--declare @FONettBookedProfit char(1), @FONettBookedLoss char(1),@FONettM2MProfit char(1),@FONettM2MLoss char(1)

select @FONettBookedProfit =FONettBookedProfit , @FONettBookedLoss=FONettBookedLoss,@FONettM2MProfit=FONettM2MProfit
,@FONettM2MLoss=FONettM2MLoss,@FOValidateM2MwithNett=FOValidateM2MwithNett
from dbo.tbl_config with (nolock)

if ((@FOFinalNettBPL > 0) and (@FONettBookedProfit = 'Y'))
begin
if (@isFO=0)
begin
    set @FinalAmount += @FOFinalNettBPL;
end
end
if ((@FOFinalNettBPL < 0) and (@FONettBookedLoss = 'Y'))
begin
set @FinalAmount += @FOFinalNettBPL;
end
declare @NettBPL numeric(18,2)= @FinalAmount;


if ((@FOFinalNettM2M > 0) and (@FONettM2MProfit = 'Y'))
begin
set @FinalAmount += @FOFinalNettM2M;
end
if ((@FOFinalNettM2M < 0) and (@FONettM2MLoss = 'Y'))
begin
set @FinalAmount += @FOFinalNettM2M;
end
declare @NettM2M numeric(18,2) = @FinalAmount - @NettBPL;

if (@FOValidateM2MwithNett = 'Y')
begin
if (@NettM2M > 0)
begin
    return @NettBPL;
end
end
return @FinalAmount;
end

GO
/****** Object:  UserDefinedFunction [dbo].[fn_API_GettoalCapital_FinalM2MAndBPL_NRI]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create function [dbo].[fn_API_GettoalCapital_FinalM2MAndBPL_NRI](@isFo bit,@CashFinalNettBPL numeric(18,2),@FOFinalNettBPL numeric(18,2),@CashFinalNettM2M numeric(18,2),@FOFinalNettM2M numeric(18,2))
returns numeric(18,2)
as
begin

declare @CashNettBookedProfit char(1),@CashNettBookedLoss char(1), @FONettBookedProfit char(1), @FONettBookedLoss char(1)
,@CashNettM2MProfit char(1),@CashNettM2MLoss char(1),@CashValidateM2MwithNett char(1),@FOValidateM2MwithNett char(1)
,@FONettM2MProfit char(1),@FONettM2MLoss char(1) 

select @CashNettBookedProfit =CashNettBookedProfit,@CashNettBookedLoss=CashNettBookedLoss
, @FONettBookedProfit =FONettBookedProfit , @FONettBookedLoss=FONettBookedLoss,@CashNettM2MProfit=CashNettM2MProfit,@CashNettM2MLoss=CashNettM2MLoss
,@CashValidateM2MwithNett =CashValidateM2MwithNett,@FOValidateM2MwithNett=FOValidateM2MwithNett
,@FONettM2MProfit=FONettM2MProfit,@FONettM2MLoss=FONettM2MLoss
from dbo.tbl_config with (nolock)

declare @FinalAmount numeric(18,2)=0,@NettBPL numeric(18,2)=0,@NettM2M numeric(18,2)=0
Declare @Result numeric(18,2)=0

if @isFo = 0 
begin
if ((@CashFinalNettBPL > 0) and (@CashNettBookedProfit = 'Y'))
begin
set @FinalAmount += @CashFinalNettBPL
End

if ((@CashFinalNettBPL < 0) and (@CashNettBookedLoss = 'Y'))
begin 
set @FinalAmount += @CashFinalNettBPL; 
end

set @NettBPL = @FinalAmount;

if ((@CashFinalNettM2M > 0) and (@CashNettM2MProfit = 'Y'))
begin  
set    @FinalAmount += @CashFinalNettM2M
end

if ((@CashFinalNettM2M < 0) and (@CashNettM2MLoss = 'Y')) 
begin  
set   @FinalAmount += @CashFinalNettM2M
end

set @NettM2M = @FinalAmount - @NettBPL;

if (@CashValidateM2MwithNett = 'Y')
begin 
if (@NettM2M > 0) 
begin 
set @Result= @NettBPL
end 
else
begin 
set @Result= @FinalAmount
end 
end
else
begin
set @Result= @FinalAmount
End
End

else 

begin


if ((@FOFinalNettBPL < 0) and (@FONettBookedLoss = 'Y'))
begin 
set @FinalAmount += @FOFinalNettBPL; 
end

set @NettBPL = @FinalAmount;


if ((@FOFinalNettM2M > 0) and (@FONettM2MProfit = 'Y'))
begin  
set    @FinalAmount += @FOFinalNettM2M
end

if ((@FOFinalNettM2M < 0) and (@FONettM2MLoss = 'Y'))
begin 
set   @FinalAmount += @FOFinalNettM2M;
end

set @NettM2M = @FinalAmount - @NettBPL;


if  (@FOValidateM2MwithNett = 'Y')
begin 
if (@NettM2M > 0) 
begin 
	set @Result= @NettBPL;
end 
else
begin 
	set @Result= @FinalAmount;
end 
end
else
begin 
	set @Result= @FinalAmount;
end 

End

Return  @Result
end
GO
/****** Object:  UserDefinedFunction [dbo].[fn_API_GetTotalCapitalForMArginreport]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


CREATE function [dbo].[fn_API_GetTotalCapitalForMArginreport](@Clientcode char(12),@isFOOrder bit ,@isCommodityOrder bit ,@isCurrencyOrder bit )  
returns numeric(18,2)  
as  
begin  
declare @CashIntraRules int ;  
Declare @FORules int;  
Declare @CurrencyRules int;  
Declare @CommodityRules int;  
declare @BaseCapital numeric(18, 2),@MtfFundedValue numeric(18, 2),@AdditionalCapital numeric(18, 2)  
,@ScripCapital numeric(18,2),@StockCapitalIntraday numeric(18,2),@InitialMarginPaid numeric(18,2),@InitialMarginPaidComm numeric(18,2)  
,@MarginPaid numeric(18,2),@FundTransfered numeric(18,2),@MaxUnrealisedAmount numeric(18,2),@MFEquityAmount numeric(18,2)  
,@WithDrawnAmount numeric(18,2),@MFIPOAmt numeric(18,2),@CreditLimit numeric(18,2),@mtfBPL numeric(18,2)  
,@LiquidAssets numeric(18,2)  
--, numeric(18,2), numeric(18,2), numeric(18,2), numeric(18,2)  
/* 5050 fund allocation Mandar K P12 R1 */  
declare @StockCapitalCash numeric(18,2)  
declare @ScripCapitalCash numeric(18,2)  
declare @StockCapitalApproved   numeric(18,2)   
declare @MFEquityAmountApproved numeric(18,2)  
declare @MFEquityAmountCash numeric(18,2)  
/* 5050 fund allocation Mandar K P12 R1 */  
  

  --BNDTRD-4604--Changes Added By Junaid Pall P34--
Declare @IsNRI char(1)  
Declare @BaseCapitalCommodity float
Declare @FundTransferedCommodity float
Declare @StockCapitalDelivery float
Declare @StockCapitalCommodity float
Declare @StockCapitalNormal float

Declare @InitialMarginPaidCurr float
Declare @InitialMarginPaidNCDEX float
Declare @InitialMarginPaidNSECO Float

Declare @SegmentWiseOption char(1)='N' --TO BE HANDLED IN P37


Declare @NriNonAppStockCapBenifit char(1)
		select @NriNonAppStockCapBenifit=NriNonAppStockCapBenifit 
		--,@SegmentWiseOption= SegmentWiseOption 
		from tbl_Config with(Nolock)
 --BNDTRD-4604--Changes Added By Junaid Pall P34--
  
select  
 @CashIntraRules = ISNULL(CONVERT(numeric(18, 2), CashIntraRules        ), 0.00),  
 @FORules = ISNULL(CONVERT(numeric(18, 2), FORules        ), 0.00),  
 @CurrencyRules = ISNULL(CONVERT(numeric(18, 2), CurrencyRules        ), 0.00),  
 @CommodityRules = ISNULL(CONVERT(numeric(18, 2), CommodityRules        ), 0.00),  
@MtfFundedValue = ISNULL(CONVERT(numeric(18, 2), MtfFundedValue), 0.00),  
@BaseCapital = ISNULL(CONVERT(numeric(18, 2), BaseCapital), 0.00),  
@AdditionalCapital  = ISNULL(CONVERT(numeric(18, 2), AdditionalCapital ), 0.00),  
@ScripCapital = ISNULL(CONVERT(numeric(18, 2), ScripCapital), 0.00),  
@MarginPaid = ISNULL(CONVERT(numeric(18, 2), MarginPaid), 0.00),  
@InitialMarginPaid = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaid), 0.00),  
@InitialMarginPaidComm = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaidComm), 0.00),  
@StockCapitalIntraday = ISNULL(CONVERT(numeric(18, 2), StockCapitalFO), 0.00), -- (Max Stock Price)  
@FundTransfered = ISNULL(CONVERT(numeric(18, 2), FundTransferAmt), 0.00),  
@MaxUnrealisedAmount = ISNULL(CONVERT(numeric(18, 2), MaxUnrealisedAmount), 0.00),  
@MFEquityAmount = ISNULL(CONVERT(numeric(18, 2), MFEquityAmount), 0.00),  
@mtfBPL = ISNULL(CONVERT(numeric(18, 2), MtfBPL), 0.00),  
@LiquidAssets  = ISNULL(CONVERT(numeric(18, 2), LiquidAssets), 0.00),  
@WithDrawnAmount  = ISNULL(CONVERT(numeric(18, 2), WithDrawnAmount), 0.00),  
@MFIPOAmt  = ISNULL(CONVERT(numeric(18, 2), BlockedMFIPOAmt), 0.00),  
@CreditLimit  = ISNULL(CONVERT(numeric(18, 2), CreditLimit), 0.00),  
/* 5050 fund allocation Mandar K P12 R1 */  
@StockCapitalCash = ISNULL(CONVERT(numeric(18, 2), StockCapitalCash), 0.00),  
@ScripCapitalCash = ISNULL(CONVERT(numeric(18, 2), ScripCapitalCash), 0.00),  
@StockCapitalApproved  = ISNULL(CONVERT(numeric(18, 2), StockCapitalApproved), 0.00),  
@MFEquityAmountApproved = ISNULL(CONVERT(numeric(18, 2), MFEquityAmountApproved), 0.00),  
@MFEquityAmountCash = ISNULL(CONVERT(numeric(18, 2), MFEquityAmountCash), 0.00) , 
  
/* 5050 fund allocation Mandar K P12 R1 */  

 --BNDTRD-4604--Changes Added By Junaid Pall P34--
@IsNRI=IsNRI ,
@BaseCapitalCommodity= ISNULL(CONVERT(numeric(18, 2), BaseCapitalCommodity), 0.00)  ,
@FundTransferedCommodity = ISNULL(CONVERT(numeric(18, 2), FundTransferedCommodity), 0.00)  ,
@StockCapitalDelivery = ISNULL(CONVERT(numeric(18, 2), StockCapitalDelivery), 0.00),   
@StockCapitalCommodity = ISNULL(CONVERT(numeric(18, 2), StockCapitalCommodity), 0.00),   
@StockCapitalNormal = ISNULL(CONVERT(numeric(18, 2), StockCapital), 0.00),

@InitialMarginPaidCurr = ISNULL(CONVERT(numeric(18, 2), InitialmarginPaidCurrency), 0.00),
@InitialMarginPaidNCDEX = ISNULL(CONVERT(numeric(18, 2), InitialmarginPaidNcdex), 0.00),
@InitialMarginPaidNSECO = ISNULL(CONVERT(numeric(18, 2), InitialmarginPaidNseco), 0.00)

 --BNDTRD-4604--Changes Added By Junaid Pall P34--

 from dbo.tbl_members with (nolock) where membercode = @Clientcode  
  
 declare @FinalRule int;  
 if (@isCommodityOrder = 1) begin set @FinalRule=@CommodityRules end  
 else if (@isCurrencyOrder = 1) begin set @FinalRule=@CurrencyRules end  
 else if (@isFOOrder = 1) begin set @FinalRule=@FORules end  
 else if (@isFOOrder = 0) begin set @FinalRule = @CashIntraRules end  
  
declare @TotalCapital           numeric(18, 2)  
/* 5050 fund allocation Mandar K P12 R1 */  
declare @TotalCashCapital       numeric(18, 2)    
declare @TotalNONCashCapital    numeric(18,2)   
declare @AdditionalCapital_New  numeric(18,2)    
/* 5050 fund allocation Mandar K P12 R1 */  
  
/* 5050 fund allocation Mandar K P12 R1 */  
  



IF (@IsNRI='Y' and @isFOOrder=1 )
Begin 

	    set  @TotalCashCapital =   
	 (case when (@FinalRule & 1)     = 1   then  @BaseCapitalCommodity  else 0 end   
	 + case when (@FinalRule & 8)   = 8   then  @StockCapitalDelivery  Else 0 end  
	 + case when (@FinalRule & 32)  = 32  then  @InitialMarginPaidComm  else 0 end  
	 + case when (@FinalRule & 32)  = 32  then  @InitialMarginPaid else 0  end   
     + case when (@FinalRule & 32)  = 32  then  @InitialMarginPaidNCDEX else 0  end    
	 + case when (@FinalRule & 32)  = 32  then  @InitialMarginPaidNSECO else 0  end    
     + case when (@FinalRule & 32)  = 32  then  @InitialMarginPaidCurr else 0  end    
     + case when (@FinalRule & 128) = 128 then  @FundTransferedCommodity else 0 end 
	 + case when (@FinalRule & 64)  = 64  then @MarginPaid else 0 end  

	 
	 )  
	  
	 set @TotalNONCashCapital = (  
	 case when (@FinalRule & 8)   = 8   then @StockCapitalCommodity  else 0 end  
	 +case when (@FinalRule & 8)   = 8   then  Case when @NriNonAppStockCapBenifit='Y'THEN @StockCapitalNormal else 0 end  else 0 end)  
End
ELSE IF (@IsNRI='Y' and @isFOOrder=0 )
Begin

	 set  @TotalCashCapital =   
	 (case when (@FinalRule & 1)     = 1   then @BaseCapital + @MtfFundedValue else 0 end  
	 --+ case when (@FinalRule & 4)   = 4   then @ScripCapital +  @ScripCapitalCash else 0 end   
	 + case when (@FinalRule & 8)   = 8   then @StockCapitalCash else 0 end  
	 + case when (@FinalRule & 128) = 128 then @FundTransfered else 0 end  
	 + case when (@FinalRule & 256) = 256 then @MaxUnrealisedAmount else 0 end   
	 + case when (@FinalRule & 512) = 512 then @MFEquityAmountCash else 0 end
	 	 + case when (@FinalRule & 64)  = 64  then @MarginPaid else 0 end  

	 
	 )  
	  
	 set @TotalNONCashCapital = (  
	 case when (@FinalRule & 8)   = 8   then @StockCapitalApproved else 0 end  
	 +case when (@FinalRule & 512) = 512 then @MFEquityAmount + @MFEquityAmountApproved else 0 end
	 +case when (@FinalRule & 8)   = 8   then  Case when @NriNonAppStockCapBenifit='Y'THEN @StockCapitalIntraday else 0 end else 0 end  
	 )

End
Else
Begin
 
		 set  @TotalCashCapital =   
	 (case when (@FinalRule & 1)     = 1   then @BaseCapital + @MtfFundedValue else 0 end  
	 + case when (@FinalRule & 4)   = 4   then @ScripCapital + @ScripCapitalCash else 0 end  
	 + case when (@FinalRule & 8)   = 8   then @StockCapitalCash else 0 end  
	 + case when (@FinalRule & 32)  = 32  then
	 
	 ---Changes For Segmentwise Option Premium--
		 CASE WHEN @SegmentWiseOption='Y' THEN 
		 case when  @isCommodityOrder = 1 THEN @InitialMarginPaidComm+ @InitialMarginPaidNCDEX+@InitialMarginPaidNSECO ELSE  0 END
		 + case when  @isCurrencyOrder = 1 THEN @InitialMarginPaidCurr ELSE  0 END 
		 +case when  @isFOOrder = 1 THEN @InitialMarginPaid ELSE  0 END 
		 ELSE
			 case when  @isCommodityOrder = 0 then @InitialMarginPaid + @InitialMarginPaidCurr 
			 else  @InitialMarginPaidComm+ @InitialMarginPaidNCDEX+@InitialMarginPaidNSECO end 	 
		END 
		else 0 end  	
	 ---Changes For Segmentwise Option Premium--
	 
	 + case when (@FinalRule & 64)  = 64  then @MarginPaid else 0 end  
	 + case when (@FinalRule & 128) = 128 then @FundTransfered else 0 end  
	 + case when (@FinalRule & 256) = 256 then @MaxUnrealisedAmount else 0 end   
	 + case when (@FinalRule & 512) = 512 then @MFEquityAmountCash else 0 end  
	 )  
	  
	 set @TotalNONCashCapital = (  
	 case when (@FinalRule & 8)   = 8   then @StockCapitalApproved else 0 end  
	 +case when (@FinalRule & 512) = 512 then @MFEquityAmount + @MFEquityAmountApproved else 0 end
	 +   case when (@FinalRule & 8)   = 8   THEN CASE WHEN @isCommodityOrder=1 THEN @StockCapitalCommodity 
	 ELSE CASE WHEN @isCurrencyOrder=1 OR @isFOOrder=1 THEN @StockCapitalIntraday ELSE @StockCapitalDelivery END END ELSE 0 END)

End
 


 set @AdditionalCapital_New = (case when (@FinalRule & 2)   = 2   then @AdditionalCapital + @LiquidAssets  else 0 end)  
  
 declare @TotalCapitalMin numeric(18, 2)  
   
  
  
  
 if exists (select  1 from tbl_Members (nolock) where MemberCode = @Clientcode and IsCashClient = 'N')  
 begin  
  set @TotalCapital = @TotalCashCapital + @TotalNONCashCapital+ @AdditionalCapital_New;
 
  
  IF (@IsNRI='Y' and @isFOOrder=1 )
  Begin
	set @TotalCapital = @TotalCapital-@WithDrawnAmount-@MFIPOAmt 
  End
  Else if ((@isFOOrder=1 or @isCommodityOrder=1 or @isCurrencyOrder=1 ) and @IsNRI='N' )   
 begin  
                set @TotalCapital = @TotalCapital-@WithDrawnAmount-@MFIPOAmt  
 end  
 else if  (@FinalRule & 1024) = 1024   and @isFOOrder=0
 begin  
   if  @CreditLimit > 0  
   begin  
		select  @TotalCapitalMin = (select   min(myval) from (values ( @TotalCapital-@WithDrawnAmount-@MFIPOAmt), (@CreditLimit)) as A (myval) )   
                set  @TotalCapital = @TotalCapitalMin   
   end  
end 
ELSE
Begin
	set @TotalCapital = @TotalCapital-@WithDrawnAmount-@MFIPOAmt  
End
 end   
  
 else   
 begin  
 if (@TotalCashCapital > 0)  
 begin  
 if (@TotalCashCapital > @TotalNONCashCapital)  
 begin  
  set @TotalCapital = @TotalCashCapital + @TotalNONCashCapital + @AdditionalCapital_New;  
  end  
  else   
  begin  
  set  @TotalCapital = @TotalCashCapital + @TotalCashCapital + @AdditionalCapital_New;  
  end  
  end  
  else   
  begin  
  set @TotalCapital =  @AdditionalCapital_New;  
  end  
  
 end  
    
  
     
  
return @TotalCapital  
end  
GO
/****** Object:  UserDefinedFunction [dbo].[fn_API_GetTotalCapitalForOption]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE  function [dbo].[fn_API_GetTotalCapitalForOption] (@Clientcode char(12),@isFOOrder bit ,@isCommodityOrder bit ,@isCurrencyOrder bit , @ExchangeCode int)  
returns numeric(18,2)  
as  
begin  
declare @CashIntraRules int ;  
Declare @FORules int;  
Declare @CurrencyRules int;  
Declare @CommodityRules int;  
declare @BaseCapital numeric(18, 2),@MtfFundedValue numeric(18, 2),@AdditionalCapital numeric(18, 2)  
,@ScripCapital numeric(18,2),@StockCapitalIntraday numeric(18,2),@InitialMarginPaid numeric(18,2),@InitialMarginPaidComm numeric(18,2)  
,@MarginPaid numeric(18,2),@FundTransfered numeric(18,2),@MaxUnrealisedAmount numeric(18,2),@MFEquityAmount numeric(18,2)  
,@WithDrawnAmount numeric(18,2),@MFIPOAmt numeric(18,2),@CreditLimit numeric(18,2),@mtfBPL numeric(18,2)  
,@LiquidAssets numeric(18,2)  
--, numeric(18,2), numeric(18,2), numeric(18,2), numeric(18,2)  
/* 5050 fund allocation Mandar K P12 R1 */  
declare @StockCapitalCash numeric(18,2)  
declare @ScripCapitalCash numeric(18,2)  
declare @StockCapitalApproved   numeric(18,2)   
declare @MFEquityAmountApproved numeric(18,2)  
declare @MFEquityAmountCash numeric(18,2)  
/* 5050 fund allocation Mandar K P12 R1 */  
  

  --BNDTRD-4604--Changes Added By Junaid Pall P34--
Declare @IsNRI char(1)  
Declare @BaseCapitalCommodity float
Declare @FundTransferedCommodity float
Declare @StockCapitalDelivery float
Declare @StockCapitalCommodity float
Declare @StockCapitalNormal float
Declare @InitialMarginPaidCurr float
Declare @InitialMarginPaidNCDEX float
Declare @InitialMarginPaidNSECO Float

Declare @InitialMarginRuleFO int,	@InitialMarginRuleCurrency	int, @InitialMarginRuleMCX int,
 @InitialMarginRuleNCDEX int,	@InitialMarginRuleNSECO int


Declare @NriNonAppStockCapBenifit char(1)
		select @NriNonAppStockCapBenifit=NriNonAppStockCapBenifit ,@InitialMarginRuleFO=InitialMarginRuleFO,
		@InitialMarginRuleCurrency	=InitialMarginRuleCurrency, @InitialMarginRuleMCX =InitialMarginRuleMCX,
 @InitialMarginRuleNCDEX =InitialMarginRuleNCDEX,	@InitialMarginRuleNSECO=InitialMarginRuleNSECO		
		from tbl_Config with(Nolock)

 --BNDTRD-4604--Changes Added By Junaid Pall P34--




  
select  
 @CashIntraRules = ISNULL(CONVERT(numeric(18, 2), CashIntraRules        ), 0.00),  
 @FORules = ISNULL(CONVERT(numeric(18, 2), FORules        ), 0.00),  
 @CurrencyRules = ISNULL(CONVERT(numeric(18, 2), CurrencyRules        ), 0.00),  
 @CommodityRules = ISNULL(CONVERT(numeric(18, 2), CommodityRules        ), 0.00),  
@MtfFundedValue = ISNULL(CONVERT(numeric(18, 2), MtfFundedValue), 0.00),  
@BaseCapital = ISNULL(CONVERT(numeric(18, 2), BaseCapital), 0.00),  
@AdditionalCapital  = ISNULL(CONVERT(numeric(18, 2), AdditionalCapital ), 0.00),  
@ScripCapital = ISNULL(CONVERT(numeric(18, 2), ScripCapital), 0.00),  
@MarginPaid = ISNULL(CONVERT(numeric(18, 2), MarginPaid), 0.00),  
@InitialMarginPaid = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaid), 0.00),  
@InitialMarginPaidComm = ISNULL(CONVERT(numeric(18, 2), InitialMarginPaidComm), 0.00),  
@StockCapitalIntraday = ISNULL(CONVERT(numeric(18, 2), StockCapitalFO), 0.00), -- (Max Stock Price)  
@FundTransfered = ISNULL(CONVERT(numeric(18, 2), FundTransferAmt), 0.00),  
@MaxUnrealisedAmount = ISNULL(CONVERT(numeric(18, 2), MaxUnrealisedAmount), 0.00),  
@MFEquityAmount = ISNULL(CONVERT(numeric(18, 2), MFEquityAmount), 0.00),  
@mtfBPL = ISNULL(CONVERT(numeric(18, 2), MtfBPL), 0.00),  
@LiquidAssets  = ISNULL(CONVERT(numeric(18, 2), LiquidAssets), 0.00),  
@WithDrawnAmount  = ISNULL(CONVERT(numeric(18, 2), WithDrawnAmount), 0.00),  
@MFIPOAmt  = ISNULL(CONVERT(numeric(18, 2), BlockedMFIPOAmt), 0.00),  
@CreditLimit  = ISNULL(CONVERT(numeric(18, 2), CreditLimit), 0.00),  
/* 5050 fund allocation Mandar K P12 R1 */  
@StockCapitalCash = ISNULL(CONVERT(numeric(18, 2), StockCapitalCash), 0.00),  
@ScripCapitalCash = ISNULL(CONVERT(numeric(18, 2), ScripCapitalCash), 0.00),  
@StockCapitalApproved  = ISNULL(CONVERT(numeric(18, 2), StockCapitalApproved), 0.00),  
@MFEquityAmountApproved = ISNULL(CONVERT(numeric(18, 2), MFEquityAmountApproved), 0.00),  
@MFEquityAmountCash = ISNULL(CONVERT(numeric(18, 2), MFEquityAmountCash), 0.00) , 
  
/* 5050 fund allocation Mandar K P12 R1 */  

 --BNDTRD-4604--Changes Added By Junaid Pall P34--
@IsNRI=IsNRI ,
@BaseCapitalCommodity= ISNULL(CONVERT(numeric(18, 2), BaseCapitalCommodity), 0.00)  ,
@FundTransferedCommodity = ISNULL(CONVERT(numeric(18, 2), FundTransferedCommodity), 0.00)  ,
@StockCapitalDelivery = ISNULL(CONVERT(numeric(18, 2), StockCapitalDelivery), 0.00),   
@StockCapitalCommodity = ISNULL(CONVERT(numeric(18, 2), StockCapitalCommodity), 0.00),   
@StockCapitalNormal = ISNULL(CONVERT(numeric(18, 2), StockCapital), 0.00),
@InitialMarginPaidCurr = ISNULL(CONVERT(numeric(18, 2), InitialmarginPaidCurrency), 0.00),
@InitialMarginPaidNCDEX = ISNULL(CONVERT(numeric(18, 2), InitialmarginPaidNcdex), 0.00),
@InitialMarginPaidNSECO = ISNULL(CONVERT(numeric(18, 2), InitialmarginPaidNseco), 0.00)
 --BNDTRD-4604--Changes Added By Junaid Pall P34--

 from dbo.tbl_members with (nolock) where membercode = @Clientcode  
  
 declare @FinalRule int;  
 if (@isCommodityOrder = 1) begin set @FinalRule=@CommodityRules end  
 else if (@isCurrencyOrder = 1) begin set @FinalRule=@CurrencyRules end  
 else if (@isFOOrder = 1) begin set @FinalRule=@FORules end  
 else if (@isFOOrder = 0) begin set @FinalRule = @CashIntraRules end  
  
declare @TotalCapital           numeric(18, 2)  
/* 5050 fund allocation Mandar K P12 R1 */  
declare @TotalCashCapital       numeric(18, 2)    
declare @TotalNONCashCapital    numeric(18,2)   
declare @AdditionalCapital_New  numeric(18,2)    
/* 5050 fund allocation Mandar K P12 R1 */  
  
/* 5050 fund allocation Mandar K P12 R1 */  
  


IF (@IsNRI='Y' and @isFOOrder=1 )
Begin 

	    set  @TotalCashCapital =   
	 (case when (@FinalRule & 1)     = 1   then  @BaseCapitalCommodity  else 0 end   
	 + case when (@FinalRule & 8)   = 8   then  @StockCapitalDelivery  Else 0 end  
	 + case when (@FinalRule & 32)  = 32  then  @InitialMarginPaidComm  else 0 end  
	 + case when (@FinalRule & 32)  = 32  then  @InitialMarginPaid else 0  end    
	 + case when (@FinalRule & 128) = 128 then  @FundTransferedCommodity else 0 end   
	  + case when (@FinalRule & 64)  = 64  then @MarginPaid else 0 end 
	 
	 )  
	  
	 set @TotalNONCashCapital = (  
	 case when (@FinalRule & 8)   = 8   then @StockCapitalCommodity  else 0 end  
	 +case when (@FinalRule & 8)   = 8   then  Case when @NriNonAppStockCapBenifit='Y'THEN @StockCapitalNormal else 0 end  else 0 end)  
End
ELSE IF (@IsNRI='Y' and @isFOOrder=0 )
Begin

	 set  @TotalCashCapital =   
	 (case when (@FinalRule & 1)     = 1   then @BaseCapital + @MtfFundedValue else 0 end  
	 --+ case when (@FinalRule & 4)   = 4   then @ScripCapital + @ScripCapitalCash else 0 end  
	 + case when (@FinalRule & 8)   = 8   then @StockCapitalCash else 0 end  
	 + case when (@FinalRule & 128) = 128 then @FundTransfered else 0 end  
	 + case when (@FinalRule & 256) = 256 then @MaxUnrealisedAmount else 0 end   
	 + case when (@FinalRule & 512) = 512 then @MFEquityAmountCash else 0 end  
	  + case when (@FinalRule & 64)  = 64  then @MarginPaid else 0 end 
	 
	 )  
	  
	 set @TotalNONCashCapital = (  
	 case when (@FinalRule & 8)   = 8   then @StockCapitalApproved else 0 end  
	 +case when (@FinalRule & 512) = 512 then @MFEquityAmount + @MFEquityAmountApproved else 0 end
	 +case when (@FinalRule & 8)   = 8   then  Case when @NriNonAppStockCapBenifit='Y'THEN @StockCapitalIntraday else 0 end else 0 end  
	 )

End
Else
Begin
 
		 set  @TotalCashCapital =   
	 (case when (@FinalRule & 1)     = 1   then @BaseCapital + @MtfFundedValue else 0 end  
	 + case when (@FinalRule & 4)   = 4   then @ScripCapital + @ScripCapitalCash else 0 end  
	 + case when (@FinalRule & 8)   = 8   then @StockCapitalCash else 0 end  
	
	--@FinalRule & 32 start---
	+ case when (@FinalRule & 32)  = 32  then 	 
	 --case when  @isCommodityOrder = 0 then @InitialMarginPaidComm else @InitialMarginPaid + @InitialMarginPaidComm end else 0 end  
	 Case When @ExchangeCode=6 Then Case When( @InitialMarginRuleMCX & 1 )=1  then @InitialMarginPaid Else 0 end
								+   Case When( @InitialMarginRuleMCX & 2 )=2  then @InitialMarginPaidCurr Else 0 end
								+   Case When( @InitialMarginRuleMCX & 4 )=4  then @InitialMarginPaidComm Else 0 end
								+   Case When( @InitialMarginRuleMCX & 8 )=8  then @InitialMarginPaidNCDEX Else 0 end
								+   Case When( @InitialMarginRuleMCX & 16 )=16  then @InitialMarginPaidNSECO Else 0 end
	 When @ExchangeCode=5 Then Case When( @InitialMarginRuleNCDEX & 1 )=1  then @InitialMarginPaid Else 0 end
								+   Case When( @InitialMarginRuleNCDEX & 2 )=2  then @InitialMarginPaidCurr Else 0 end
								+   Case When( @InitialMarginRuleNCDEX & 4 )=4  then @InitialMarginPaidComm Else 0 end
								+   Case When( @InitialMarginRuleNCDEX & 8 )=8  then @InitialMarginPaidNCDEX Else 0 end
								+   Case When( @InitialMarginRuleNCDEX & 16 )=16  then @InitialMarginPaidNSECO Else 0 end
    When @ExchangeCode IN ( 7,8) Then Case When( @InitialMarginRuleNSECO & 1 )=1  then @InitialMarginPaid Else 0 end
								+   Case When( @InitialMarginRuleNSECO & 2 )=2  then @InitialMarginPaidCurr Else 0 end
								+   Case When( @InitialMarginRuleNSECO & 4 )=4  then @InitialMarginPaidComm Else 0 end
								+   Case When( @InitialMarginRuleNSECO & 8 )=8  then @InitialMarginPaidNCDEX Else 0 end
								+   Case When( @InitialMarginRuleNSECO & 16 )=16  then @InitialMarginPaidNSECO Else 0 end
   When @ExchangeCode IN (3,10) Then Case When( @InitialMarginRuleCurrency & 1 )=1  then @InitialMarginPaid Else 0 end
								+   Case When( @InitialMarginRuleCurrency & 2 )=2  then @InitialMarginPaidCurr Else 0 end
								+   Case When( @InitialMarginRuleCurrency & 4 )=4  then @InitialMarginPaidComm Else 0 end
								+   Case When( @InitialMarginRuleCurrency & 8 )=8  then @InitialMarginPaidNCDEX Else 0 end
								+   Case When( @InitialMarginRuleCurrency & 16 )=16  then @InitialMarginPaidNSECO Else 0 end
    When @ExchangeCode IN (2,9) Then Case When( @InitialMarginRuleFO & 1 )=1  then @InitialMarginPaid Else 0 end
								+   Case When( @InitialMarginRuleFO & 2 )=2  then @InitialMarginPaidCurr Else 0 end
								+   Case When( @InitialMarginRuleFO & 4 )=4  then @InitialMarginPaidComm Else 0 end
								+   Case When( @InitialMarginRuleFO & 8 )=8  then @InitialMarginPaidNCDEX Else 0 end
								+   Case When( @InitialMarginRuleFO & 16 )=16  then @InitialMarginPaidNSECO Else 0 end
  Else 0 END 
   Else 0 End
    --@FinalRule & 32 End---


	 + case when (@FinalRule & 64)  = 64  then @MarginPaid else 0 end  
	 + case when (@FinalRule & 128) = 128 then @FundTransfered else 0 end  
	 + case when (@FinalRule & 256) = 256 then @MaxUnrealisedAmount else 0 end   
	 + case when (@FinalRule & 512) = 512 then @MFEquityAmountCash else 0 end  
	 )  
	  
	 set @TotalNONCashCapital = (  
	 case when (@FinalRule & 8)   = 8   then @StockCapitalApproved else 0 end  
	 +case when (@FinalRule & 512) = 512 then @MFEquityAmount + @MFEquityAmountApproved else 0 end
	 +   case when (@FinalRule & 8)   = 8   THEN CASE WHEN @isCommodityOrder=1 THEN @StockCapitalCommodity 
	 ELSE CASE WHEN @isCurrencyOrder=1 OR @isFOOrder=1 THEN @StockCapitalIntraday ELSE @StockCapitalDelivery END END ELSE 0 END

	 
	 ) 
End
 


 set @AdditionalCapital_New = (case when (@FinalRule & 2)   = 2   then @AdditionalCapital + @LiquidAssets  else 0 end)  
  
 declare @TotalCapitalMin numeric(18, 2)  
   
  
  
  
 if exists (select  1 from tbl_Members (nolock) where MemberCode = @Clientcode and IsCashClient = 'N')  
 begin  
  set @TotalCapital = @TotalCashCapital + @TotalNONCashCapital+ @AdditionalCapital_New;  
  
  IF (@IsNRI='Y' and @isFOOrder=1 )
  Begin
	set @TotalCapital = @TotalCapital-@WithDrawnAmount-@MFIPOAmt 
  End
  Else if ((@isFOOrder=1 or @isCommodityOrder=1 or @isCurrencyOrder=1 ) and @IsNRI='N' )   
 begin  
                set @TotalCapital = @TotalCapital-@WithDrawnAmount-@MFIPOAmt  
 end  
 else  if  (@FinalRule & 1024) = 1024  and @isFOOrder=0
 begin  
   if  @CreditLimit > 0  
   begin  
	 select  @TotalCapitalMin = (select   min(myval) from (values ( @TotalCapital-@WithDrawnAmount-@MFIPOAmt), (@CreditLimit)) as A (myval) )   
                set  @TotalCapital = @TotalCapitalMin   
   end  
end  
ELSE
Begin
	set @TotalCapital = @TotalCapital-@WithDrawnAmount-@MFIPOAmt  
End
 end   
  
 else   
 begin  
 if (@TotalCashCapital > 0)  
 begin  
 if (@TotalCashCapital > @TotalNONCashCapital)  
 begin  
  set @TotalCapital = @TotalCashCapital + @TotalNONCashCapital + @AdditionalCapital_New;  
  end  
  else   
  begin  
  set  @TotalCapital = @TotalCashCapital + @TotalCashCapital + @AdditionalCapital_New;  
  end  
  end  
  else   
  begin  
  set @TotalCapital =  @AdditionalCapital_New;  
  end  
  
 end  
   
  
  
return @TotalCapital  
end  
  

GO
/****** Object:  UserDefinedFunction [dbo].[fn_CalculateBPLforOptions]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE FUNCTION [dbo].[fn_CalculateBPLforOptions] 
(
	 @BotAmount float,
	 @SoldAmount float,
	 @BotQty bigint,
	 @SoldQty bigint,
	 @Multiplier smallint
)
RETURNS float
AS
BEGIN

	 declare @rCurrentBPL float
	 set @rCurrentBPL = 0;

	 if @BotQty > @SoldQty
	 begin
		set @rCurrentBPL = @SoldAmount - ((@BotAmount / @BotQty) * @SoldQty);
	 end
	 else if @BotQty < @SoldQty
	 begin 
		set @rCurrentBPL = ((@SoldAmount / @SoldQty) * @BotQty ) - @BotAmount;
	 end
	 else
	 begin
		set @rCurrentBPL = @SoldAmount - @BotAmount;
	 end;

	set @rCurrentBPL = @rCurrentBPL * @Multiplier;
	
	return @rCurrentBPL;

END
GO
/****** Object:  UserDefinedFunction [dbo].[fn_MarginCalculateFinalAmount]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


 CREATE function [dbo].[fn_MarginCalculateFinalAmount] (@CashFinalNettBPL Float, @FOFinalNettBPL Float,@CashFinalNettM2M Float,@FOFinalNettM2M Float,@CashNettBookedProfit char,@CashNettBookedLoss char,
 @FONettBookedProfit char,@FONettBookedLoss char,@CashNettM2MProfit char,@CashNettM2MLoss char,@FONettM2MProfit char,@FONettM2MLoss char,@isFo char,@CashValidateM2MwithNett char,@FOValidateM2MwithNett char)
returns Float
as
--select [dbo].[fn_MarginCalculateFinalAmount](286.3,-547.5,-415720.6,1281065.0,'Y','Y','Y','Y','N','Y','N','Y','N','N','N')
begin
DECLARE @Finalamount float
DECLARE @NetBpl float
set  @NetBpl=0
DECLARE @NettM2M float
set  @NettM2M=0
set @Finalamount=0
if((@CashFinalNettBPL>0)and (@CashNettBookedProfit ='Y')) 
begin
  set @Finalamount=@Finalamount+@CashFinalNettBPL
  end
 if((@CashFinalNettBPL < 0) and (@CashNettBookedLoss = 'Y'))
begin
     set @Finalamount=@Finalamount+@CashFinalNettBPL
	 end
 if ((@FOFinalNettBPL > 0) and (@FONettBookedProfit = 'Y'))
begin
 set @Finalamount=@Finalamount+@FOFinalNettBPL
 end
 if((@FOFinalNettBPL < 0) and (@FONettBookedLoss = 'Y'))
 begin
     set @Finalamount= @Finalamount +@FOFinalNettBPL
	 end

     set @NetBpl=@Finalamount




 if ((@CashFinalNettM2M > 0) and (@CashNettM2MProfit = 'Y'))
 set    @FinalAmount = @FinalAmount+@CashFinalNettM2M  
 if ((@CashFinalNettM2M < 0) and (@CashNettM2MLoss = 'Y'))
 set    @FinalAmount = @FinalAmount+@CashFinalNettM2M
 if ((@FOFinalNettM2M > 0) and (@FONettM2MProfit = 'Y'))
  set @FinalAmount = @FinalAmount+@FOFinalNettM2M
if ((@FOFinalNettM2M < 0) and (@FONettM2MLoss = 'Y'))
 begin
set  @FinalAmount= @FinalAmount+ @FOFinalNettM2M
end
  
            set  @NettM2M = @FinalAmount - @NetBPL
		  if ((@isFo='N') and(@CashValidateM2MwithNett = 'Y'))
		  begin
                if (@NettM2M > 0)
                    return @NetBPL


          end              
       
            if ((@isFo='Y') and (@FOValidateM2MwithNett ='Y'))
			begin
                if (@NettM2M > 0)
                    return @NetBPL
					end

           
         
            return @Finalamount

end


GO
/****** Object:  UserDefinedFunction [dbo].[fn_MarginGetFinalM2MAndBPLForValidationComm]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO



CREATE function [dbo].[fn_MarginGetFinalM2MAndBPLForValidationComm] (
@FOFinalNettBPLComm float,
@FOFinalNettM2MComm float,
@@FONettBookedProfit char,
@FONettBookedLoss char,
@FONettM2MProfit char,
@FONettM2MLoss char,
@FOValidateM2MwithNett char
)
Returns float
As 
Begin 
DECLARE @Finalamount float
DECLARE @NetBpl float
set  @NetBpl=0
DECLARE @NettM2M float
set  @NettM2M=0
set @Finalamount=0
 if ((@FOFinalNettBPLComm > 0) and (@@FONettBookedProfit= 'Y'))
         set  @Finalamount=@Finalamount+@FOFinalNettBPLComm
 else if ((@FOFinalNettBPLComm < 0) and (@FONettBookedLoss= 'Y'))
         set  @Finalamount=@Finalamount+@FOFinalNettBPLComm

set @NetBpl=@Finalamount

if ((@FOFinalNettM2MComm > 0) and (@FONettM2MProfit= 'Y'))
         set  @Finalamount=@Finalamount+@FOFinalNettM2MComm
else if ((@FOFinalNettM2MComm < 0) and (@FONettM2MLoss= 'Y'))
         set  @Finalamount=@Finalamount+@FOFinalNettM2MComm

set @NettM2M = @Finalamount - @NetBpl

if (@FOValidateM2MwithNett = 'Y')
                if (@NettM2M > 0)
                    return @NetBpl
                
				
				 return @Finalamount

End

GO
/****** Object:  UserDefinedFunction [dbo].[fn_MarginGetTotalCapital]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO



CREATE Function [dbo].[fn_MarginGetTotalCapital]( 
@isFOOrder char,@Producttype int,@isCommodityOrder char,
@isCurrencyOrder char,@membercode varchar(20)
)
returns float
as
 --SELECT  dbo.fn_MarginGetTotalCapital('N',1,'N','N','dha004') as fn_MarginGetTotalCapital_forCash
  --SELECT  dbo.fn_MarginGetTotalCapital('Y',1,'N','N','dha004') as fn_MarginGetTotalCapital_forFO

begin
DECLARE @Rule_BaseCapitalPosition int
DECLARE @Rule_FundTransferPosition int
DECLARE @Rule_InitialMarginPaidPosition int
DECLARE @Rule_InstrumentCapitalPosition int
DECLARE @Rule_MaxUnrealisedPosition int
DECLARE @Rule_MarginPaidPosition int 
DECLARE @Rule_MFAmountPosition int
DECLARE @Rule_CreditLimitPosition int
DECLARE @Rule_AdditionalCapitalPosition int
DECLARE @Rule_LiquidAssetPosition int 
DECLARE @Rule_StockCapitalPosition int

DECLARE @CommodityRules int
DECLARE @CurrencyRules int
DECLARE @FORules int
DECLARE @CashIntraRules int
DECLARE @BaseCapitalCommodity float
DECLARE @BaseCapital float
DECLARE @FundTransferedCommodity float
DECLARE @FundTransfered float
DECLARE @InitialMarginPaid float
DECLARE @ScripCapital float
DECLARE @MarginPaid float
DECLARE @MaxUnrealisedAmountCommodity float
DECLARE @MaxUnrealisedAmount float
DECLARE @WithDrawnAmountCommodity float
DECLARE @WithDrawnAmount float
DECLARE @AdditionalCapitalCommodity float
DECLARE @AdditionalCapitalCurrency float
DECLARE @AdditionalCapitalFO float
DECLARE @AdditionalCapitalIntraday float
DECLARE @AdditionalCapitalDelivery float
DECLARE @DealerAdditionalCapitalCommodity float
DECLARE @LiquidAsset float
DECLARE @MFEquityAmount float
DECLARE @CreditLimit float
DECLARE @ProductRule int
DECLARE @TotalCapital float
DECLARE @StockCapitalCommodity float
DECLARE @StockCapitalCurrency float
DECLARE @StockCapitalFO float
DECLARE @StockCapitalIntraday float
DECLARE @StockCapitalDelivery float
DECLARE @BlockedMFIPOAmt float

set @Rule_BaseCapitalPosition = 1
set @Rule_FundTransferPosition = 128
set @Rule_InitialMarginPaidPosition = 32
set @Rule_InstrumentCapitalPosition = 4
set @Rule_MaxUnrealisedPosition = 256
set @Rule_MarginPaidPosition = 64
set @Rule_MFAmountPosition = 512
set @Rule_CreditLimitPosition = 1024
set @Rule_AdditionalCapitalPosition=2
set @Rule_LiquidAssetPosition=16
set @Rule_StockCapitalPosition=8

set @ProductRule=0
set @TotalCapital=0


select @CommodityRules=CommodityRules,@CurrencyRules=CurrencyRules,@FORules=FORules,@CashIntraRules=CashIntraRules,
@BaseCapitalCommodity=BaseCapitalCommodity,@BaseCapital=BaseCapital,@FundTransferedCommodity=FundTransferedCommodity,
@FundTransfered=FundTransferAmt,@InitialMarginPaid=InitialMarginPaid,@ScripCapital=ScripCapital,@MarginPaid=MarginPaid,
@MaxUnrealisedAmountCommodity=MaxUnrealisedAmountCommodity,@MaxUnrealisedAmount=MaxUnrealisedAmount,@WithDrawnAmountCommodity=WithDrawnAmountCommodity,
@WithDrawnAmount=WithDrawnAmount,@AdditionalCapitalCommodity=AdditionalCapitalCommodity,@AdditionalCapitalCurrency=AdditionalCapitalCurrency,
@AdditionalCapitalFO=AdditionalCapitalFO,@AdditionalCapitalIntraday=AdditionalCapital,@AdditionalCapitalDelivery=AdditionalCapitalDelivery,
@DealerAdditionalCapitalCommodity=DealerAdditionalCapitalCommodity,@LiquidAsset=LiquidAssets,@MFEquityAmount=MFEquityAmount,@CreditLimit=CreditLimit,
@StockCapitalCommodity=StockCapitalCommodity,@StockCapitalCurrency=StockCapitalCurrency,@StockCapitalFO=StockCapitalFO,@StockCapitalIntraday=StockCapital,
@StockCapitalDelivery=StockCapitalDelivery,@BlockedMFIPOAmt=BlockedMFIPOAmt from tbl_Members
with(nolock)
 where membercode=@membercode

if(@isCommodityOrder='Y')
set @ProductRule= @CommodityRules

else if(@isCurrencyOrder='Y')
set @ProductRule=@CurrencyRules

else if(@isFOOrder='Y')
set @ProductRule=@FORules

else if(@Producttype=1)
set @ProductRule=@CashIntraRules

if ((@ProductRule | @Rule_BaseCapitalPosition) =@ProductRule)
set @TotalCapital=@TotalCapital+(case when @isCommodityOrder='Y' then @BaseCapitalCommodity else @BaseCapital end) 

if ((@ProductRule | @Rule_FundTransferPosition) = @ProductRule)
set @TotalCapital=@TotalCapital+(case when @isCommodityOrder='Y' then @FundTransferedCommodity else @FundTransfered end) 

if ((@ProductRule | @Rule_InitialMarginPaidPosition) = @ProductRule)
set @TotalCapital=@TotalCapital+@InitialMarginPaid

if ((@ProductRule | @Rule_InstrumentCapitalPosition) =@ProductRule)
set @TotalCapital=@TotalCapital+@ScripCapital

if ((@ProductRule | @Rule_MaxUnrealisedPosition) = @ProductRule)
set @TotalCapital=@TotalCapital+(case when @isCommodityOrder='Y' then @MaxUnrealisedAmountCommodity else @MaxUnrealisedAmount end) 

/*
if((@ProductRule | @Rule_AdditionalCapitalPosition) =@ProductRule)
set @TotalCapital = @TotalCapital+(case when @isCommodityOrder='Y' then @AdditionalCapitalCommodity 
                                    when @isCurrencyOrder='Y' then @AdditionalCapitalCurrency  
                                    when @isFOOrder='Y' then @AdditionalCapitalFO
								    when  @Producttype = 1 then @AdditionalCapitalIntraday else @AdditionalCapitalDelivery end)
*/

if ((@ProductRule | @Rule_StockCapitalPosition) = @ProductRule)
set @TotalCapital = @TotalCapital+(case when @isCommodityOrder='Y' then @StockCapitalCommodity 
                                    when @isCurrencyOrder='Y' then 0--@StockCapitalCurrency  
                                    when @isFOOrder='Y' then 0--@StockCapitalFO
								    when  @Producttype = 1 then @StockCapitalIntraday else @StockCapitalDelivery end)
/*
if((@ProductRule | @Rule_LiquidAssetPosition) =@ProductRule)
set @TotalCapital=@TotalCapital+(case when @isCommodityOrder='Y' then @DealerAdditionalCapitalCommodity else @LiquidAsset end) 
*/

if ((@ProductRule | @Rule_MarginPaidPosition) = @ProductRule)
set @TotalCapital = @TotalCapital + @MarginPaid

if ((@ProductRule | @Rule_MFAmountPosition) = @ProductRule)
set @TotalCapital = @TotalCapital + 0--@MFEquityAmount

if (@isCommodityOrder='Y')
 return (@TotalCapital - @WithDrawnAmountCommodity)
            
else if ((@ProductRule | @Rule_CreditLimitPosition) =@ProductRule)
 return (case when @CreditLimit > 0 then IIF((@TotalCapital- @WithDrawnAmount - @BlockedMFIPOAmt) < @CreditLimit,(@TotalCapital - @WithDrawnAmount - @BlockedMFIPOAmt),@CreditLimit  )end)         
 
 return (@TotalCapital - @WithDrawnAmount - @BlockedMFIPOAmt)






end
 

GO
/****** Object:  UserDefinedFunction [dbo].[fn_MarginGetTotalCapitalMTF]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE FUNCTION [dbo].[fn_MarginGetTotalCapitalMTF]
(
@membercode varchar(20)
)
RETURNS float
AS
BEGIN

DECLARE @Rule_BaseCapitalPosition int
DECLARE @Rule_FundTransferPosition int
DECLARE @Rule_InitialMarginPaidPosition int
DECLARE @Rule_InstrumentCapitalPosition int
DECLARE @Rule_MaxUnrealisedPosition int
DECLARE @Rule_MarginPaidPosition int 
DECLARE @Rule_MFAmountPosition int
DECLARE @Rule_CreditLimitPosition int
DECLARE @Rule_AdditionalCapitalPosition int
DECLARE @Rule_LiquidAssetPosition int 
DECLARE @Rule_StockCapitalPosition int
DECLARE @BaseCapital float
DECLARE @FundTransfered float
DECLARE @InitialMarginPaid float
DECLARE @ScripCapital float
DECLARE @MarginPaid float
DECLARE @MaxUnrealisedAmount float
DECLARE @WithDrawnAmount float
DECLARE @CreditLimit float
DECLARE @ProductRule int
DECLARE @TotalCapital float
DECLARE @BlockedMFIPOAmt float
DECLARE @StockCapitalMTF float
DECLARE @mtfFundedValue float
DECLARE @StockCapitalCash float
DECLARE @StockCapitalApproved float
Declare @IsNRI char(1)  

set @Rule_BaseCapitalPosition = 1
set @Rule_FundTransferPosition = 128
set @Rule_InitialMarginPaidPosition = 32
set @Rule_InstrumentCapitalPosition = 4
set @Rule_MaxUnrealisedPosition = 256
set @Rule_MarginPaidPosition = 64
set @Rule_MFAmountPosition = 512
set @Rule_CreditLimitPosition = 1024
set @Rule_AdditionalCapitalPosition=2
set @Rule_LiquidAssetPosition=16
set @Rule_StockCapitalPosition=8

set @TotalCapital=0

select @ProductRule = CashMtfRules from tbl_Members where MemberCode = @membercode;

select 
@BaseCapital=BaseCapital,
@FundTransfered=FundTransferAmt,@InitialMarginPaid=InitialMarginPaid,@ScripCapital=ScripCapital,@MarginPaid=MarginPaid,
@MaxUnrealisedAmount=MaxUnrealisedAmount,
@WithDrawnAmount=WithDrawnAmount,
@CreditLimit=CreditLimit,
@BlockedMFIPOAmt=BlockedMFIPOAmt,@StockCapitalMTF = StockCapitalMtf,@mtfFundedValue=mtffundedValue,
@StockCapitalCash=StockCapitalCash,
@StockCapitalApproved=StockCapitalApproved,
@IsNRI=IsNRI 
 from tbl_Members
with(nolock)
 where membercode=@membercode


if ((@ProductRule | @Rule_BaseCapitalPosition) =@ProductRule)
set @TotalCapital=@TotalCapital + @BaseCapital + @mtfFundedValue

if ((@ProductRule | @Rule_FundTransferPosition) = @ProductRule)
set @TotalCapital=@TotalCapital + @FundTransfered  

if ((@ProductRule | @Rule_InitialMarginPaidPosition) = @ProductRule)
set @TotalCapital=@TotalCapital + case when @IsNRI='Y' then 0 else  @InitialMarginPaid End -- NRI DOnt add

if ((@ProductRule | @Rule_InstrumentCapitalPosition) =@ProductRule)
set @TotalCapital=@TotalCapital + case when @IsNRI='Y' then 0 else  @ScripCapital End --NRI DOnt Add

if ((@ProductRule | @Rule_MaxUnrealisedPosition) = @ProductRule)
set @TotalCapital=@TotalCapital + @MaxUnrealisedAmount 

if ((@ProductRule | @Rule_StockCapitalPosition) = @ProductRule)
set @TotalCapital = @TotalCapital + @StockCapitalMTF

if ((@ProductRule | @Rule_MarginPaidPosition) = @ProductRule)
set @TotalCapital = @TotalCapital + @MarginPaid

if ((@ProductRule | @Rule_MFAmountPosition) = @ProductRule)
set @TotalCapital = @TotalCapital + 0 --@MFEquityAmount
            
else if ((@ProductRule | @Rule_CreditLimitPosition) =@ProductRule)
 return (case when @CreditLimit > 0 then IIF((@TotalCapital- @WithDrawnAmount - @BlockedMFIPOAmt) < @CreditLimit,(@TotalCapital - @WithDrawnAmount - @BlockedMFIPOAmt),@CreditLimit  )end)         
 
 return (@TotalCapital - @WithDrawnAmount - @BlockedMFIPOAmt )

END




GO
/****** Object:  UserDefinedFunction [dbo].[fn_MarginGetTotalUtiliseOthers]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
 CREATE FUNCTION [dbo].[fn_MarginGetTotalUtiliseOthers]
(
@membercode varchar(20)
)
RETURNS float
AS
BEGIN
	
DECLARE @TotalUtilised float;
DECLARE @TotalUtilisedcommodity float;

DECLARE @VarIntraInitialMargin		float;
DECLARE @MPLUSMargin				float;
DECLARE @CashIntraNTimes			float;
DECLARE @VarDeliveryInitialMargin	float;
DECLARE @CashDeliveryNTimes			float;
--DECLARE @MtfInitialMargin			float;
DECLARE @ByPassSpan					char;
DECLARE @InitialMargin				float;
DECLARE @FONTimes					float;
DECLARE @InitialMarginCurrency		float;
DECLARE @CurrencyNTimes				float;
DECLARE @FOExposureMargin			float;
DECLARE @CurrencyExposureMargin		float;
DECLARE @MPLUSFOMargin				float;
DECLARE @ValuePlusEQMargin			float;
DECLARE @ValuePlusNTimesIntra		float;
DECLARE @ValuePlusCurrencyMargin	float;
DECLARE @ValuePlusNTimesCurrency	float;
DECLARE @ValuePlusFnOMargin			float;
DECLARE @ValuePlusNTimesFnO			float;
DECLARE @EPLUSFOMargin				float;
DECLARE @EPLUSCurrMargin			float;
DECLARE @ExpiryPlusNTimesCurrency	float;
DECLARE @ScripCapitalBlockedBSE		float;
DECLARE @ScripCapitalBlockedNSE		float;
DECLARE @SLBIntialMargin			float;
DECLARE @MTFMargin					float;
DECLARE @AdditionalLongMarginBlockedFO	float;
DECLARE @AdditionalShortMarginBlockedFO float;--Abs
DECLARE @PhysicalMargin					float; --abs

--commodity
DECLARE @InitialMarginCommodity				float;
DECLARE @InitialMarginNCDEX					float;
DECLARE @CommodityNTimes					float;
DECLARE @CommodityExposureMargin			float;
DECLARE @NCDEXExposureMargin				float;
DECLARE @ValuePlusFOMargin					float;
DECLARE @ValuePlusNTimes					float;
DECLARE @AdditionalLongMarginBlocked		float;
DECLARE @AdditionalShortMarginBlocked		float; --abs
DECLARE @SpecialLongMarginBlocked			float;
DECLARE @SpecialShortMarginBlocked			float; --abs
DECLARE @AdditionalLongMarginBlockedNcdex	float;
DECLARE @AdditionalShortMarginBlockedNcdex	float;
DECLARE @SpecialLongMarginBlockedNcdex		float;
DECLARE @SpecialShortMarginBlockedNcdex		float; --abs

---------Changes Added By Junaid Pall------------
Declare  @FOPremiumBenefit varchar(20);
Select  @FOPremiumBenefit= FOPremiumBenefit from tbl_Config(nolock)
---------Changes Added By Junaid Pall------------

select @VarIntraInitialMargin = VarIntraIntialMargin, @MPLUSMargin = MPLUSMargin, @CashIntraNTimes = CashIntraNTimes,			
@VarDeliveryInitialMargin = VarDeliveryInitialMargin, @CashDeliveryNTimes = CashDeliveryNTimes,			
@ByPassSpan = ByPassSpan,

---------Changes Added By Junaid Pall------------
@InitialMargin =  case when InitialMargin < 0 then 0 else ( case when isnull(OptNetSellPremDer, 0) > 0 and @FOPremiumBenefit='Y' then InitialMargin + isnull(OptNetSellPremDer, 0) 
else InitialMargin end)
 end,
---------Changes Added By Junaid Pall------------
				
@FONTimes = FONTimes, @InitialMarginCurrency = InitialMarginCurrency, @CurrencyNTimes = CurrencyNTimes,				
@FOExposureMargin = ExposureMargin, @CurrencyExposureMargin	= CurrencyExposureMargin, @MPLUSFOMargin = MPLUSFOMargin,				
@ValuePlusEQMargin = ValuePlusEquityMargin, @ValuePlusNTimesIntra = ValuePlusNTimesIntra,		
@ValuePlusCurrencyMargin = ValuePlusCurrencyMargin, @ValuePlusNTimesCurrency = ValuePlusNTimesCurrency,	
@ValuePlusFnOMargin	= ValuePlusFnOMargin, @ValuePlusNTimesFnO	= ValuePlusNTimesFnO,	
@EPLUSFOMargin = EPLUSFOMargin , @EPLUSCurrMargin = EPLUSCurrMargin , @ExpiryPlusNTimesCurrency = ExpiryPlusNTimesCurrency,	
@ScripCapitalBlockedBSE	= ScripCapitalBlockedBSE,@ScripCapitalBlockedNSE	= ScripCapitalBlockedNSE,	
@SLBIntialMargin = SLBIntialMargin,@MTFMargin = MTFMargin,@AdditionalLongMarginBlockedFO = AdditionalLongMarginBlockedFO,
@AdditionalShortMarginBlockedFO = ABS(AdditionalShortMarginBlockedFO),@PhysicalMargin = ABS(PhysicalMargin),

--commodity method columns
@InitialMarginCommodity	= InitialMarginCommodity,@InitialMarginNCDEX = InitialMarginNCDEX,					
@CommodityNTimes = CommodityNTimes,@CommodityExposureMargin = CommodityExposureMargin,
@NCDEXExposureMargin = NCDEXExposureMargin, @ValuePlusFOMargin = ValuePlusFOMargin,
@ValuePlusNTimes = ValuePlusNTimes,@AdditionalLongMarginBlocked = AdditionalLongMarginBlocked,
@AdditionalShortMarginBlocked = ABS(AdditionalShortMarginBlocked),@SpecialLongMarginBlocked = SpecialLongMarginBlocked,
@SpecialShortMarginBlocked = ABS(SpecialShortMarginBlocked),@AdditionalLongMarginBlockedNcdex = AdditionalLongMarginBlockedNcdex,
@AdditionalShortMarginBlockedNcdex = AdditionalShortMarginBlockedNcdex,@SpecialLongMarginBlockedNcdex = SpecialLongMarginBlockedNcdex,
@SpecialShortMarginBlockedNcdex = ABS(SpecialShortMarginBlockedNcdex)

from tbl_Members where MemberCode = @membercode;

/*TotalUtilisedMargin*/
set @TotalUtilised = (@VarIntraInitialMargin + @MPLUSMargin) / @CashIntraNTimes +
                @VarDeliveryInitialMargin / @CashDeliveryNTimes;
                --/*MtfChanges*/
                --+ @MtfInitialMargin / @CashIntraNTimes;

if ((@ByPassSpan = 'N') OR (@ByPassSpan = 'B')
                OR (@ByPassSpan = 'P') OR (@ByPassSpan = 'O'))
               set @TotalUtilised = @TotalUtilised + (@InitialMargin / @FONTimes) + (@InitialMarginCurrency / @CurrencyNTimes);
            if ((@ByPassSpan = 'E') OR (@ByPassSpan = 'B') OR (@ByPassSpan = 'O'))
              set @TotalUtilised = @TotalUtilised + (@FOExposureMargin / @FONTimes) + (@CurrencyExposureMargin / @CurrencyNTimes);
           set @TotalUtilised = @TotalUtilised + @MPLUSFOMargin / @FONTimes + @ValuePlusEQMargin / @ValuePlusNTimesIntra +
                @ValuePlusCurrencyMargin / @ValuePlusNTimesCurrency + @ValuePlusFnOMargin / @ValuePlusNTimesFnO
                + @EPLUSFOMargin
                + @EPLUSCurrMargin / @ExpiryPlusNTimesCurrency
                + (@ScripCapitalBlockedBSE + @ScripCapitalBlockedNSE) / @CashIntraNTimes;
                
/*Till Here*/

/*TotalUtilisedMarginCommodity*/
set @TotalUtilisedcommodity = 0;

if ((@ByPassSpan = 'N') OR (@ByPassSpan = 'B') OR (@ByPassSpan = 'P') OR (@ByPassSpan = 'O'))
               set @TotalUtilisedcommodity = @TotalUtilisedcommodity + (@InitialMarginCommodity + @InitialMarginNCDEX) / @CommodityNTimes;
            if ((@ByPassSpan = 'E') OR (@ByPassSpan = 'B') OR (@ByPassSpan = 'O'))
               set @TotalUtilisedcommodity = @TotalUtilisedcommodity + (@CommodityExposureMargin + @NCDEXExposureMargin) / @CommodityNTimes;
           set @TotalUtilisedcommodity = @TotalUtilisedcommodity + @ValuePlusFOMargin / @ValuePlusNTimes +
                ((@AdditionalLongMarginBlocked + @AdditionalShortMarginBlocked + @SpecialLongMarginBlocked + @SpecialShortMarginBlocked) +
                (@AdditionalLongMarginBlockedNcdex + @AdditionalShortMarginBlockedNcdex + @SpecialLongMarginBlockedNcdex + @SpecialShortMarginBlockedNcdex))
                / @CommodityNTimes;
/*Till Here*/

return @TotalUtilised + @TotalUtilisedcommodity
END
GO
/****** Object:  UserDefinedFunction [dbo].[fn_MTFGetFinalMTMBPLforValidate]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
 CREATE FUNCTION [dbo].[fn_MTFGetFinalMTMBPLforValidate]
(
@Clientcode varchar(20)
)
RETURNS float
AS
BEGIN
	
DECLARE @FinalAmount float; --locl
DECLARE @FinalAmountCommodity float; --locl

DECLARE @CashFinalNettBPL float; --locl
DECLARE @IntraBPLProfit float;
DECLARE @IntraBPLLoss float;
DECLARE @DeliveryBPLProfit float;
DECLARE @DeliveryBPLLoss float;
DECLARE @MPlusBPLProfit float;
DECLARE @MplusBPLLoss float;
DECLARE @ValuePlusEQBPLProfit float;
DECLARE @ValueplusEQBPLLoss float;
DECLARE @FOFinalNettBPL float; --locl
DECLARE @FOBPLProfit float;
DECLARE @FOBPLLoss float;
DECLARE @MPlusFOBPLProfit float;
DECLARE @MplusFOBPLLoss float;
DECLARE @CurrencyBPLProfit float;
DECLARE @CurrencyBPLLoss float;
DECLARE @ValuePlusCurrencyBPLProfit float;
DECLARE @ValuePlusCurrencyBPLLoss float;
DECLARE @ValuePlusFnOBPLProfit float;
DECLARE @ValueplusFnOBPLLoss float;
DECLARE @EPlusFOBPLProfit float;
DECLARE @EPlusFOBPLLoss float;
DECLARE @EPLUSCurrBPLProfit float;
DECLARE @EPLUSCurrBPLLoss float;
DECLARE @CashNettBookedProfit char; --global
DECLARE @CashNettBookedLoss char; --global
DECLARE @FONettBookedProfit char; --global
DECLARE @FONettBookedLoss char; --global
DECLARE @NettBPL float; --locl
DECLARE @CashFinalNettM2M float; --locl
DECLARE @IntraMarkToMarket float;
DECLARE @DelMarkToMarket float;
DECLARE @MPlusMarkToMarket float;
DECLARE @ValuePlusEQMarkToMarket float;
DECLARE @FOFinalNettM2M float; --locl
DECLARE @FOMarkToMarket float;
DECLARE @OptMarkToMarketMargin float;
DECLARE @MPlusFOMarkToMarket float;
DECLARE @CurrencyMarkToMarket float;
DECLARE @CurrencyOptMarkToMarket float;
DECLARE @ValuePlusCurrencyMarkToMarket float;
DECLARE @ValuePlusFnOMarkToMarket float;
DECLARE @EPlusFOMarkToMarket float;
DECLARE @EPLUSCurrMarkToMarket float;
DECLARE @CashNettM2MProfit char; --global
DECLARE @CashNettM2MLoss char; --global
DECLARE @FONettM2MProfit char; --global
DECLARE @FONettM2MLoss char; --global
DECLARE @NettM2M float; --locl
DECLARE @CashValidateM2MwithNett char; --global
DECLARE @FOValidateM2MwithNett char; --global
DECLARE @MTFBPLProfit float; --locl
DECLARE @MTFBPLLoss float; --locl
DECLARE @MTFMTM float; --locl

--Commodity
DECLARE @CommodityBPLProfit float;
DECLARE @CommodityBPLLoss float;
DECLARE @NCDEXBPLProfit float;
DECLARE @NCDEXBPLLoss float;
DECLARE @ValuePlusFOBPLProfit float;
DECLARE @ValueplusFOBPLLoss float;
DECLARE @CommodityMarkToMarket float;
DECLARE @ValuePlusFOMarkToMarket float;
DECLARE @NCDEXMarkToMarket float;
DECLARE @OptMarkToMarketMCX float;
DECLARE @OptMarkToMarketNCDEX float;


set @FinalAmount = 0
set @FinalAmountCommodity =0;

select @CashNettBookedProfit = CashNettBookedProfit,@CashNettBookedLoss = CashNettBookedLoss,
 @FONettBookedProfit = FONettBookedProfit,@FONettBookedLoss = FONettBookedLoss,
 @CashNettM2MProfit = CashNettM2MProfit,@CashNettM2MLoss = CashNettM2MLoss,
 @FONettM2MProfit = FONettM2MProfit, @FONettM2MLoss = FONettM2MLoss,
 @CashValidateM2MwithNett = CashValidateM2MwithNett,@FOValidateM2MwithNett = FOValidateM2MwithNett
 from tbl_Config;

select @IntraBPLProfit = IntraBPLProfit,@IntraBPLLoss = IntraBPLLoss, 
@DeliveryBPLProfit = DeliveryBPLProfit,@DeliveryBPLLoss = DeliveryBPLLoss,
---Changes Added By Junaid Pall
@MPlusBPLProfit = 0,@MplusBPLLoss = 0,
---Changes Added By Junaid Pall
@ValuePlusEQBPLProfit = ValuePlusEquityBPLProfit, @ValueplusEQBPLLoss = ValueplusEquityBPLLoss,
@FOBPLProfit = FOBPLProfit,@FOBPLLoss = FOBPLLoss,@MPlusFOBPLProfit = MPlusFOBPLProfit,
@MplusFOBPLLoss = MplusFOBPLLoss, @CurrencyBPLProfit = CurrencyBPLProfit,
@CurrencyBPLLoss = CurrencyBPLLoss, @ValuePlusCurrencyBPLProfit = ValuePlusCurrencyBPLProfit,
@ValuePlusCurrencyBPLLoss = ValuePlusCurrencyBPLLoss, @ValuePlusFnOBPLProfit = ValuePlusFnOBPLProfit,
@ValueplusFnOBPLLoss = ValueplusFnOBPLLoss, @EPlusFOBPLProfit = EPlusFOBPLProfit,
@EPlusFOBPLLoss = EPlusFOBPLLoss, @EPLUSCurrBPLProfit = EPLUSCurrBPLProfit,
@EPLUSCurrBPLLoss = EPLUSCurrBPLLoss,@IntraMarkToMarket = IntraMarkToMarket,
@DelMarkToMarket = DeliveryM2M, @MPlusMarkToMarket = MPlusMarkToMarket,
@MPlusMarkToMarket = MPlusMarkToMarket, @ValuePlusEQMarkToMarket = ValuePlusEquityMarkToMarket,
@FOMarkToMarket = FOMarkToMarket,@OptMarkToMarketMargin = OptMarkToMarketMargin,
@MPlusFOMarkToMarket = MPlusFOMarkToMarket,@CurrencyMarkToMarket = CurrencyMarkToMarket,
@CurrencyOptMarkToMarket = CurrencyOptMarkToMarket,@ValuePlusCurrencyMarkToMarket = ValuePlusCurrencyMarkToMarket,
@ValuePlusFnOMarkToMarket = ValuePlusFnOMarkToMarket,@EPlusFOMarkToMarket = EPlusFOMarkToMarket,
@EPLUSCurrMarkToMarket = EPLUSCurrMarkToMarket,@CommodityBPLProfit = CommodityBPLProfit,
@CommodityBPLLoss = CommodityBPLLoss,@NCDEXBPLProfit = NCDEXBPLProfit,@NCDEXBPLLoss = @NCDEXBPLLoss,
@ValuePlusFOBPLProfit = ValuePlusFOBPLProfit,@ValueplusFOBPLLoss = ValueplusFOBPLLoss,
@CommodityMarkToMarket = CommodityMarkToMarket,@ValuePlusFOMarkToMarket = ValuePlusFOMarkToMarket,
@NCDEXMarkToMarket = NCDEXMarkToMarket,@OptMarkToMarketMCX = OptionM2MMCX,
@OptMarkToMarketNCDEX = OptionM2MNCDEX,@MTFBPLProfit = MtfBPLProfit,@MTFBPLLoss = MtfBPLLoss,
@MTFMTM = MtfMarkToMarket
from tbl_Members where MemberCode = @Clientcode;

/*PrivateGetFinalM2MAndBPLForValidation*/
set @CashFinalNettBPL = @IntraBPLProfit + @IntraBPLLoss +
                @DeliveryBPLProfit + @DeliveryBPLLoss + @MPlusBPLProfit + @MplusBPLLoss +
                @ValuePlusEQBPLProfit + @ValueplusEQBPLLoss + @MTFBPLProfit + @MTFBPLLoss; -- mtfbplprofit + loss

set @FOFinalNettBPL = @FOBPLProfit + @FOBPLLoss + @MPlusFOBPLProfit + @MplusFOBPLLoss + @CurrencyBPLProfit + @CurrencyBPLLoss +
                @ValuePlusCurrencyBPLProfit + @ValuePlusCurrencyBPLLoss + @ValuePlusFnOBPLProfit + @ValueplusFnOBPLLoss
                + @EPlusFOBPLProfit + @EPlusFOBPLLoss + @EPLUSCurrBPLProfit + @EPLUSCurrBPLLoss;

if ((@CashFinalNettBPL > 0) AND (@CashNettBookedProfit = 'Y'))
BEGIN
	set @FinalAmount += @CashFinalNettBPL;
END

if ((@CashFinalNettBPL < 0) AND (@CashNettBookedLoss = 'Y'))
BEGIN
    set @FinalAmount += @CashFinalNettBPL;
END
            
if ((@FOFinalNettBPL > 0) AND (@FONettBookedProfit = 'Y'))
BEGIN
    set @FinalAmount += @FOFinalNettBPL;
END

if ((@FOFinalNettBPL < 0) AND (@FONettBookedLoss = 'Y'))
BEGIN
	set @FinalAmount += @FOFinalNettBPL;
END

set @NettBPL = @FinalAmount;
set @CashFinalNettM2M = @IntraMarkToMarket + @DelMarkToMarket + @MPlusMarkToMarket + @ValuePlusEQMarkToMarket + @MTFMTM; --mtfmtm

set @FOFinalNettM2M = @FOMarkToMarket + @OptMarkToMarketMargin + @MPlusFOMarkToMarket + @CurrencyMarkToMarket + @CurrencyOptMarkToMarket +
                @ValuePlusCurrencyMarkToMarket + @ValuePlusFnOMarkToMarket + @EPlusFOMarkToMarket + @EPLUSCurrMarkToMarket;   
				
if ((@CashFinalNettM2M > 0) AND (@CashNettM2MProfit = 'Y'))
   set @FinalAmount += @CashFinalNettM2M;

if ((@CashFinalNettM2M < 0) AND (@CashNettM2MLoss = 'Y'))
   set @FinalAmount += @CashFinalNettM2M;

if ((@FOFinalNettM2M > 0) AND (@FONettM2MProfit = 'Y'))
   set @FinalAmount += @FOFinalNettM2M;

if ((@FOFinalNettM2M < 0) AND (@FONettM2MLoss = 'Y'))
    set @FinalAmount += @FOFinalNettM2M;

set @NettM2M = @FinalAmount - @NettBPL;

if ((@CashValidateM2MwithNett = 'Y') AND @NettM2M > 0)
        set @FinalAmount = @NettBPL;

/*Till Here*/

/*PrivateGetFinalM2MAndBPLForValidationForCommodity*/
/*resetting local variables for reusing*/
set @NettBPL = 0;
set @NettM2M = 0;
set @FOFinalNettM2M = 0;
set @FOFinalNettBPL = 0;


set @FOFinalNettBPL = @CommodityBPLProfit + @CommodityBPLLoss +
                @NCDEXBPLProfit + @NCDEXBPLLoss + @ValuePlusFOBPLProfit + @ValueplusFOBPLLoss;-- + @MtfBPL;

if ((@FOFinalNettBPL > 0) AND (@FONettBookedProfit = 'Y'))
        set @FinalAmountCommodity += @FOFinalNettBPL;

if ((@FOFinalNettBPL < 0) AND (@FONettBookedLoss = 'Y'))
        set @FinalAmountCommodity += @FOFinalNettBPL;

set @NettBPL = @FinalAmountCommodity;
set @FOFinalNettM2M = @CommodityMarkToMarket + @ValuePlusFOMarkToMarket + @NCDEXMarkToMarket + @OptMarkToMarketMCX + @OptMarkToMarketNCDEX;

if ((@FOFinalNettM2M > 0) AND (@FONettM2MProfit = 'Y'))
    set @FinalAmountCommodity += @FOFinalNettM2M;

if ((@FOFinalNettM2M < 0) AND (@FONettM2MLoss = 'Y'))
    set @FinalAmountCommodity += @FOFinalNettM2M;

set @NettM2M = @FinalAmountCommodity - @NettBPL;

if (@FOValidateM2MwithNett = 'Y' AND @NettM2M > 0)
    set @FinalAmountCommodity = @NettBPL;

/*Till Here*/


return @FinalAmount + @FinalAmountCommodity;

END

GO
/****** Object:  UserDefinedFunction [dbo].[fn_Split]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
Create function [dbo].[fn_Split](
 @String varchar (8000),
 @Delimiter varchar (10)
 )
returns @ValueTable table ([Value] varchar(8000))
begin
 declare @NextString varchar(8000)
 declare @Pos int
 declare @NextPos int
 declare @CommaCheck varchar(1)
 
 --Initialize
 set @NextString = ''
 set @CommaCheck = right(@String,1) 
 
 --Check for trailing Comma, if not exists, INSERT
 --if (@CommaCheck <> @Delimiter )
 set @String = @String + @Delimiter
 
 --Get position of first Comma
 set @Pos = charindex(@Delimiter,@String)
 set @NextPos = 1
 
 --Loop while there is still a comma in the String of levels
 while (@pos <>  0)  
 begin
  set @NextString = substring(@String,1,@Pos - 1)
 
  insert into @ValueTable ( [Value]) Values (@NextString)
 
  set @String = substring(@String,@pos +1,len(@String))
  
  set @NextPos = @Pos
  set @pos  = charindex(@Delimiter,@String)
 end
 
 return
end
GO
/****** Object:  UserDefinedFunction [dbo].[fnEncDecRc4]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS OFF
GO
SET QUOTED_IDENTIFIER OFF
GO
CREATE FUNCTION [dbo].[fnEncDecRc4](@pwd [nvarchar](max), @text [nvarchar](max))
RETURNS [nvarchar](max) WITH EXECUTE AS CALLER
AS 
EXTERNAL NAME [EncDecCLR64Bit].[UserDefinedFunctions].[fnEncDecRc4]
GO
/****** Object:  UserDefinedFunction [dbo].[fnEncDecRc4_Original]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO



--ANd this function does the encrypt/decrypt part

Create FUNCTION [dbo].[fnEncDecRc4_Original]
(
	@Pwd VARCHAR(256),
	@Text VARCHAR(8000)
)
RETURNS	VARCHAR(8000)
AS

BEGIN
	DECLARE	@Box TABLE (i TINYINT, v TINYINT)

	INSERT	@Box
		(
			i,
			v
		)
	SELECT	i,
		v
	FROM	dbo.fnInitRc4(@Pwd)

	DECLARE	@Index SMALLINT,
		@i SMALLINT,
		@j SMALLINT,
		@t TINYINT,
		@k SMALLINT,
      		@CipherBy TINYINT,
      		@Cipher VARCHAR(8000)

	SELECT	@Index = 1,
		@i = 0,
		@j = 0,
		@Cipher = ''

	WHILE @Index <= DATALENGTH(@Text)
		BEGIN
			SELECT	@i = (@i + 1) % 256

			SELECT	@j = (@j + b.v) % 256
			FROM	@Box b
			WHERE	b.i = @i

			SELECT	@t = v
			FROM	@Box
			WHERE	i = @i

			UPDATE	b
			SET	b.v = (SELECT w.v FROM @Box w WHERE w.i = @j)
			FROM	@Box b
			WHERE	b.i = @i

			UPDATE	@Box
			SET	v = @t
			WHERE	i = @j

			SELECT	@k = v
			FROM	@Box
			WHERE	i = @i

			SELECT	@k = (@k + v) % 256
			FROM	@Box
			WHERE	i = @j

			SELECT	@k = v
			FROM	@Box
			WHERE	i = @k

			SELECT	@CipherBy = ASCII(SUBSTRING(@Text, @Index, 1)) ^ @k,
				@Cipher = @Cipher + CHAR(@CipherBy)

			SELECT	@Index = @Index  +1
      		END

	RETURN	@Cipher
END






GO
/****** Object:  UserDefinedFunction [dbo].[fnInitRc4]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

--This function is used to initialize the seed for the RC4 algorithm
Create FUNCTION [dbo].[fnInitRc4]
(
	@Pwd VARCHAR(256)
)
RETURNS @Box TABLE (i TINYINT, v TINYINT)
AS

BEGIN
	DECLARE	@Key TABLE (i TINYINT, v TINYINT)

	DECLARE	@Index SMALLINT,
		@PwdLen TINYINT

	SELECT	@Index = 0,
		@PwdLen = LEN(@Pwd)

	WHILE @Index <= 255
		BEGIN
			INSERT	@Key
				(
					i,
					v
				)
			VALUES	(
					@Index,
					 ASCII(SUBSTRING(@Pwd, @Index % @PwdLen + 1, 1))
				)

			INSERT	@Box
				(
					i,
					v
				)
			VALUES	(
					@Index,
					@Index
				)

			SELECT	@Index = @Index + 1
		END


	DECLARE	@t TINYINT,
		@b SMALLINT

	SELECT	@Index = 0,
		@b = 0

	WHILE @Index <= 255
		BEGIN
			SELECT		@b = (@b + b.v + k.v) % 256
			FROM		@Box AS b
			INNER JOIN	@Key AS k ON k.i = b.i
			WHERE		b.i = @Index

			SELECT	@t = v
			FROM	@Box
			WHERE	i = @Index

			UPDATE	b1
			SET	b1.v = (SELECT b2.v FROM @Box b2 WHERE b2.i = @b)
			FROM	@Box b1
			WHERE	b1.i = @Index

			UPDATE	@Box
			SET	v = @t
			WHERE	i = @b

			SELECT	@Index = @Index + 1
		END

	RETURN
END




GO
/****** Object:  UserDefinedFunction [dbo].[FUNC_CHECK_PWD_FOR_ALPHA_NUMERIC_SPECIAL_CHAR]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE FUNCTION [dbo].[FUNC_CHECK_PWD_FOR_ALPHA_NUMERIC_SPECIAL_CHAR](@PWD as nvarchar(25)) 

RETURNS VARCHAR(25)
As

--Select dbo.FUNC_CHECK_PWD_FOR_ALPHA_NUMERIC_SPECIAL_CHAR('TESTING@123')

BEGIN

	-- Create variables for the character string and for the current 
	-- position in the string.
	DECLARE @position int, @string char(30), @asciival int
	DECLARE @alpha int, @numeric int, @special int
	-- Initialize the current position and the string variables.
	SET @PWD=Ltrim(Rtrim(@PWD))
	SET @alpha=0
	SET @numeric=0
	--SET @special=0 --@@Rajesh
	SET @position = 1
	SET @string = @PWD
	WHILE @position <= DATALENGTH(@string)
	   BEGIN
		   --SELECT ASCII(SUBSTRING(@string, @position, 1)),CHAR(ASCII(SUBSTRING(@string, @position, 1)))
			SET @asciival=0
			SET @asciival=ASCII(SUBSTRING(@string, @position, 1))		
			If @asciival>=97 And @asciival<=122
				BEGIN
					SET @alpha=1
				END
			If @asciival>=65 And @asciival<=90
				BEGIN
					SET @alpha=1
				END
			If @asciival>=48 And @asciival<=57
				BEGIN
					SET @numeric=1
				END
			/*If @asciival>=91 And @asciival<=96
				BEGIN
					SET @special=1
				END
			If @asciival>=58 And @asciival<=64
				BEGIN
					SET @special=1
				END
			If @asciival>=123 And @asciival<=126
				BEGIN
					SET @special=1
				END
			If @asciival>=33 And @asciival<=47
				BEGIN
					SET @special=1
				END
			If @asciival=130
				BEGIN
					SET @special=1
				END*/ --@@Rajesh

			SET @position = @position + 1
	   END

	If @alpha=0 
	BEGIN
		return 'NOALPHA'
	END

	If @numeric=0 
	BEGIN
		return 'NONUMERIC'
	END

	/*If @special=0 
	BEGIN
		return 'NOSPECIAL'
	END*/ --@@Rajesh

	--If @alpha<>0 And @numeric<>0 And @special<>0
	--BEGIN
	Return 'CORRECT'
	--END

END

/*
select ASCII('a') --97
select ASCII('z') --122
select ASCII('A') --65
select ASCII('Z') --90
select ASCII('1') --49
select ASCII('9') --57
select ASCII('@') --57

select CHAR(91)
select CHAR(92)
select CHAR(93)
select CHAR(94)
select CHAR(95)
select CHAR(96)
---------------
select CHAR(58)
select CHAR(59)
select CHAR(60)
select CHAR(61)
select CHAR(62)
select CHAR(63)
select CHAR(64)
---------------
select CHAR(33)
select CHAR(34)
select CHAR(35)
select CHAR(36)
select CHAR(37)
select CHAR(38)
select CHAR(39)
select CHAR(40)
select CHAR(41)
select CHAR(42)
select CHAR(43)
select CHAR(44)
select CHAR(45)
select CHAR(46)
select CHAR(47)
---------------
select CHAR(123)
select CHAR(124)
select CHAR(125)
select CHAR(126)
select CHAR(130)
*/




