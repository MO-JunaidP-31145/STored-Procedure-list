	[ExerciseStartDate] [varchar](50) NULL,
	[ExerciseEndDate] [varchar](50) NULL,
	[TickerSelection] [varchar](50) NULL,
	[OldTokenNumber] [varchar](50) NULL,
	[CreditRating] [char](12) NULL,
	[InstName] [char](25) NULL,
	[EGMAGM] [char](1) NULL,
	[InterestDivident] [char](1) NULL,
	[RightsBonus] [char](1) NULL,
	[MFAON] [char](1) NULL,
	[Remarks] [char](24) NULL,
	[ExStyle] [char](1) NULL,
	[ExAllowed] [char](1) NULL,
	[ExRejectionAllowed] [char](1) NULL,
	[PlAllowed] [char](1) NULL,
	[CheckSum] [char](1) NULL,
	[IsCOrporateAdjusted] [char](1) NULL,
	[SymbolForAsset] [char](10) NULL,
	[InstrumentOfAsset] [char](6) NULL,
	[BasePrice] [varchar](50) NULL,
	[DeleteFlag] [char](1) NULL
) ON [PRIMARY]
GO
