	[StrikePrice] [int] NULL,
	[CALevel] [int] NULL,
	[ParticipantId] [char](12) NULL,
	[RecordInsertedDate] [datetime] NOT NULL,
	[IPAddress] [varchar](20) NULL,
	[MacOrDeviceID] [varchar](40) NULL,
	[Reserved] [varchar](10) NULL
) ON [PRIMARY]
