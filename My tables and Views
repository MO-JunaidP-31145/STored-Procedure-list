/****** Object:  View [dbo].[Vw_CommOptionEx_MarginPerLot]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


CREATE View [dbo].[Vw_CommOptionEx_MarginPerLot]
as
select * from 
(
select dense_rank() over(partition by A.ScripShortName order by A.ExpiryDate) Rowno,A.ExchangeCode,A.ScripCode,A.ULToken,A.ScripShortName,
A.ScripFullName,A.AdditionalLongMargin,A.SpecialLongMargin,A.AdditionalShortMargin,A.SpecialShortMargin,
A.StrikePrice,A.Multiplier, A.Marketlot,B.SpanMarginPerLot,B.ClosePrice
from tbl_Scrips A
left outer join 
tbl_SpanExposurePercent B 
on A.ExchangeCode = B.ExchangeCode and A.ULToken = B.ScripCode
where A.OptionType in ('CE','PE')and A.Exchangecode in (5,6) and 
--Convert(varchar(12),dateadd(ss,A.ExpiryDate,'01-Jan-1980'),106) = Convert(varchar(12),Convert(datetime ,'2017-10-27 00:00:00.000'),106) 
Convert(varchar(12),dateadd(ss,A.ExpiryDate,'01-Jan-1980'),106) = Convert(varchar(12),GETDATE(),106) 
)h where Rowno = 1

GO
/****** Object:  View [dbo].[vw_PhysicalOption_View]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

 
CREATE view [dbo].[vw_PhysicalOption_View]  
as  
--declare @ltp table (ScripShortName varchar(50),MarketPrice float,ExchangeCode int,ScripCode int,ScripIsinNo varchar(50))  
--insert into @ltp  
  
--select * from @ltp  
  
select   
a.MemberCode [ClientCode]  
,c.MemberName [ClientName]  
,c.Branch  
,c.SubBroker  
,b.ScripShortName [Symbol]  
,b.InstrumentName [InstrumentType]  
,b.optionType [OptionType]  
,b.StrikePrice [StrikePrice]  
,convert(varchar,CAst(dateadd(ss,b.ExpiryDate ,'1980-01-01') as dateTime),106)[ExpiryDate]  
,a.BotQty - a.SoldQty [OpenQty]  
--,a.exposureMargin [Exposure]  
,d.MarketPrice * (abs(a.BotQty - a.SoldQty)) [Exposure]  
,b.MarketPrice [LTP]  
,d.MarketPrice [SpotPrice]  
,case when b.optionType = 'XX' then 0 else d.marketPrice - b.strikePrice end [SpotMinusStrike]  
,case   
when b.optionType = 'CE' then case when (d.marketPrice - b.strikePrice) > 0 then 'ITM' else 'OTM' end  
when b.optionType = 'PE' then case when (d.marketPrice - b.strikePrice) < 0 then 'ITM' else 'OTM' end  
else '' end [ITMOTM]  
/*
,Round((
+c.BaseCapital
+c.FOBPLLoss
+c.FOBPLProfit
+c.CurrencyBPLLoss
+c.CurrencyBPLProfit
+c.IntraBPLLoss
+c.IntraBPLProfit
+c.DeliveryBPLLoss
+c.DeliveryBPLProfit
+c.MPlusBPLProfit
+c.MplusBPLLoss
+c.MPlusFOBPLProfit
+c.MplusFOBPLLoss
+c.ScripCapital
+c.StockCapitalFO
+c.CFBuyOptionValue
+c.MarginPaid
+c.MFEquityAmount
+c.MaxUnrealisedAmount
+c.FundTransferAmt
+c.InitialMarginPaid
-c.WithDrawnAmount
+c.OptionM2MProfit
+c.OptionM2MLoss
+c.CurrencyOptMarkToMarketProfit
+c.CurrencyOptMarkToMarketLoss
+c.CFSellOptionValue
+c.ValuePlusFnOBPLProfit
+c.ValueplusFnOBPLLoss
+c.ValuePlusCurrencyBPLProfit
+c.ValuePlusCurrencyBPLLoss
+c.ValuePlusEquityBPLProfit
+c.ValueplusEquityBPLLoss
-c.MPLUSFOMargin
-c.MPLUSMargin
-c.InitialMargin
-c.ExposureMargin
---c.FOExposureMargin
-c.CurrencyExposureMargin
-c.InitialMarginCurrency
+c.FOMarkToMarket
+c.CurrencyMarkToMarket
+c.IntraMarkToMarket
--+c.DelMarkToMarket
+c.DeliveryM2M
-c.InitialMarginCommodity
-c.CommodityExposureMargin
-c.InitialMarginNCDEX
-c.NCDEXExposureMargin
+c.EPlusFOBPLProfit
+c.EPlusFOBPLLoss
+c.EPLUSCurrBPLProfit
+c.EPLUSCurrBPLLoss
+c.UnRealisedAmount
+c.CommodityMarkToMarket
+c.CommodityBPLProfit
+c.CommodityBPLLoss
+c.ValuePlusFOBPLProfit
+c.ValueplusFOBPLLoss
+c.NCDEXMarkToMarket
+c.NCDEXBPLProfit
+c.NCDEXBPLLoss
--+c.OptMarkToMarketLossMCX
--+c.OptMarkToMarketProfitMCX
--+c.OptMarkToMarketLossNCDEX
--+c.OptMarkToMarketProfitNCDEX
+c.OptionM2MLossMCX
+c.OptionM2MProfitMCX
+c.OptionM2MLossNCDEX
+c.OptionM2MProfitNCDEX
-c.AdditionalLongMarginBlocked
-c.AdditionalShortMarginBlocked
-c.SpecialLongMarginBlocked
-c.SpecialShortMarginBlocked
-c.AdditionalLongMarginBlockedNcdex
-c.AdditionalShortMarginBlockedNcdex
-c.SpecialLongMarginBlockedNcdex
-c.SpecialShortMarginBlockedNcdex
),2) [BaseCapital]
*/
,Round(c.baseCapital,2) [BaseCapital]
,c.IsNonPOA
,isnull(e.AvailableQty,0)AvailableQty  
,null [QtyToConvert]  
,b.marketLot  
,b.exchangeCode  
,f.OpenQty[IntentQty],f.Exposure[IntentExposure],f.LTP[IntentLTP],f.SpotPrice[IntentSportPrice],f.SpotMinusStrike[IntentSpotMinusStrike]  
,a.ScripCode  
,f.id,f.ITMOTM[IntentITMOTM],f.AvailableCash[IntentAvailableCash],f.AvailableQty[IntentAvailableQty],f.IntentonForDelivery,f.QtyMArkedForConvert  

,b.scripFullName 
,b.MarketPrice
,case 
when (a.BotQty - a.SoldQty) > 0 then Round((b.MarketPrice * (100 - (select top 1 CapitalM2MCircuitFilter from dbo.tbl_Config)))/100,1) 
when (a.BotQty - a.SoldQty) < 0 then Round((b.MarketPrice * (100 + (select top 1 CapitalM2MCircuitFilter from dbo.tbl_Config)))/100,1) end as Price
from dbo.tbl_MemberInstrument a with (nolock)  
inner join dbo.tbl_Scrips b with (nolock) on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.ScripCode  
inner join   
(select a.ScripShortName,a.MarketPrice,a.ExchangeCode,a.ScripCode,a.ScripIsinNo from dbo.tbl_Scrips a with (nolock) left outer join  
(select ScripShortName,ULToken from dbo.tbl_Scrips b with (nolock) where ExchangeCode = 2 and optiontype in ('XX','CE','PE') group by ScripShortName,ULToken) b  
on a.scripshortname = b.scripshortname and a.ultoken = b.ultoken  
where a.exchangecode = 0 and b.ScripShortName is not null  
) d on b.ScripShortName = d.ScripShortName  
left outer join dbo.tbl_Members c with (nolock)  on a.membercode = c.MemberCode  
left outer join dbo.tbl_DPHoldings e with (nolock) on d.scripisinno = e.ScripIsInNo and a.MemberCode = e.MemberCode  
  
LEFT OUTER JOIN tbl_PhysicalOptionsDelivery F with (nolock) ON A.ExchangeCode = f.ExchangeCode and a.scripcode = f.scripcode and a.membercode = f.Clientcode  
INNER JOIN
(
SELECT EXPIRYDATE FROM (
select ROW_NUMBER() OVER(ORDER BY expirydate)ID,expirydate from dbo.tbl_Scrips b with (nolock) where ExchangeCode = 2 and optiontype='XX' and ScripShortName = 'NIFTY'
)ID WHERE ID = 1
)G ON G.ExpiryDate = B.ExpiryDate
left outer join pmp.dbo.tbl_PhysicalOptionsException H on c.Branch = H.val and h.Typ = 'B'
left outer join pmp.dbo.tbl_PhysicalOptionsException I on c.SUBBROKER = I.val and I.Typ = 'S'
left outer join pmp.dbo.tbl_PhysicalOptionsException J on a.MemberCode = J.val and J.Typ = 'C'
left outer join dbo.tbl_MultipleExceptionMaster K on a.MemberCode = k.clientCode and k.exceptionType = 'PHYSICAL'
where b.IsFOScrip = 1 and b.ExchangeCode = 2 and b.optiontype in ('CE','PE','XX') and b.InstrumentName like '%STK%'  
and BotQty - SoldQty <> 0  
AND (H.val IS NULL AND i.VAL IS NULL AND J.VAL IS NULL)
and K.Clientcode is null
--and (a.membercode like 'h169%')
--and a.MemberCode in ('','AA017','AA018','AA019')
--order by b.ScripShortName,a.MemberCode  
  
  
--select top 0 * from tbl_PhysicalOptionsDelivery  
















GO
