/****** Object:  View [dbo].[Vw_M2M_CapitalGlobalSqOff_BOSMSClient]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


CREATE View [dbo].[Vw_M2M_CapitalGlobalSqOff_BOSMSClient]
as
       select a.Membercode,a.VPlusLossPercent,a.CapitalGlobalLossPercent, a.Flag, b.MOBILENo as Mobile, b.Email as EmailId, 
	   b.Branch,a.ValuePluseSqOffPercent,a.CapitalGlobalSqOffPercent
       from
       (
              select MemberCode, ROUND(EquityValuePluseLossPercent,2) AS VPlusLossPercent, 
                     ROUND(CapitalGlobalLossPercent,2) AS CapitalGlobalLossPercent, 'N' as Flag,
					 Round(EquityValuePluseSqOffPercent,2) as ValuePluseSqOffPercent,
					 Round(CapitalGlobalSqOffPercent,2) as CapitalGlobalSqOffPercent
              from tbl_members with(nolock)
			  --WHERE (CapitalGlobalLossPercent>50 and AllowCapitalBasedSqOff = 'Y') or EquityValuePluseLossPercent > 50
			  WHERE (CapitalGlobalLossPercent>(CapitalGlobalSqOffPercent-20) and AllowCapitalBasedSqOff = 'Y') or EquityValuePluseLossPercent > 50
              /*where (CapitalGlobalLossPercent>50 or EquityValuePluseLossPercent > 50)
              and AllowCapitalBasedSqOff = 'Y'*/

       ) a ,
       (
                     select ClientCode, MOBILENo, Email,Branch
                     from PMP.DBO.Tbl_BOD_ClientActivation with(nolock)
                     --where len(MOBILENo)>8 and isnumeric(MOBILENo)=1
       ) b
       where a.membercode=b.ClientCode







GO
/****** Object:  View [dbo].[vw_Holding_valuation]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO





CREATE view [dbo].[vw_Holding_valuation]
as
select MemberCode,a.ScripIsInNo,a.ScripName
,AvailableQty HoldingQty
,AvailableQty - BlockedQty as [RemainingQty] 
,b.MarketPrice [PriceNse]
,c.MarketPrice [PriceBse]
,(AvailableQty - BlockedQty) * (case when b.MarketPrice < c.MarketPrice then c.MarketPrice else b.MarketPrice end)Valuation
,AvailableQty - BlockedQty - /*BuyBackQty*/TPinQty - CollateralQty - debitqty as [RemainingQtyBuyBack] 
,(AvailableQty - BlockedQty - /*BuyBackQty*/TPinQty - CollateralQty - debitqty) * (case when b.MarketPrice < c.MarketPrice then c.MarketPrice else b.MarketPrice end)ValuationBuyBack
,POAQty,CollateralQty,DebitQty,BlockedQty,BuyBackQty,TPinQty

from awacs10.dbo.tbl_DPHoldings (nolock) a
inner join awacs10.dbo.tbl_scrips (nolock) b on a.ScripIsInNo = b.ScripIsInNo and b.ExchangeCode = 0 
inner join awacs10.dbo.tbl_scrips (nolock) c on a.ScripIsInNo = c.ScripIsInNo and c.ExchangeCode = 1 



GO
