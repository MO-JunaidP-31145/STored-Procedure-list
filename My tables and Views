/****** Object:  View [dbo].[vw_PhysicalOption_View_MCX]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
Create view [dbo].[vw_PhysicalOption_View_MCX]  
as  
select   
a.MemberCode [ClientCode]  
,c.MemberName [ClientName]  
,c.Branch  
,c.SubBroker  
,b.ScripShortName [Symbol]  
,b.InstrumentName [InstrumentType]  
,b.optionType [OptionType]  
,b.StrikePrice [StrikePrice]  
,convert(varchar,CAst(dateadd(ss,b.ExpiryDate ,'1980-01-01') as dateTime),106)[ExpiryDate]  
,a.BotQty - a.SoldQty [OpenQty]  
--,a.exposureMargin [Exposure]  
,d.MarketPrice * (abs(a.BotQty - a.SoldQty)) [Exposure]  
,b.MarketPrice [LTP]  
,d.MarketPrice [SpotPrice]  
,case when b.optionType = 'XX' then 0 else d.marketPrice - b.strikePrice end [SpotMinusStrike]  
,case   
when b.optionType = 'CE' then case when (H.marketPrice - b.strikePrice) > 0 then 'ITM' else 'OTM' end  
when b.optionType = 'PE' then case when (H.marketPrice - b.strikePrice) < 0 then 'ITM' else 'OTM' end  
else '' end [ITMOTM]  

,Round(c.baseCapital,2) [BaseCapital]
,c.IsNonPOA
--,isnull(e.AvailableQty,0)AvailableQty  
,0 as AvailableQty  
,null [QtyToConvert]  
,b.marketLot  
,b.exchangeCode  
,f.OpenQty[IntentQty],f.Exposure[IntentExposure],f.LTP[IntentLTP],f.SpotPrice[IntentSportPrice],f.SpotMinusStrike[IntentSpotMinusStrike]  
,a.ScripCode  
,f.id,f.ITMOTM[IntentITMOTM],f.AvailableCash[IntentAvailableCash],f.AvailableQty[IntentAvailableQty],f.IntentionForDelivery,f.QtyMArkedForConvert  

,b.scripFullName 
,b.MarketPrice
,case 
when (a.BotQty - a.SoldQty) > 0 then Round((b.MarketPrice * (100 - (select top 1 CapitalM2MCircuitFilter from dbo.tbl_Config)))/100,1) 
when (a.BotQty - a.SoldQty) < 0 then Round((b.MarketPrice * (100 + (select top 1 CapitalM2MCircuitFilter from dbo.tbl_Config)))/100,1) end as Price

from dbo.tbl_MemberInstrument a with (nolock)  
inner join dbo.tbl_Scrips b with (nolock) on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.ScripCode  

inner join   
(select a.ScripShortName,a.MarketPrice,a.ExchangeCode,a.ScripCode,a.ScripIsinNo from dbo.tbl_Scrips a with (nolock)  left outer join  
(select ScripShortName,ULToken from dbo.tbl_Scrips b with (nolock) where ExchangeCode = 6 and optiontype in ('XX','CE','PE') group by ScripShortName,ULToken) b  
on a.scripshortname = b.scripshortname and a.ultoken = b.ultoken  
where a.exchangecode = 6 and b.ScripShortName is not null  
) d on b.ScripShortName = d.ScripShortName  and a.ScripCode = d.ScripCode
left outer join dbo.tbl_Members c with (nolock)  on a.membercode = c.MemberCode  
LEFT OUTER JOIN tbl_CommodityOptionsDelivery F with (nolock) ON A.ExchangeCode = f.ExchangeCode and a.scripcode = f.scripcode and a.membercode = f.Clientcode  
LEFT OUTER JOIN
(select * from 
(select row_number() over (partition by ExchangeCode, ScripShortName order by  ExpiryDate asc  )as srno ,
ScripShortName ,ScripCode ,ExchangeCode, MarketPrice
from dbo.tbl_Scrips (nolock)
where ExchangeCode = 6 and MARKETPRICE > 0 and OptionType = 'XX'
) a where srno = 1)  H on B.ExchangeCode = H.ExchangeCode and B.ScripShortName = H.ScripShortName


where b.IsFOScrip = 1 and b.ExchangeCode = 6 and b.optiontype in ('CE','PE','XX') --and b.InstrumentName like '%STK%'  
and BotQty - SoldQty <> 0  
and BotQty - SoldQty <> 0  
and  case   when b.optionType = 'CE' then  case when ( H.marketPrice - b.strikePrice) > 0 then 'ITM' else 'OTM' end  
when b.optionType = 'PE' then case when (H.marketPrice - b.strikePrice) < 0 then 'ITM' else 'OTM' end  
else '' end    <> 'OTM'
and dateadd(ss,b.ExpiryDate,'01-01-1980') between getdate() and getdate()+10 -- To fetch only near month open positions

GO
/****** Object:  Table [dbo].[tbl_enum_OrderStatusMaster]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[tbl_enum_OrderStatusMaster](
	[OrderStatusCode] [numeric](18, 0) NOT NULL,
	[OrderStatusName] [varchar](50) NOT NULL,
 CONSTRAINT [PK_tbl_enum_OrderStatusMaster] PRIMARY KEY CLUSTERED 
(
	[OrderStatusCode] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON, OPTIMIZE_FOR_SEQUENTIAL_KEY = OFF) ON [PRIMARY]
) ON [PRIMARY]
GO
/****** Object:  View [dbo].[Vw_OrderViewForWelthManagementSystem]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[Vw_OrderViewForWelthManagementSystem] 
as

select a.UniqueOrderId,

a.TradedQty,TotalQty,TotalRemainingQty,



a.OrderType,F.OrderStatusName as  OrderStatus,ConfirmedSequenceNo,    
MessageCode,ErrorCode,b.NumberOfDecimals,Source,

D.ExchangeName ExchangeCode,
b.ScripCode,b.OrderNo as OrderNumber ,b.ClientCode,b.BuyOrSell,b.DisclosedQty,    

--C.OrderQty ,TotalQtyRemaining, QtyTradedToday,
 c.OrderQty, c.QtyRemaining as TotalQtyRemaining,  c.TradeQty as QtyTradedToday,



C.TradePrice,TriggerPrice,GoodTillDate,EntryDateTime,MinFillQty, dateadd(ss, LastModifiedTime,'01-01-1980') as LastModifiedTime,    
b.OrderInitiatorId,Remarks,b.BookType,b.OrderLife,b.ExchangeDispatcherId, E.ProductName as  ProductType ,b.ProductFlag,b.TerminalID,    
 dateadd(ss, b.ExpiryDate,'01-01-1980') as ExpiryDate ,BunchSeqNo,AMOOrder,AMOOrderValidated,MPlusDiffPrice,Symbol,Series,    
b.Instrumentname,b.StrikePrice,b.CALevel,ErrorDetail,UserRefText,ParticipantCode, s.LastTradedPrice,
s. ScripFullName as SecurityName,
s.InstrumentName as  InstrumentType,
Optiontype,
isnull(C.TradeNo,0) as TradeNo
--TradeNo,
--OrderNumber

from tbl_OrderMaster a with (nolock)
inner join tbl_Order b with (nolock) on a.UniqueOrderId=b.UniqueOrderId and b.BunchSeqNo=a.ConfirmedSequenceno    
inner join tbl_ExchangeMaster D  (nolock) on B.ExchangeCode = D.ExchangeCode
inner join tbl_enum_ProductMaster E  (nolock) on B.ProductType = E.ProductCode
inner join  tbl_enum_OrderStatusMaster F (nolock) on B.OrderStatus = F.OrderStatusCode
left join tbl_Trade C with (nolock) on a.UniqueOrderId=c.UniqueOrderId 
inner join tbl_Scrips s with (nolock) on b.ScripCode = s.ScripCode and b.ExchangeCode = s.ExchangeCode
where b.OrderNo<>'0'
and b.ClientCode in --(select clientcode from #test)
(select  clientcode  from [Advisory].MOSLFAWLIVE.dbo.VW_PWM_EQUITY_RM_CLIENT with (nolock) )


GO
/****** Object:  View [dbo].[VW_OrderTradeForOTP]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE view [dbo].[VW_OrderTradeForOTP]  
As  
Select 
A.ClientCode,   T.OrderNo,   
dateadd(ss,T.TradeDateTime,'01-01-1980') as  Tradedate,  
dateadd(ss,EntryDateTime,'01-01-1980')  Order_Placed_Time,  
ScripFullName, ScripName,A.OrderQty AS TotalOrderQty,  TradeQty,  
MarketLot as  Lot_Size,  A.BuyOrSell, 
CONVERT(DECIMAL(18,2),(A.Price/POWER(10.0,A.NumberOfDecimals))) As  OrderPrice,
CONVERT(DECIMAL(18,2),(T.TradePrice/POWER(10.0,T.NumberOfDecimals))) As TradePrice,  
A.StrikePrice,  S.ExpiryDate ,  S.OptionType,

Case 
-- Old Start

	 --When A.Remarks like 'NewOrder,%|%' Then 'Drishti Order Call Connected' 
	 --When A.Remarks like 'NewOTPOrder,%|%' Then 'Drishti OTP Order Call Connected'
	 
	 --When A.Remarks = 'NewOTPOrder' Then 'NEW OTP Order, Non Drishti'
	 --When A.Remarks = 'NewOrder, Exception'  then 'Drishti Order Call Not Connected'

	 --When A.Remarks = 'NewOTPOrder, Exception'  then 'Drishti OTP Order Call Not Connected'

	 --When A.Remarks like 'MDOrder,%|%' Then 'Drishti Order Call Connected' 
	 --When A.Remarks like 'MDOTPOrder,%|%' Then 'Drishti OTP Order with Call Connected'
	 --When A.Remarks = 'MDOTPOrder' Then 'NEW OTP Order Non Drishti'
	 --When A.Remarks = 'MDOTPOrder, Exception'  then 'Drishti OTP Order Call Not Connected'

	 When A.Remarks like 'NewOrder,%|%' Then 'Drishti Order with Registered Number' 
	 When A.Remarks like 'NewOTPOrder,%|%' Then 'Drishti OTP Order with Non Registered Number'
	 When A.Remarks like 'NewConOrder,%|%' Then 'Drishti Consent Order with Non Registered Number'
	 When A.Remarks like 'NewCallOrder,%|%' Then 'Drishti Call Recording Order with Non Registered Number'
	 
	 When A.Remarks = 'NewOTPOrder'  or A.Remarks = 'NewOTPOrder|O'  Then 'NEW OTP Order, Non Drishti'
	 When A.Remarks = 'NewConOrder'  or A.Remarks = 'NewConOrder|X'  Then 'NEW Consent Order, Non Drishti'
	 When A.Remarks = 'NewCallOrder' or A.Remarks = 'NewCallOrder|R' Then 'NEW Call Recording Order, Non Drishti'
	 
	 When A.Remarks = 'NewOrder, Exception'  then 'Drishti Order with Registered Number Call Not Connected'

	 When A.Remarks = 'NewOTPOrder, Exception'  or A.Remarks = 'NewOTPOrder|O, Exception'   then 'Drishti OTP Order Call Not Connected'
	 When A.Remarks = 'NewConOrder, Exception'  or A.Remarks = 'NewConOrder|X, Exception'  then 'Drishti Consent Order Call Not Connected'
	 When A.Remarks = 'NewCallOrder, Exception' or A.Remarks = 'NewCallOrder|R, Exception' then 'Drishti Call Recording Order Call Not Connected'

	 When A.Remarks like 'MDOrder,%|%' Then 'Drishti Order with Registered Number' 
	 
	 When A.Remarks like 'MDOTPOrder,%|%' or A.Remarks like 'MDOTPOrder|O,%|%' Then 'Drishti OTP Order with Non Registered Number'
	 When A.Remarks like 'MDConOrder,%|%' or A.Remarks like 'MDConOrder|X,%|%' Then 'Drishti Consent Order with Non Registered Number'
	 When A.Remarks like 'MDCallOrder,%|%'or A.Remarks like 'MDCallOrder|R,%|%' Then 'Drishti Call Recording Order with Non Registered Number'

	 When A.Remarks = 'MDOTPOrder'  or A.Remarks = 'MDOTPOrder|O'  Then 'NEW OTP Order'
	 When A.Remarks = 'MDConOrder'  or A.Remarks = 'MDConOrder|X'  Then 'NEW Consent Order'
	 When A.Remarks = 'MDCallOrder' or A.Remarks = 'MDCallOrder|R' Then 'NEW Call Recording Order'
	 
	 When A.Remarks = 'MDOTPOrder, Exception'  or A.Remarks = 'MDOTPOrder|O, Exception'  then 'Drishti OTP Order Call Not Connected'
	 When A.Remarks = 'MDConOrder, Exception'  or A.Remarks = 'MDConOrder|X, Exception'  then 'Drishti Consent Order Call Not Connected'
	 When A.Remarks = 'MDCallOrder, Exception' or A.Remarks = 'MDCallOrder|R, Exception' then 'Drishti Call Recording Order Call Not Connected'




-- Old End

-- NEW
	-- Normal Order
	 When A.Remarks = 'NewOrderO' or A.Remarks = 'NewOrder|O' Then 'NEW OTP Order'
	 When A.Remarks = 'NewOrderX' or A.Remarks = 'NewOrder|X' Then 'NEW Consent Order'
	 When A.Remarks = 'NewOrderR' or A.Remarks = 'NewOrder|R' Then 'NEW Call Recording Order'

	 --When A.Remarks like '%|O%' Then 'NEW OTP Order'
	 --When A.Remarks like '%|X%' Then 'NEW Consent Order'
	 --When A.Remarks like '%|R%' Then 'NEW Call Recording Order'
	 --When A.Remarks like '%|D%' Then 'Drishti Order with Registered Number' 

	 When A.Remarks like 'NewOrderD,%|%' or A.Remarks like 'NewOrder|D,%|%' Then 'Drishti Order with Registered Number' 
	 When A.Remarks like 'NewOrderO,%|%' or A.Remarks like 'NewOrder|O,%|%' Then 'Drishti OTP Order with Non Registered Number'
	 When A.Remarks like 'NewOrderX,%|%' or A.Remarks like 'NewOrder|X,%|%' Then 'Drishti Consent Order with Non Registered Number'
	 When A.Remarks like 'NewOrderR,%|%' or A.Remarks like 'NewOrder|R,%|%' Then 'Drishti Call Recording Order with Non Registered Number'
	 
	 When A.Remarks = 'NewOrderO,Exception' or A.Remarks = 'NewOrder|O,Exception'  then 'Drishti OTP Order Call Not Connected'
	 When A.Remarks = 'NewOrderX,Exception' or A.Remarks = 'NewOrder|X,Exception'  then 'Drishti Consent Order Call Not Connected'
	 When A.Remarks = 'NewOrderR,Exception' or A.Remarks = 'NewOrder|R,Exception' then 'Drishti Call Recording Order Call Not Connected'

	 -- Market Depth
	 When A.Remarks = 'MDOrderO' or A.Remarks = 'MDOrder|O' Then 'NEW OTP Order'
	 When A.Remarks = 'MDOrderX' or A.Remarks = 'MDOrder|X' Then 'NEW Consent Order'
	 When A.Remarks = 'MDOrderR' or A.Remarks = 'MDOrder|R' Then 'NEW Call Recording Order'
	 
	 When A.Remarks like 'MDOrderD,%|%' or A.Remarks like 'MDOrder|D,%|%' Then 'Drishti Order with Registered Number' 
	 When A.Remarks like 'MDOrderO,%|%' or A.Remarks like 'MDOrder|O,%|%' Then 'Drishti OTP Order with Non Registered Number'
	 When A.Remarks like 'MDOrderX,%|%' or A.Remarks like 'MDOrder|X,%|%' Then 'Drishti Consent Order with Non Registered Number'
	 When A.Remarks like 'MDOrderR,%|%' or A.Remarks like 'MDOrder|R,%|%' Then 'Drishti Call Recording Order with Non Registered Number'
	 
	 When A.Remarks = 'MDOrderO,Exception' or A.Remarks = 'MDOrder|O,Exception'  then 'Drishti OTP Order Call Not Connected'
	 When A.Remarks = 'MDOrderX,Exception' or A.Remarks = 'MDOrder|X,Exception'  then 'Drishti Consent Order Call Not Connected'
	 When A.Remarks = 'MDOrderR,Exception' or A.Remarks = 'MDOrder|R,Exception' then 'Drishti Call Recording Order Call Not Connected'

		-- TGS - Chart
	 When A.Remarks = 'TFCOrderO'           or A.Remarks = 'TFCOrder|O'            Then 'NEW OTP Order'
	 When A.Remarks = 'TFCOrderX'           or A.Remarks = 'TFCOrder|X'            Then 'NEW Consent Order'
	 When A.Remarks = 'TFCOrderR'           or A.Remarks = 'TFCOrder|R'            Then 'NEW Call Recording Order'
	 When A.Remarks like 'TFCOrderD,%|%'    or A.Remarks like 'TFCOrder|D,%|%'     Then 'Drishti Order with Registered Number' 
	 When A.Remarks like 'TFCOrderO,%|%'    or A.Remarks like 'TFCOrder|O,%|%'     Then 'Drishti OTP Order with Non Registered Number'
	 When A.Remarks like 'TFCOrderX,%|%'    or A.Remarks like 'TFCOrder|X,%|%'     Then 'Drishti Consent Order with Non Registered Number'
	 When A.Remarks like 'TFCOrderR,%|%'    or A.Remarks like 'TFCOrder|R,%|%'     Then 'Drishti Call Recording Order with Non Registered Number'
	 When A.Remarks = 'TFCOrderO,Exception' or A.Remarks = 'TFCOrder|O,Exception'  then 'Drishti OTP Order Call Not Connected'
	 When A.Remarks = 'TFCOrderX,Exception' or A.Remarks = 'TFCOrder|X,Exception'  then 'Drishti Consent Order Call Not Connected'
	 When A.Remarks = 'TFCOrderR,Exception' or A.Remarks = 'TFCOrder|R,Exception'  then 'Drishti Call Recording Order Call Not Connected'

	 -- Option Chain
	 When A.Remarks = 'OchainO'           or A.Remarks = 'Ochain|O'            Then 'NEW OTP Order'
	 When A.Remarks = 'OchainX'           or A.Remarks = 'Ochain|X'            Then 'NEW Consent Order'
	 When A.Remarks = 'OchainR'           or A.Remarks = 'Ochain|R'            Then 'NEW Call Recording Order'
	 When A.Remarks like 'OchainD,%|%'    or A.Remarks like 'Ochain|D,%|%'     Then 'Drishti Order with Registered Number' 
	 When A.Remarks like 'OchainO,%|%'    or A.Remarks like 'Ochain|O,%|%'     Then 'Drishti OTP Order with Non Registered Number'
	 When A.Remarks like 'OchainX,%|%'    or A.Remarks like 'Ochain|X,%|%'     Then 'Drishti Consent Order with Non Registered Number'
	 When A.Remarks like 'OchainR,%|%'    or A.Remarks like 'Ochain|R,%|%'     Then 'Drishti Call Recording Order with Non Registered Number'
	 When A.Remarks = 'OchainO,Exception' or A.Remarks = 'Ochain|O,Exception'  then 'Drishti OTP Order Call Not Connected'
	 When A.Remarks = 'OchainX,Exception' or A.Remarks = 'Ochain|X,Exception'  then 'Drishti Consent Order Call Not Connected'
	 When A.Remarks = 'OchainR,Exception' or A.Remarks = 'Ochain|R,Exception'  then 'Drishti Call Recording Order Call Not Connected'

	 -- Option Decoder Pro

	 When A.Remarks = 'ODOrderO'           or A.Remarks = 'ODOrder|O'            Then 'NEW OTP Order'
	 When A.Remarks = 'ODOrderX'           or A.Remarks = 'ODOrder|X'            Then 'NEW Consent Order'
	 When A.Remarks = 'ODOrderR'           or A.Remarks = 'ODOrder|R'            Then 'NEW Call Recording Order'
	 When A.Remarks like 'ODOrderD,%|%'    or A.Remarks like 'ODOrder|D,%|%'     Then 'Drishti Order with Registered Number' 
	 When A.Remarks like 'ODOrderO,%|%'    or A.Remarks like 'ODOrder|O,%|%'     Then 'Drishti OTP Order with Non Registered Number'
	 When A.Remarks like 'ODOrderX,%|%'    or A.Remarks like 'ODOrder|X,%|%'     Then 'Drishti Consent Order with Non Registered Number'
	 When A.Remarks like 'ODOrderR,%|%'    or A.Remarks like 'ODOrder|R,%|%'     Then 'Drishti Call Recording Order with Non Registered Number'
	 When A.Remarks = 'ODOrderO,Exception' or A.Remarks = 'ODOrder|O,Exception'  then 'Drishti OTP Order Call Not Connected'
	 When A.Remarks = 'ODOrderX,Exception' or A.Remarks = 'ODOrder|X,Exception'  then 'Drishti Consent Order Call Not Connected'
	 When A.Remarks = 'ODOrderR,Exception' or A.Remarks = 'ODOrder|R,Exception'  then 'Drishti Call Recording Order Call Not Connected'

	 -- Intra Option Order
	 When A.Remarks = 'IOptO'           or A.Remarks = 'IOpt|O'            Then 'NEW OTP Order'
	 When A.Remarks = 'IOptX'           or A.Remarks = 'IOpt|X'            Then 'NEW Consent Order'
	 When A.Remarks = 'IOptR'           or A.Remarks = 'IOpt|R'            Then 'NEW Call Recording Order'
	 When A.Remarks like 'IOptD,%|%'    or A.Remarks like 'IOpt|D,%|%'     Then 'Drishti Order with Registered Number' 
	 When A.Remarks like 'IOptO,%|%'    or A.Remarks like 'IOpt|O,%|%'     Then 'Drishti OTP Order with Non Registered Number'
	 When A.Remarks like 'IOptX,%|%'    or A.Remarks like 'IOpt|X,%|%'     Then 'Drishti Consent Order with Non Registered Number'
	 When A.Remarks like 'IOptR,%|%'    or A.Remarks like 'IOpt|R,%|%'     Then 'Drishti Call Recording Order with Non Registered Number'
	 When A.Remarks = 'IOptO,Exception' or A.Remarks = 'IOpt|O,Exception'  then 'Drishti OTP Order Call Not Connected'
	 When A.Remarks = 'IOptX,Exception' or A.Remarks = 'IOpt|X,Exception'  then 'Drishti Consent Order Call Not Connected'
	 When A.Remarks = 'IOptR,Exception' or A.Remarks = 'IOpt|R,Exception'  then 'Drishti Call Recording Order Call Not Connected'

	 -- Multi Price

	 When A.Remarks = 'MPOrderO'           or  A.Remarks = 'MPOrder|O'            Then 'NEW OTP Order'
	 When A.Remarks = 'MPOrderX'           or  A.Remarks = 'MPOrder|X'            Then 'NEW Consent Order'
	 When A.Remarks = 'MPOrderR'           or  A.Remarks = 'MPOrder|R'            Then 'NEW Call Recording Order'
	 When A.Remarks like 'MPOrderD,%|%'    or  A.Remarks like 'MPOrder|D,%|%'     Then 'Drishti Order with Registered Number' 
	 When A.Remarks like 'MPOrderO,%|%'    or  A.Remarks like 'MPOrder|O,%|%'     Then 'Drishti OTP Order with Non Registered Number'
	 When A.Remarks like 'MPOrderX,%|%'    or  A.Remarks like 'MPOrder|X,%|%'     Then 'Drishti Consent Order with Non Registered Number'
	 When A.Remarks like 'MPOrderR,%|%'    or  A.Remarks like 'MPOrder|R,%|%'     Then 'Drishti Call Recording Order with Non Registered Number'
	 When A.Remarks = 'MPOrderO,Exception' or  A.Remarks = 'MPOrder|O,Exception'  then 'Drishti OTP Order Call Not Connected'
	 When A.Remarks = 'MPOrderX,Exception' or  A.Remarks = 'MPOrder|X,Exception'  then 'Drishti Consent Order Call Not Connected'
	 When A.Remarks = 'MPOrderR,Exception' or  A.Remarks = 'MPOrder|R,Exception'  then 'Drishti Call Recording Order Call Not Connected'

	 When A.Remarks = 'NewOTPOrder' or A.Remarks = 'NewOTPOrder|O' Then 'OTP Client Or Illiquid Scrip Order'
	 When A.Remarks = 'MDOTPOrder'  or A.Remarks = 'MDOTPOrder|O'  Then 'OTP Client Or Illiquid Scrip Order'

	When A.Remarks like 'NewOTPOrder,%|%' or A.Remarks like 'NewOTPOrder|O,%|%' Then 'Drishti OTP Client Or Illiquid Scrip Order'
	When A.Remarks like 'MDOTPOrder,%|%'  or A.Remarks like 'MDOTPOrder|O,%|%'  Then 'Drishti OTP Client Or Illiquid Scrip Order'

	When A.Remarks like 'NewOTPOrder,Exception' or A.Remarks like 'NewOTPOrder|O,Exception' Then 'Drishti OTP Client Or Illiquid Scrip Order'
	When A.Remarks like 'MDOTPOrder,Exception'  or A.Remarks like 'MDOTPOrder|O,Exception'  Then 'Drishti OTP Client Or Illiquid Scrip Order'
	When A.Remarks like 'FavOrder,Exception'    or A.Remarks like 'FavOrder|O,Exception'    Then 'Drishti OTP Client Or Illiquid Scrip Order'
   
  Else  A.Remarks     End  AS Remarks, 
  
--convert(bigint,(isnull(T.TradeQty, 0)*isnull(convert(bigint,CONVERT(DECIMAL(18,2),(T.TradePrice/POWER(10.0,T.NumberOfDecimals)))),0))) as  Trade_Value,  
  Case When A.ExchangeCode in (5,6,3,10) Then
		 Multiplier*((CONVERT(DECIMAL(18,2),(t.TradePrice/POWER(10.0,T.NumberOfDecimals)))) *abs(T.TradeQty) )
		 Else ((CONVERT(DECIMAL(18,2),(t.TradePrice/POWER(10.0,T.NumberOfDecimals)))) *abs(T.TradeQty) ) End as  Trade_Value,

E.ExchangeName as  Segment,   
A.OrderInitiatorId as  TerminalNo,  
  
A.RecordInsertedDate  

 from tbl_Trade T with (nolock)   
inner join tbl_Scrips S with (nolock)         on T.ExchangeCode = S.ExchangeCode and T.ScripCode = S.ScripCode  
inner join tbl_ExchangeMaster E with (nolock) on T.ExchangeCode = E.ExchangeCode  
left join tbl_Order A with (nolock)          on   
--T.ClientCode = A.ClientCode and  
T.UniqueOrderId = A.UniqueOrderId   
--and T.ScripCode = A.ScripCode and T.ExchangeCode = A.ExchangeCode   AND T.OrderNo=A.OrderNo AND A.QtyTradedToday=T.OrderQty
where T.ClientCode <> T.TerminalID  AND A.BunchSeqNo = 2
AND A.Source = 'E'   

GO
/****** Object:  View [dbo].[vw_PhysicalOption_View_NCDEX]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
Create view [dbo].[vw_PhysicalOption_View_NCDEX]  
as  
select   
a.MemberCode [ClientCode]  
,c.MemberName [ClientName]  
,c.Branch  
,c.SubBroker  
,b.ScripShortName [Symbol]  
,b.InstrumentName [InstrumentType]  
,b.optionType [OptionType]  
,b.StrikePrice [StrikePrice]  
,convert(varchar,CAst(dateadd(ss,b.ExpiryDate ,'1980-01-01') as dateTime),106)[ExpiryDate]  
,a.BotQty - a.SoldQty [OpenQty]  
--,a.exposureMargin [Exposure]  
,d.MarketPrice * (abs(a.BotQty - a.SoldQty)) [Exposure]  
,b.MarketPrice [LTP]  
,d.MarketPrice [SpotPrice]  
,case when b.optionType = 'XX' then 0 else d.marketPrice - b.strikePrice end [SpotMinusStrike]  
,case   
when b.optionType = 'CE' then case when (H.marketPrice - b.strikePrice) > 0 then 'ITM' else 'OTM' end  
when b.optionType = 'PE' then case when (H.marketPrice - b.strikePrice) < 0 then 'ITM' else 'OTM' end  
else '' end [ITMOTM]  

,Round(c.baseCapital,2) [BaseCapital]
,c.IsNonPOA
--,isnull(e.AvailableQty,0)AvailableQty  
,0 as AvailableQty  
,null [QtyToConvert]  
,b.marketLot  
,b.exchangeCode  
,f.OpenQty[IntentQty],f.Exposure[IntentExposure],f.LTP[IntentLTP],f.SpotPrice[IntentSportPrice],f.SpotMinusStrike[IntentSpotMinusStrike]  
,a.ScripCode  
,f.id,f.ITMOTM[IntentITMOTM],f.AvailableCash[IntentAvailableCash],f.AvailableQty[IntentAvailableQty],f.IntentionForDelivery,f.QtyMArkedForConvert  

,b.scripFullName 
,b.MarketPrice
,case 
when (a.BotQty - a.SoldQty) > 0 then Round((b.MarketPrice * (100 - (select top 1 CapitalM2MCircuitFilter from dbo.tbl_Config)))/100,1) 
when (a.BotQty - a.SoldQty) < 0 then Round((b.MarketPrice * (100 + (select top 1 CapitalM2MCircuitFilter from dbo.tbl_Config)))/100,1) end as Price

from dbo.tbl_MemberInstrument a with (nolock)  
inner join dbo.tbl_Scrips b with (nolock) on a.ExchangeCode = b.ExchangeCode and a.ScripCode = b.ScripCode  

inner join   
(select a.ScripShortName,a.MarketPrice,a.ExchangeCode,a.ScripCode,a.ScripIsinNo from dbo.tbl_Scrips a with (nolock)  left outer join  
(select ScripShortName,ULToken from dbo.tbl_Scrips b with (nolock) where ExchangeCode = 5 and optiontype in ('XX','CE','PE') group by ScripShortName,ULToken) b  
on a.scripshortname = b.scripshortname and a.ultoken = b.ultoken  
where a.exchangecode = 5 and b.ScripShortName is not null  
) d on b.ScripShortName = d.ScripShortName  and a.ScripCode = d.ScripCode
left outer join dbo.tbl_Members c with (nolock)  on a.membercode = c.MemberCode  
LEFT OUTER JOIN tbl_CommodityOptionsDelivery F with (nolock) ON A.ExchangeCode = f.ExchangeCode and a.scripcode = f.scripcode and a.membercode = f.Clientcode  
LEFT OUTER JOIN
(select * from 
(select row_number() over (partition by ExchangeCode, ScripShortName order by  ExpiryDate asc  )as srno ,
ScripShortName ,ScripCode ,ExchangeCode, MarketPrice
from dbo.tbl_Scrips (nolock)
where ExchangeCode = 5 and MARKETPRICE > 0  and OptionType = 'XX'
) a where srno = 1)  H on B.ExchangeCode = H.ExchangeCode and B.ScripShortName = H.ScripShortName
where b.IsFOScrip = 1 and b.ExchangeCode = 5 and b.optiontype in ('CE','PE','XX') --and b.InstrumentName like '%STK%'  
and BotQty - SoldQty <> 0  
and  case   when b.optionType = 'CE' then  case when ( H.marketPrice - b.strikePrice) > 0 then 'ITM' else 'OTM' end  
when b.optionType = 'PE' then case when (H.marketPrice - b.strikePrice) < 0 then 'ITM' else 'OTM' end  
else '' end    <> 'OTM'
and dateadd(ss,b.ExpiryDate,'01-01-1980') between getdate() and getdate()+10 -- To fetch only near month open positions

GO
