GO






CREATE view [dbo].[vw_NonPOAHoldings_BOTH]      
as      
 /*select distinct a.MemberCode,a.DPID,a.ScripIsInNo,a.AvailableQty  as TotalQty,isnull(b.SellQty,0) as TransferedQty,      
 (a.AvailableQty-isnull(b.SellQty,0)) as AvailableQty,a.ScripName, LTP,'EQ' as ScripType      
 from PMP.dbo.tbl_DPHoldings_NonPOA a       
 left outer join (select sum(SellQty) as SellQty,ClientCode,ISIN,DPID from PMP.dbo.tbl_DematSell     
 where Convert(VARCHAR(10),convert(DateTime,RequestedDate),120)=convert(varchar(10),GETDATE(),120) group by ClientCode,ISIN,DPID) b       
 on a.DPID=b.DPID and a.MemberCode=b.ClientCode and b.ISIN=a.ScripIsInNo      
 WHERE a.LTP>0 
 union  
 select  distinct a.clientcode,a.dpid, a.isin,units,isnull(c.sellqty,0) as transferedqty,      
 (a.units-isnull(c.sellqty,0)) as availableqty,      
 (select top 1 schemename from [192.168.103._109].RTGSLAAWMST.dbo.[MFBSEScripMaster] where ISIN = a.isin) as schemename,      
 --d.schemename,       
 rate,'mf' as scriptype       
 from mcubeipo.dbo.mcubemfholdings a          
 left outer join (select sum(sellqty) as sellqty,clientcode,isin,dpid from pmp.dbo.tbl_mfnonpoa_dematsell     
 where Convert(VARCHAR(10),convert(DateTime,RequestedDate),120)=convert(varchar(10),GETDATE(),120) group by clientcode,isin,dpid) c       
 on a.dpid=c.dpid and a.clientcode=c.clientcode and c.isin=a.isin       
 where a.poa=0 AND a.Rate>0 and a.ISIN not in (select ScripIsInNo from PMP.dbo.tbl_DPHoldings_NonPOA where MemberCode=a.ClientCode)*/

 
select distinct a.MemberCode,a.DPID,a.ScripIsInNo,a.AvailableQty  as TotalQty,
 CASE When ISNULL(b.SellQty,0)>a.AvailableQty Then a.AvailableQty ELSE isnull(b.SellQty,0) END as TransferedQty,
 CASE When (a.AvailableQty-isnull(b.SellQty,0))<0 Then 0 ElSe (a.AvailableQty-isnull(b.SellQty,0)) End as AvailableQty,
 a.ScripName, LTP,'EQ' as ScripType, BuyAvgPrice as AvgPrice  
 from PMP.dbo.tbl_DPHoldings_NonPOA a   
 left outer join (Select sum(SellQty) SellQty,ClientCode,ISIN,DPID From
	 (select ApproovedQty as SellQty,ClientCode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise 
	 where convert(Date,RequestedDate)=convert(Date,GETDATE())
	 Union ALL
	 select /*remainingqty*/(ApproovedQty-AdjustedQty) sellQty,Clientcode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise
	 Where convert(Date,RequestedDate)<convert(Date,GETDATE()) And ReqType IN('D')
	 /*select Sum(ApproovedQty) as SellQty,0 prvQty,ClientCode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise 
	 where convert(Date,RequestedDate)=convert(Date,GETDATE()) 
	 group by clientcode,isin,DPID
	 Union ALL
	 --select 0,Sum(remainingqty) prvQty,Clientcode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise
	 select 0,Sum(ApproovedQty-AdjustedQty) prvQty,Clientcode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise	 
	 Where convert(Date,RequestedDate)<convert(Date,GETDATE()) And ReqType IN('D')
	 group by clientcode,isin,DPID*/
	 )t
 group by clientcode,isin,DPID
 ) b    
 on a.DPID=b.DPID and a.MemberCode=b.ClientCode and b.ISIN=a.ScripIsInNo  
 WHERE a.LTP>0
 UNION
 select  distinct a.ClientCode AS MemberCode,a.DPID, a.ISIN AS ScripIsInNo,Units AS TotalQty,
 Case When isnull(c.SellQty,0)>Units Then Units ELSE isnull(c.SellQty,0) END as TransferedQty,        
 Case When (a.Units-isnull(c.SellQty,0))<0 Then 0 Else (a.Units-isnull(c.SellQty,0)) End as AvailableQty,      
  --d.SchemeName AS ScripName,       
  (select top 1 schemename from [DION].RTGSLAAWMST.dbo.[MFBSEScripMaster] where ISIN = a.isin) as ScripName,      
  Rate AS LTP,'MF' as ScripType, 0 as AvgPrice         
 from [trd_secondary].mcubeipo.dbo.MCubeMFHoldings a              
 left outer join (Select sum(SellQty) SellQty,ClientCode,ISIN,DPID From
	 (select ApproovedQty as SellQty,ClientCode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise 
	 where convert(Date,RequestedDate)=convert(Date,GETDATE())
	 Union ALL
	 select /*remainingqty*/(ApproovedQty-AdjustedQty) sellQty,Clientcode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise
	 Where convert(Date,RequestedDate)<convert(Date,GETDATE())
	 )t
 group by clientcode,isin,DPID
 ) c          
 on a.DPID=c.DPID and a.ClientCode=c.ClientCode and C.ISIN=a.ISIN         
 where a.POA=0  AND a.Rate>0 and a.ISIN not in (select ScripIsInNo from PMP.dbo.tbl_DPHoldings_NonPOA where MemberCode=a.ClientCode)


GO
/****** Object:  View [dbo].[vw_NonPOAHoldings_Equity]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO



CREATE view [dbo].[vw_NonPOAHoldings_Equity]  
as  
 /*select distinct a.MemberCode,a.DPID,a.ScripIsInNo,a.AvailableQty  as TotalQty,isnull(b.SellQty,0) as TransferedQty,  
 (a.AvailableQty-isnull(b.SellQty,0)) as AvailableQty,a.ScripName, LTP,'EQ' as ScripType, BuyAvgPrice as AvgPrice  
 from PMP.dbo.tbl_DPHoldings_NonPOA a   
 left outer join (select sum(SellQty) as SellQty,ClientCode,ISIN,DPID from PMP.dbo.tbl_DematSell 
 where Convert(VARCHAR(10),convert(DateTime,RequestedDate),120)=convert(varchar(10),GETDATE(),120) group by ClientCode,ISIN,DPID) b   
 on a.DPID=b.DPID and a.MemberCode=b.ClientCode and b.ISIN=a.ScripIsInNo  
 WHERE a.LTP>0*/
 
select distinct a.MemberCode,a.DPID,a.ScripIsInNo,a.AvailableQty  as TotalQty,
 CASE When b.SellQty>a.AvailableQty Then a.AvailableQty ELSE isnull(b.SellQty,0) END as TransferedQty,  
 CASE When (a.AvailableQty-isnull(b.SellQty,0))<0 Then 0 ElSe (a.AvailableQty-isnull(b.SellQty,0)) End as AvailableQty,
 a.ScripName, LTP,'EQ' as ScripType, BuyAvgPrice as AvgPrice  
from PMP.dbo.tbl_DPHoldings_NonPOA a   
 left outer join (Select sum(SellQty) SellQty,ClientCode,ISIN,DPID From
	 (
	 select ApproovedQty as SellQty,ClientCode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise 
	 where convert(Date,RequestedDate)=convert(Date,GETDATE())
	 Union ALL
	 --select remainingqty sellQty,Clientcode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise
	 select (ApproovedQty-AdjustedQty) sellQty,Clientcode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise
	 Where convert(Date,RequestedDate)<convert(Date,GETDATE()) And ReqType IN('D')
	 /*
	 select Sum(ApproovedQty) as SellQty,0 prvQty,ClientCode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise 
	 where convert(Date,RequestedDate)=convert(Date,GETDATE()) 
	 group by clientcode,isin,DPID
	 Union ALL
	 --select 0,Sum(remainingqty) prvQty,Clientcode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise
	 select 0,Sum(ApproovedQty-AdjustedQty) prvQty,Clientcode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise	 
	 Where convert(Date,RequestedDate)<convert(Date,GETDATE()) And ReqType IN('D')
	 group by clientcode,isin,DPID*/
	 )t
 group by clientcode,isin,DPID
 ) b   
 on a.DPID=b.DPID and a.MemberCode=b.ClientCode and b.ISIN=a.ScripIsInNo  
 WHERE a.LTP>0


GO
