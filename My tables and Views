/****** Object:  View [dbo].[VW_SPAN_BSE_FNO_INTER_MONTH_SPREAD]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE VIEW [dbo].[VW_SPAN_BSE_FNO_INTER_MONTH_SPREAD]
AS

--Select * from VW_SPAN_NSE_FNO_INTER_MONTH_SPREAD Order by Code

select 
A.CC as Code,Convert(numeric(24,6),MAX(E.val)) as IMS
from Test_ccDef A WITH(NOLOCK)
JOIN Test_dSpread B WITH(NOLOCK)
ON A.CCDeF_ID=B.CCDEF_ID
JOIN Test_rate E WITH (NOLOCK)
ON B.dSpread_Id=E.dSpread_Id
Where B.Spread IN ('1','2','3')
GROUP BY A.CC


GO
/****** Object:  View [dbo].[VW_SPAN_BSE_FNO_SOM_INTEREST_RATE]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE VIEW [dbo].[VW_SPAN_BSE_FNO_SOM_INTEREST_RATE]
AS

--Select * from VW_SPAN_NSE_FNO_SOM_INTEREST_RATE Order by Code

select A.CC as Code,Convert(numeric(24,6),MAX(E.val)) as SOM
from Test_ccDef A WITH(NOLOCK)
JOIN Test_dSpread B WITH(NOLOCK)
 ON A.CCDeF_ID=B.CCDEF_ID
JOIN Test_somTiers C WITH(NOLOCK)
ON B.CCDEF_ID=C.CCDEF_ID
JOIN Test_tier D  WITH(NOLOCK)
ON C.somTiers_ID=D.SomTiers_ID
JOIN Test_rate E WITH (NOLOCK)
ON D.Tier_ID=E.Tier_ID
GROUP BY A.CC
GO
/****** Object:  View [dbo].[VW_SynchAllDashBoard]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


CREATE View [dbo].[VW_SynchAllDashBoard]
as
Select A.TableName,A.[Live-56-110_Count],B.[trd_secondary-56-153_Count],C.[mobiledb1-56-138_Count]  ,
D.[mobiledb2-56-148_Count],E.[ReportServer-56-147_Count]
,F.[trd_dr-166-110_Count]
,G.[Reportserver3-56-191_Count]
from 
(
select object_name(object_id)'TableName',row_count 'Live-56-110_Count',max(index_id) 'MaxIndexId' 
from sys.dm_db_partition_stats 
where   object_name(object_id) in('tbl_Members','tbl_MemberInstrument','tbl_Scrips','tbl_Order','tbl_OrderMaster','tbl_Trade','tbl_BracketOrder','tbl_BracketOrderMaster','tbl_RiskParameter','tbl_Indices_Master',
'tbl_MemberSymbols','tbl_BranchDealerMapping','tbl_BranchClientMapping','tbl_ClientDealerMapping_Optimized_New','tbl_ClientDealerMapping_New' ,'tbl_DPHoldings','AW_BasketScripList','tbl_MarginExceptions','tbl_SymbolRisk','tbl_SymbolRisk_Tierwise','tbl_brokerage_Master'
,'tbl_AutoGreyListMapping','tbl_RiskGreyListMapping') 
group by object_name(object_id),row_count 
)A
inner join
(
select object_name(object_id)'TableName',row_count 'trd_secondary-56-153_Count',max(index_id) 'MaxIndexId' 
from [trd_secondary].awacs10.sys.dm_db_partition_stats 
where   object_name(object_id) in('tbl_Members','tbl_MemberInstrument','tbl_Scrips','tbl_Order','tbl_OrderMaster','tbl_Trade','tbl_BracketOrder','tbl_BracketOrderMaster','tbl_RiskParameter','tbl_Indices_Master',
'tbl_MemberSymbols','tbl_BranchDealerMapping','tbl_BranchClientMapping','tbl_ClientDealerMapping_Optimized_New','tbl_ClientDealerMapping_New' ,'tbl_DPHoldings','AW_BasketScripList','tbl_MarginExceptions','tbl_SymbolRisk','tbl_SymbolRisk_Tierwise','tbl_brokerage_Master'
,'tbl_AutoGreyListMapping','tbl_RiskGreyListMapping') 
group by object_name(object_id),row_count 
)B
on A.TableName=B.TableName
inner join
(
select object_name(object_id)'TableName',row_count 'mobiledb1-56-138_Count',max(index_id) 'MaxIndexId' 
from [mobiledb1].awacs10.sys.dm_db_partition_stats 
where   object_name(object_id) in('tbl_Members','tbl_MemberInstrument','tbl_Scrips','tbl_Order','tbl_OrderMaster','tbl_Trade','tbl_BracketOrder','tbl_BracketOrderMaster','tbl_RiskParameter','tbl_Indices_Master',
'tbl_MemberSymbols','tbl_BranchDealerMapping','tbl_BranchClientMapping','tbl_ClientDealerMapping_Optimized_New','tbl_ClientDealerMapping_New' ,'tbl_DPHoldings','AW_BasketScripList','tbl_MarginExceptions','tbl_SymbolRisk','tbl_SymbolRisk_Tierwise','tbl_brokerage_Master'
,'tbl_AutoGreyListMapping','tbl_RiskGreyListMapping') 
group by object_name(object_id),row_count 
)C
on A.TableName=C.TableName
inner join
(
select object_name(object_id)'TableName',row_count 'mobiledb2-56-148_Count',max(index_id) 'MaxIndexId' 
from [mobiledb2].awacs10.sys.dm_db_partition_stats 
where   object_name(object_id) in('tbl_Members','tbl_MemberInstrument','tbl_Scrips','tbl_Order','tbl_OrderMaster','tbl_Trade','tbl_BracketOrder','tbl_BracketOrderMaster','tbl_RiskParameter','tbl_Indices_Master',
'tbl_MemberSymbols','tbl_BranchDealerMapping','tbl_BranchClientMapping','tbl_ClientDealerMapping_Optimized_New','tbl_ClientDealerMapping_New' ,'tbl_DPHoldings','AW_BasketScripList','tbl_MarginExceptions','tbl_SymbolRisk','tbl_SymbolRisk_Tierwise','tbl_brokerage_Master'
,'tbl_AutoGreyListMapping','tbl_RiskGreyListMapping') 
group by object_name(object_id),row_count 
)D
on A.TableName= D.TableName
inner join
(
select object_name(object_id)'TableName',row_count 'ReportServer-56-147_Count',max(index_id) 'MaxIndexId' 
from [ReportServer].awacs10.sys.dm_db_partition_stats 
where   object_name(object_id) in('tbl_Members','tbl_MemberInstrument','tbl_Scrips','tbl_Order','tbl_OrderMaster','tbl_Trade','tbl_BracketOrder','tbl_BracketOrderMaster','tbl_RiskParameter','tbl_Indices_Master',
'tbl_MemberSymbols','tbl_BranchDealerMapping','tbl_BranchClientMapping','tbl_ClientDealerMapping_Optimized_New','tbl_ClientDealerMapping_New' ,'tbl_DPHoldings','AW_BasketScripList','tbl_MarginExceptions','tbl_SymbolRisk','tbl_SymbolRisk_Tierwise','tbl_brokerage_Master'
,'tbl_AutoGreyListMapping','tbl_RiskGreyListMapping') 
group by object_name(object_id),row_count 
)E
on A.TableName= E.TableName

inner join 
(
select object_name(object_id)'TableName',row_count 'trd_dr-166-110_Count',max(index_id) 'MaxIndexId' 
from [10.166.56.110].awacs10.sys.dm_db_partition_stats 
where   object_name(object_id) in('tbl_Members','tbl_MemberInstrument','tbl_Scrips','tbl_Order','tbl_OrderMaster','tbl_Trade','tbl_BracketOrder','tbl_BracketOrderMaster','tbl_RiskParameter','tbl_Indices_Master',
'tbl_MemberSymbols','tbl_BranchDealerMapping','tbl_BranchClientMapping','tbl_ClientDealerMapping_Optimized_New','tbl_ClientDealerMapping_New' ,'tbl_DPHoldings','AW_BasketScripList','tbl_MarginExceptions','tbl_SymbolRisk','tbl_SymbolRisk_Tierwise','tbl_brokerage_Master'
,'tbl_AutoGreyListMapping','tbl_RiskGreyListMapping') 
group by object_name(object_id),row_count  
)F
on A.TableName=F.TableName

inner join 
(
select object_name(object_id)'TableName',row_count 'Reportserver3-56-191_Count',max(index_id) 'MaxIndexId' 
from [REPORTSERVER3].awacs10.sys.dm_db_partition_stats 
where   object_name(object_id) in('tbl_Members','tbl_MemberInstrument','tbl_Scrips','tbl_Order','tbl_OrderMaster','tbl_Trade','tbl_BracketOrder','tbl_BracketOrderMaster','tbl_RiskParameter','tbl_Indices_Master',
'tbl_MemberSymbols','tbl_BranchDealerMapping','tbl_BranchClientMapping','tbl_ClientDealerMapping_Optimized_New','tbl_ClientDealerMapping_New' ,'tbl_DPHoldings','AW_BasketScripList','tbl_MarginExceptions','tbl_SymbolRisk','tbl_SymbolRisk_Tierwise','tbl_brokerage_Master'
,'tbl_AutoGreyListMapping','tbl_RiskGreyListMapping') 
group by object_name(object_id),row_count  
)G
on A.TableName=G.TableName


GO
/****** Object:  View [dbo].[WhatIsGoingOn]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO



CREATE VIEW [dbo].[WhatIsGoingOn]
AS
SELECT
OBJECT_NAME(objectid) as ObjectName
,SUBSTRING(stateText.text, (statement_start_offset/2)+1,
((CASE statement_end_offset
WHEN -1 THEN DATALENGTH(stateText.text)
ELSE statement_end_offset
END - statement_start_offset)/2) + 1) AS statement_text
,DB_Name() as DatabaseName
,req.cpu_time AS CPU_Time
,sysp.physical_io
,sysp.lastwaittype
,DATEDIFF(minute, last_request_start_time, getdate()) AS RunningMinutes
,req.Percent_Complete
,sysp.cmd
,sess.HOST_NAME as RunningFrom
,LEFT(CLIENT_INTERFACE_NAME, 25) AS RunningBy
,sess.session_id AS SessionID
,req.blocking_session_id AS BlockingWith
,req.reads
,req.writes
,sess.[program_name]
,sess.login_name
,sess.status
,sess.last_request_start_time
,sysp.login_time
,sysp.last_batch
,req.logical_reads

FROM
sys.dm_exec_requests req
INNER JOIN sys.dm_exec_sessions sess ON sess.session_id = req.session_id
AND sess.is_user_process = 1
INNER JOIN sys.sysprocesses sysp ON sysp.spid=sess.session_id
CROSS APPLY
sys.dm_exec_sql_text(sysp.sql_handle) AS stateText



GO
