SET QUOTED_IDENTIFIER ON
GO

create View [dbo].[VW_RMS_SCH_DELIVERY_EQ_Position]
as
Select C.ExchangeName, MemberCode, A.ScripCode,Scripname,ProductCode,BotQty, BotAmount, SoldQty, SoldAmount,
BotQty-SoldQty as NetQty,
ActualBotAmount,ActualSoldAmount
,ActualBotQty,ActualSoldQty
from AWACS10.dbo.tbl_MemberInstrument (nolock) A inner join AWACS10.dbo.tbl_Scrips (nolock) B on a.ExchangeCode=B.ExchangeCode and a.ScripCode=b.ScripCode 
 inner join AWACS10.dbo.tbl_ExchangeMaster (nolock) C  on a.ExchangeCode=c.ExchangeCode
 where A.ExchangeCode in (0,1,21) and ProductCode = 2






GO
