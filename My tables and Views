/****** Object:  UserDefinedFunction [dbo].[SplitString]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
/* 
* This function created by Vineet Sharma
* Used for seperate string from separator
*/

Create function [dbo].[SplitString] 
    (
        @str nvarchar(4000), 
        @separator char(1)
    )
    returns table
    AS
    return (
        with tokens(p, a, b) AS (
            select 
                1, 
                1, 
                charindex(@separator, @str)
            union all
            select
                p + 1, 
                b + 1, 
                charindex(@separator, @str, b + 1)
            from tokens
            where b > 0
        )
        select
            p-1 zeroBasedOccurance,
            substring(
                @str, 
                a, 
                case when b > 0 then b-a ELSE 4000 end) 
            AS s
        from tokens
      )
GO
/****** Object:  View [dbo].[Vw_A]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create View [dbo].[Vw_A]
as

Select ra_Id,
	Sum(Case when Sno=1 then convert(numeric(18,4),a_text) else 0 end) as Line1,
	Sum(Case when Sno=2 then convert(numeric(18,4),a_text) else '0' end) as Line2, 
	Sum(Case when Sno=3 then convert(numeric(18,4),a_text) else '0' end) as Line3,
	Sum(Case when Sno=4 then convert(numeric(18,4),a_text) else '0' end) as Line4,
	Sum(Case when Sno=5 then convert(numeric(18,4),a_text) else '0' end) as Line5,
	Sum(Case when Sno=6 then convert(numeric(18,4),a_text) else '0' end) as Line6,
	Sum(Case when Sno=7 then convert(numeric(18,4),a_text) else '0' end) as Line7,
	Sum(Case when Sno=8 then convert(numeric(18,4),a_text) else '0' end) as Line8,
	Sum(Case when Sno=9 then convert(numeric(18,4),a_text) else '0' end) as Line9,
	Sum(Case when Sno=10 then convert(numeric(18,4),a_text) else '0' end) as Line10,
	Sum(Case when Sno=11 then convert(numeric(18,4),a_text) else '0' end) as Line11,
	Sum(Case when Sno=12 then convert(numeric(18,4),a_text) else '0' end) as Line12,
	Sum(Case when Sno=13 then convert(numeric(18,4),a_text) else '0' end) as Line13,
	Sum(Case when Sno=14 then convert(numeric(18,4),a_text) else '0' end) as Line14,
	Sum(Case when Sno=15 then convert(numeric(18,4),a_text) else '0' end) as Line15,
	Sum(Case when Sno=16 then convert(numeric(18,4),a_text) else '0' end) as Line16
	from (SELECT ROW_NUMBER() OVER (PARTITION BY cast(ra_id as Int) ORDER BY cast(ra_id as Int)) AS Sno,* FROM Test_a) A
group by ra_Id
GO
/****** Object:  View [dbo].[vw_NonPOAHoldings]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO


CREATE view [dbo].[vw_NonPOAHoldings]        
 AS        
 /*select  distinct a.ClientCode AS MemberCode,a.DPID, a.ISIN AS ScripIsInNo,Units AS TotalQty,isnull(c.SellQty,0) as TransferedQty,        
  (a.Units-isnull(c.SellQty,0)) as AvailableQty,      
  --d.SchemeName AS ScripName,       
  (select top 1 schemename from [192.168.103__.109].RTGSLAAWMST.dbo.[MFBSEScripMaster] where ISIN = a.isin) as ScripName,      
  Rate AS LTP,'MF' as ScripType         
 from mcubeipo.dbo.MCubeMFHoldings a              
 left outer join (select sum(SellQty) as SellQty,ClientCode,ISIN,DPID from PMP.dbo.tbl_MFNonPOA_DematSell     
 where Convert(VARCHAR(10),convert(DateTime,RequestedDate),120)=convert(varchar(10),GETDATE(),120) group by ClientCode,ISIN,DPID) c         
 on a.DPID=c.DPID and a.ClientCode=c.ClientCode and C.ISIN=a.ISIN         
 where a.POA=0  AND a.Rate>0 and a.ISIN not in (select ScripIsInNo from PMP.dbo.tbl_DPHoldings_NonPOA where MemberCode=a.ClientCode)*/

select  distinct a.ClientCode AS MemberCode,a.DPID, a.ISIN AS ScripIsInNo,Units AS TotalQty,
 Case When isnull(c.SellQty,0)>Units Then Units ELSE isnull(c.SellQty,0) END as TransferedQty,        
 Case When (a.Units-isnull(c.SellQty,0))<0 Then 0 Else (a.Units-isnull(c.SellQty,0)) End as AvailableQty,
  --d.SchemeName AS ScripName,       
  (select top 1 schemename from [DION].RTGSLAAWMST.dbo.[MFBSEScripMaster] where ISIN = a.isin) as ScripName,      
  --(select top 1 schemename from [192.168.103__.109].RTGSLAAWMST.dbo.[MFBSEScripMaster] where ISIN = a.isin) as ScripName,      
  Rate AS LTP,'MF' as ScripType         
 from [trd_secondary].EDIS.dbo.MCubeMFHoldings a    
 --from [trd_secondary].mcubeipo.dbo.MCubeMFHoldings a    
 left outer join (Select sum(SellQty) SellQty,ClientCode,ISIN,DPID From
	 (select ApproovedQty as SellQty,ClientCode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise 
	 where convert(Date,RequestedDate)=convert(Date,GETDATE())
	 Union ALL
	 select /*remainingqty*/ (ApproovedQty-AdjustedQty) sellQty,Clientcode,ISIN,DPID from PMP.dbo.TBL_EDIS_FinalDetailClientwise
	 Where convert(Date,RequestedDate)<convert(Date,GETDATE())
	 )t
 group by clientcode,isin,DPID
 ) c         
 on a.DPID=c.DPID and a.ClientCode=c.ClientCode and C.ISIN=a.ISIN         
 where a.POA=0  AND a.Rate>0 and a.ISIN not in (select ScripIsInNo from PMP.dbo.tbl_DPHoldings_NonPOA where MemberCode=a.ClientCode)
 


GO
/****** Object:  View [dbo].[vw_NonPOAHoldings_BOTH]    Script Date: 6/3/2025 9:23:40 AM ******/
