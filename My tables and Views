	[ClientCode] [varchar](50) NULL,
	[ExchangeCode] [tinyint] NULL,
	[ScripCode] [int] NULL,
	[BuyOrSell] [char](1) NULL,
	[CallType] [char](15) NULL,
	[DealerId] [nvarchar](10) NULL,
	[recorddate] [datetime] NULL,
	[Saved] [char](1) NULL,
	[Submit] [char](1) NULL
) ON [PRIMARY]
GO
