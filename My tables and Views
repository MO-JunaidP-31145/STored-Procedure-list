/****** Object:  View [dbo].[vw_AutoClientTraded]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create view [dbo].[vw_AutoClientTraded]
as
select b.ClientCode
from dbo.tbl_AutoClient (nolock) a
inner join dbo.tbl_Trade (nolock) b 
on a.MemberCode = b.ClientCode
group by b.ClientCode
GO
/****** Object:  View [dbo].[vw_Dealerwise_tradeforSolicitation]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO



CREATE view [dbo].[vw_Dealerwise_tradeforSolicitation]  
as 

select a.* 
,b.ScripName
,case when b.StrikePrice > 0  then b.strikeprice else 0 end strikeprice 
,case when b.IsFOScrip = 1 then b.optiontype else 'Cash' end OptionType
,c.ExchangeName
from 
(
select ClientCode,ExchangeCode,ScripCode,BuyOrSell,TerminalID
from tbl_trade 
group by ClientCode,ExchangeCode,ScripCode,BuyOrSell,TerminalID
)a
left outer join tbl_scrips b on a.exchangecode = b.ExchangeCode and a.ScripCode = b.ScripCode
left outer join tbl_ExchangeMaster c on a.exchangecode = c.ExchangeCode

GO
