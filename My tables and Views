create view [dbo].[vw_RmsSelling_HoldingQty]
as
SELECT MEMBERCODE,RMSSELLINGQTY,SCRIPISINNO 
FROM AWACS10.DBO.TBL_DPHOLDINGS WITH(NOLOCK)
where RMSSellingQty > 0
GO
/****** Object:  View [dbo].[vw_RmsSelling_Amt]    Script Date: 6/3/2025 9:23:40 AM ******/
SET ANSI_NULLS ON
